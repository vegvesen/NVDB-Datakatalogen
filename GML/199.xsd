  <simpleType name="UtviklingsfaseType">
    <annotation>
      <documentation>9988: Angir treets utviklingsstadie og dermed hvilken type skjøtsel som kreves.</documentation>
    </annotation>
    <union memberTypes="nvdb:UtviklingsfaseEnumerationType nvdb:UtviklingsfaseOtherType"/>
