  <complexType name="HoldeplassutrustningType">
    <complexContent>
      <extension base="nvdb:AbstraktHoldeplassutrustningType">
        <sequence>
          <element minOccurs="0" name="UU_TypeTransportmiddel" type="nvdb:UU_TypeTransportmiddelType">
            <annotation>
              <documentation>10259: Angir hvilken type transportmiddel det er tilrettelagt for</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="UU_Type" type="nvdb:UU_TypeType">
