        <sc:taggedValue tag="NVDB_ID">824</sc:taggedValue>
        <sc:taggedValue tag="NVDB_ID">824</sc:taggedValue>
        <sc:taggedValue tag="NVDB_navn">Skredpunkt</sc:taggedValue>
      </appinfo>
    </annotation>
  </element>
