                        <sc:taggedValue tag="NVDB_ID">10181</sc:taggedValue>
                        <sc:taggedValue tag="NVDB_navn">Restrisiko for sikringstiltak A</sc:taggedValue>
                     </appinfo>
                  </annotation>
               </element>
               <element minOccurs="0" name="sikringstiltakB" type="string">
                  <annotation>
                     <documentation>12100: Kort beskrivelse av planlagt tiltak for utbedring av skredpunktet.</documentation>
                     <appinfo>
                        <sc:taggedValue tag="NVDB_ID">12100</sc:taggedValue>
                        <sc:taggedValue tag="NVDB_navn">Sikringstiltak B</sc:taggedValue>
                     </appinfo>
                  </annotation>
               </element>
               <element minOccurs="0"
                        name="kostnadsoverslag_MillKr_ForSikringstiltakB"
