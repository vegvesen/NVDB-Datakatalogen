            <sc:taggedValue tag="NVDB_ID">19445</sc:taggedValue>
            <sc:taggedValue tag="NVDB_navn">Taubanemast</sc:taggedValue>
          </appinfo>
        </annotation>
      </enumeration>
      <enumeration value="Taubanesentral">
