                     <documentation>10548: Angir om skiltpunktet har skiltholder.</documentation>
                     <appinfo>
                        <sc:taggedValue tag="NVDB_ID">10548</sc:taggedValue>
                        <sc:taggedValue tag="NVDB_navn">Skiltholder</sc:taggedValue>
                     </appinfo>
                  </annotation>
               </element>
               <element minOccurs="0" name="RAL_Kode" type="string">
