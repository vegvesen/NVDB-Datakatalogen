                <sc:taggedValue tag="NVDB_ID">9415</sc:taggedValue>
                <sc:taggedValue tag="NVDB_navn">Antall oppstillingspl. Forflytningshemmede</sc:taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element minOccurs="0" name="areal_Totalt" type="integer">
