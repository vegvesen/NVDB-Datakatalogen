<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Armeringsnett</Name>
  <Definition>Benyttes geoteknisk for å øke bæreevnen og som stabiliserende tiltak. Stålarmering kan benyttes for å hindre telesprekker i asfalt.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>materialtype</Name>
      <Definition>Angir hvilket materiale armeringsnettet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Armeringsnett_Materialtype">
                <EnumItem>Glassfiber</EnumItem>
                <EnumItem>Stål</EnumItem>
                <EnumItem>Plast</EnumItem>
                <EnumItem>Tre</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>plassering</Name>
      <Definition>Angir hvor i konstruksjonen armeringsnettet er plassert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Armeringsnett_Plassering">
                <EnumItem>Under slitelag</EnumItem>
                <EnumItem>Under bindlag</EnumItem>
                <EnumItem>Under bærelag</EnumItem>
                <EnumItem>Under forsterkningslag/på traubunn</EnumItem>
                <EnumItem>Under/i fylling</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal</Name>
      <Definition>Angir areal av område med armeringsnett.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde_Gjennomsnitt</Name>
      <Definition>Angir gjennomsnittsbredde av armeringsnett.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utleggingsdato</Name>
      <Definition>Angir dato for utlegging.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>budsjett</Name>
      <Definition>Angir hvilket budsjett utlegging går over.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Armeringsnett_Budsjett">
                <EnumItem>Anlegg</EnumItem>
                <EnumItem>Vedlikehold</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produktnavn</Name>
      <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og evt. serienummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Armeringsnett_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Armeringsnett_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
