<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_ARTSRIKVEGKANT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>plantelivVerdi</Name>
         <Definition>Angir hvor verdifullt planteliv det er i vegens sideområde.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_PlantelivVerdi">
                  <EnumItem>Høy verdi</EnumItem>
                  <EnumItem>Noe høy verdi</EnumItem>
                  <EnumItem>Svært høy verdi</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>typeVegkant</Name>
         <Definition>Angir hvilken type vegkant objektet gjelder.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_TypeVegkant">
                  <EnumItem>Annen type vegkant</EnumItem>
                  <EnumItem>Frisk-fuktig/ høgstaude vegkant</EnumItem>
                  <EnumItem>Fukteng-våteng vegkant</EnumItem>
                  <EnumItem>Nordlig/alpin vegkant</EnumItem>
                  <EnumItem>Tørrengvegkant</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bakgrunn_Kort</Name>
         <Definition>Vurdering/beskrivelse av forekomsten. Kun viktig informasjon som ikke finnes i andre felt. Beskriv dominerende arter, typiske arter, ev sjeldne eller rødlista arter. Ev status som naturtypelokalitet (og i tilfelle verdi A, B eller C) nevnes, hvis ikke kommenteres ikke NT-lokalitet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lokalitet</Name>
         <Definition>Stedsbeskrivelse som angir hvor forekomsten finnes.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>spesiellBeskrivelseMerknad</Name>
         <Definition>Gir beskrivelse for skjøtsel, tidsrom for kantklipp, andre hensyn og ev. annen type skjøtsel.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sone</Name>
         <Definition>Angir hvilken sone beskrivelsen gjelder for.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_Sone">
                  <EnumItem>Alle</EnumItem>
                  <EnumItem>Asfaltkantsone</EnumItem>
                  <EnumItem>Bakkantsone</EnumItem>
                  <EnumItem>Bakkantsone og grøft</EnumItem>
                  <EnumItem>Grøft</EnumItem>
                  <EnumItem>Midtsone</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir samlet areal.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>egenSkjøtselsplan</Name>
         <Definition>Angir om det finnes en egen skjøtselsplan for strekningen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_EgenSkjøtselsplan">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>restriksjonGrøftKantrensk</Name>
         <Definition>Angir om det skal tas spesielle hensyn ved skjøtsel.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_RestriksjonGrøftKantrensk">
                  <EnumItem>Utføres etter særskilt beskrivelse</EnumItem>
                  <EnumItem>Utføres som avtalt i driftskontrakt</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sprøyteforbud</Name>
         <Definition>Angir om det skal tas spesielle hensyn ved bruk av plantevernmidler.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_Sprøyteforbud">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>restriksjonKantklipp</Name>
         <Definition>Angir hvordan klipp skal utføres for å ta hensyn til vegetasjonen. Tidsrom for klipp skrives i feltet "Spesiell beskrivelse/ merknad".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Artsrik vegkant_RestriksjonKantklipp">
                  <EnumItem>1m bredde slått 1, full bredde slått 2</EnumItem>
                  <EnumItem>Følger ordinær kantklipp i kontrakt</EnumItem>
                  <EnumItem>Klipp iht. egen skjøtselsplan</EnumItem>
                  <EnumItem>Skal bare ha slått 1</EnumItem>
                  <EnumItem>Skal bare ha slått 2</EnumItem>
                  <EnumItem>Skal ha slått 1 og 2</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datoSistPåvist</Name>
         <Definition>Angir dato for når forekomsten sist ble påvist.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>registrertDato</Name>
         <Definition>Angir dato for når forekomsten ble registrert i NVDB første gang.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
