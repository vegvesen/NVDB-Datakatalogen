<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Avkjørsel</Name>
  <Definition>Kjørbar tilknytning til vegnettet for en eiendom eller et begrenset antall eiendommer (1). Merknad: Det regnes som vegkryss om tilknytningsveg er offentlig veg.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hva som er primært bruksområde for avkjørselen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Bruksområde">
                <EnumItem>Bolig</EnumItem>
                <EnumItem>Hytte</EnumItem>
                <EnumItem>Gardsbruk</EnumItem>
                <EnumItem>Dyrkamark/jordbruk</EnumItem>
                <EnumItem>Skog/skogbruk</EnumItem>
                <EnumItem>Forretning</EnumItem>
                <EnumItem>Industri</EnumItem>
                <EnumItem>Service</EnumItem>
                <EnumItem>Skole/Barnehage</EnumItem>
                <EnumItem>Fritidsaktivitet</EnumItem>
                <EnumItem>Naust</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>primæraktivitet</Name>
      <Definition>Angir hva som er primær aktivitet langs avkjørselsveg.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Primæraktivitet">
                <EnumItem>1-2leiligheter</EnumItem>
                <EnumItem>3-5 leiligheter</EnumItem>
                <EnumItem>6-10 leiligheter</EnumItem>
                <EnumItem>&gt; 10 leiligheter</EnumItem>
                <EnumItem>1-2 hytter</EnumItem>
                <EnumItem>3-5 hytter</EnumItem>
                <EnumItem>&gt; 5 hytter</EnumItem>
                <EnumItem>1 gardsbruk i drift</EnumItem>
                <EnumItem>2-3 gardsbruk i drift</EnumItem>
                <EnumItem>Nedlagt gardsbruk</EnumItem>
                <EnumItem>Beiteområde/dyrehold</EnumItem>
                <EnumItem>Jord/grustak</EnumItem>
                <EnumItem>Grustak/steinbrudd</EnumItem>
                <EnumItem>Tømmeropplagring/velteplass</EnumItem>
                <EnumItem>Liten forretning</EnumItem>
                <EnumItem>Middels landhandel</EnumItem>
                <EnumItem>Stort forretningssenter</EnumItem>
                <EnumItem>Kiosk/gatekjøkken</EnumItem>
                <EnumItem>Hotell &lt; 50 plasser</EnumItem>
                <EnumItem>Hotell &gt; 50 plasser</EnumItem>
                <EnumItem>Bensinstasjon, bilverksted</EnumItem>
                <EnumItem>Sykehus &lt; 50 plasser</EnumItem>
                <EnumItem>Sykehus &gt; 50 plasser</EnumItem>
                <EnumItem>Kirke/menighetshus/gravplass</EnumItem>
                <EnumItem>Samfunnshus/grendahus</EnumItem>
                <EnumItem>Stoppested kollektivtrafikk</EnumItem>
                <EnumItem>Parkering</EnumItem>
                <EnumItem>Utfartsplass, skitrekk</EnumItem>
                <EnumItem>Fritid, kino</EnumItem>
                <EnumItem>Lekeplass/løkke</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandPersonaktivitet</Name>
      <Definition>Angir korteste avstand fra en avkjørsel til personaktivitet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stigning</Name>
      <Definition>Angir stigning på avkjørselveg.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde</Name>
      <Definition>Angir fast bredde av avkjørselsvegen (ikke inkludert breddeutvidelse i kryssområdet).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dreneringsforhold</Name>
      <Definition>Angir hvordan dreneringsforhold er ved avkjørsel.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Dreneringsforhold">
                <EnumItem>Ok</EnumItem>
                <EnumItem>Tiltaksbehov</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>siktforhold</Name>
      <Definition>Gir informasjon rundt siktforhold.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>saksnummer</Name>
      <Definition>Angir saksnummer til sak i vegeiers arkivsystem som omhandler avkjørselen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse_URL</Name>
      <Definition>Gir URL til arkivreferanse.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>søknadMottattDato</Name>
      <Definition>Angir dato for når søknad ble mottatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedtak</Name>
      <Definition>Angir hva som er siste vedtak for denne avkjørselen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Vedtak">
                <EnumItem>Varig tillatelse</EnumItem>
                <EnumItem>Midlertidig tillatelse</EnumItem>
                <EnumItem>Endret bruk</EnumItem>
                <EnumItem>Utvidet bruk</EnumItem>
                <EnumItem>Avslag</EnumItem>
                <EnumItem>Avslag etter klage</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedtaksdato</Name>
      <Definition>Dato da det sist ble gjort vedtak i tilknytning til avkjørsel.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tillatelseGjelderTilDato</Name>
      <Definition>Angir hvilken dato tillatelse til å etablere avkjørselen gjelder til.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kontrollertOgGodkjentdato</Name>
      <Definition>Dato da ferdig avkjørsel ble kontrollert og godkjent.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år avkjørsel ble godkjent.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av avkjørselen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avkjørsel_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
