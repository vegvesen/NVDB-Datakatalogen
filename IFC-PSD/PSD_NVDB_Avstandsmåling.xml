<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Avstandsmåling</Name>
  <Definition>Målt avstand på tvers av vegen mellom gitt referansepunkt og et vegobjekt.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>avstandVegkant_Start</Name>
      <Definition>Angir avstand fra vegkant i start av målestrekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandVegkant_Slutt</Name>
      <Definition>Angir avstand fra vegkant i slutt av målestrekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandReferanselinje_Start</Name>
      <Definition>Angir avstand fra referanselinje i start av målestrekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandReferanselinje_Slutt</Name>
      <Definition>Angir avstand fra referanselinje i slutt av målestrekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kvalitetAvstandsmåling</Name>
      <Definition>Angir kvalitet på måling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Avstandsmåling_KvalitetAvstandsmåling">
                <EnumItem>Skjønn</EnumItem>
                <EnumItem>Målt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
