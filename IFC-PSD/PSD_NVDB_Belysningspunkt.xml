<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Belysningspunkt</Name>
  <Definition>Lokasjon/konteiner med samling av en eller flere lysarmaturer og lysmast i ett punkt. Det kan forekomme varianter uten lysmast, for eksempel lysarmatur opphengt i tunneltak, og det kan forekomme varianter uten lysarmatur, f.eks. lysmast kun for kabelframføring.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hva vegobjektet brukes til, hvilken funksjon det har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Bruksområde">
                <EnumItem>Belysning veg/gate</EnumItem>
                <EnumItem>Belysning tunnel</EnumItem>
                <EnumItem>Sikkerhetsbelysning tunnel</EnumItem>
                <EnumItem>Belysning bru</EnumItem>
                <EnumItem>Belysning område/plass</EnumItem>
                <EnumItem>Belysning ferjeleie</EnumItem>
                <EnumItem>Belysning Vegkryss</EnumItem>
                <EnumItem>Belysning gang/sykkelveg</EnumItem>
                <EnumItem>Belysning gangfelt</EnumItem>
                <EnumItem>Belysning undergang</EnumItem>
                <EnumItem>Belysning utstyr</EnumItem>
                <EnumItem>Belysning leskur</EnumItem>
                <EnumItem>Belysning skilt</EnumItem>
                <EnumItem>Belysning privat område</EnumItem>
                <EnumItem>Kun kabelframføring</EnumItem>
                <EnumItem>Evakueringslys tunnel</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antall</Name>
      <Definition>Vegobjektet kan representere flere objekt av samme type. Vi angir her hvor mange objekt dette objektet representerer. Merknad: Normalt skal det etableres et objekt per belysningspunkt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>plassering</Name>
      <Definition>Angir plassering for vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Plassering">
                <EnumItem>Lysmast</EnumItem>
                <EnumItem>Mindre stolpe</EnumItem>
                <EnumItem>Henger i tak/kabelstige</EnumItem>
                <EnumItem>Festet til vegg/bygning</EnumItem>
                <EnumItem>På portal</EnumItem>
                <EnumItem>På skiltpunkt</EnumItem>
                <EnumItem>Henger i vaier</EnumItem>
                <EnumItem>Nedfelt i belegning/dekke</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandFraVegkant</Name>
      <Definition>Angir hvilken avstand det er fra vegkant til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>strømtilførsel_Kategori</Name>
      <Definition>Angir hvilken type strømtilkobling det er til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Strømtilførsel_Kategori">
                <EnumItem>Jordkabel</EnumItem>
                <EnumItem>Line</EnumItem>
                <EnumItem>EX</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>strømtilførsel_TypeKabel</Name>
      <Definition>Detaljert spesifikasjon av kabel for strømtilførsel.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>overbelastningsvern</Name>
      <Definition>Angir om det er overbelastningsvern i punktet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Overbelastningsvern">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fellesføringMedAnnenEtat</Name>
      <Definition>Angir om det er felles luftlinjeframføring på felles stolperekke med f.eks. energiverk, teleselskap, bredbåndsselskap etc.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fellesføringLeietaker</Name>
      <Definition>Navn på leietaker(e) ved fellesføring der SVV har vedlikeholdsansvar.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>FKB_ID</Name>
      <Definition>Refererer til FKB-identitet. Benyttes i forbindelse med felles forvaltning av geometri.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier_Navn</Name>
      <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Belysningspunkt_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Lokalt e-verk</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
