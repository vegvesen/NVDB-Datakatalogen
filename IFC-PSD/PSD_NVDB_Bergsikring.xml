<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_BERGSIKRING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type vegobjektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_Type">
                  <EnumItem>Anker</EnumItem>
                  <EnumItem>Bart berg</EnumItem>
                  <EnumItem>Bolter</EnumItem>
                  <EnumItem>Fjellbånd</EnumItem>
                  <EnumItem>Nett</EnumItem>
                  <EnumItem>Nett, isnett</EnumItem>
                  <EnumItem>Nett, steinsprangnett</EnumItem>
                  <EnumItem>Nett, wirenett</EnumItem>
                  <EnumItem>Sprøytebetong</EnumItem>
                  <EnumItem>Sprøytebetongbuer, dobbel</EnumItem>
                  <EnumItem>Sprøytebetongbuer, enkel</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir areal av sikringsfeltet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallBolterAnkerStag</Name>
         <Definition>Angir hvor mange bolter, anker eller stag som er brukt i sikringsområde.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lengde</Name>
         <Definition>Angir total lengde av sikringsfelt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>typeSprøytebetong</Name>
         <Definition>Angir hvilken type sprøytebetong som er benyttet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_TypeSprøytebetong">
                  <EnumItem>Med armeringsnett</EnumItem>
                  <EnumItem>Med PP-fiber</EnumItem>
                  <EnumItem>Med stålfiber</EnumItem>
                  <EnumItem>Uarmert</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høydeOverVeg</Name>
         <Definition>Angir største høyde til overkant av bergsikring.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tykkelseSprøytebetong</Name>
         <Definition>Angir tykkelse av sprøytebetong.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>plassering</Name>
         <Definition>Angir sikringsfeltets plassering.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_Plassering">
                  <EnumItem>Dagen, naturlig terreng</EnumItem>
                  <EnumItem>Dagen, skjæring</EnumItem>
                  <EnumItem>Tunnel, halve profil høyre side</EnumItem>
                  <EnumItem>Tunnel, Halve profil venstre side</EnumItem>
                  <EnumItem>Tunnel, hele profilet</EnumItem>
                  <EnumItem>Tunnel, heng</EnumItem>
                  <EnumItem>Tunnel, heng og vegger</EnumItem>
                  <EnumItem>Tunnel, høyre vegg</EnumItem>
                  <EnumItem>Tunnel, høyre vegg og heng</EnumItem>
                  <EnumItem>Tunnel, såle</EnumItem>
                  <EnumItem>Tunnel, venstre vegg</EnumItem>
                  <EnumItem>Tunnel, venstre vegg og heng</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>wiredimensjon</Name>
         <Definition>Angir dimensjon for sikringswire.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>adkomst</Name>
         <Definition>Beskriver hvordan man får adkomst til vegobjektet for vedlikehold.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_Adkomst">
                  <EnumItem>Bil</EnumItem>
                  <EnumItem>Gravemaskin</EnumItem>
                  <EnumItem>Helikopter</EnumItem>
                  <EnumItem>Hjullaster</EnumItem>
                  <EnumItem>Kran</EnumItem>
                  <EnumItem>Tau</EnumItem>
                  <EnumItem>Til fots</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bergsikring_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
