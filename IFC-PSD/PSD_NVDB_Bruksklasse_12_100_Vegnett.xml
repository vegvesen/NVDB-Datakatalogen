<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_BRUKSKLASSE_12_100_VEGNETT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>AksellastTeleløsning</Name>
         <Definition>Angir største tillatte aksellast for periode med aksellastrestriksjoner pga teleløsning.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Bruksklasse, 12100-vegnett_AksellastTeleløsning">
                  <EnumItem>4 tonn</EnumItem>
                  <EnumItem>5 tonn</EnumItem>
                  <EnumItem>6 tonn</EnumItem>
                  <EnumItem>7 tonn</EnumItem>
                  <EnumItem>8 tonn</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>strekningsbeskrivelse</Name>
         <Definition>Beskrivelse av aktuell vegstrekning. Denne teksten blir hentet direkte inn i veglistene.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>merknad</Name>
         <Definition>Merknad knyttet til aktuell strekning. Kommer med uthevet tekst i veglistene.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>veglisteGjelderAlltid</Name>
         <Definition>Veglister er forskrift og gjelder alltid foran kartvisning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
