<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Bruksklasse_Tømmertransport</Name>
  <Definition>Gjeldende bruksklasse for tømmertransport i vegliste</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>bruksklasseHelår</Name>
      <Definition>Angir helårs bruksklasse.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bruksklasse_Tømmertransport_BruksklasseHelår">
                <EnumItem>Bk10 - 50 tonn</EnumItem>
                <EnumItem>Bk10 - 42 tonn</EnumItem>
                <EnumItem>BkT8 - 60 tonn</EnumItem>
                <EnumItem>BkT8 - 50 tonn</EnumItem>
                <EnumItem>BkT8 - 40 tonn</EnumItem>
                <EnumItem>Bk8 - 32 tonn</EnumItem>
                <EnumItem>Bk6 - 28 tonn</EnumItem>
                <EnumItem>Spesiell begrensning</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bruksklasseVinter</Name>
      <Definition>Angir lovlig bruksklasse (maks aksellast) for vinter.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bruksklasse_Tømmertransport_BruksklasseVinter">
                <EnumItem>Bk10 - 60 tonn</EnumItem>
                <EnumItem>Bk10 - 50 tonn</EnumItem>
                <EnumItem>BkT8 - 60 tonn</EnumItem>
                <EnumItem>BkT8 - 40 tonn</EnumItem>
                <EnumItem>Bk8 - 32 tonn</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>maksVogntoglengde</Name>
      <Definition>Angir maksimal lengde for vogntog.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bruksklasse_Tømmertransport_MaksVogntoglengde">
                <EnumItem>12,40</EnumItem>
                <EnumItem>15,00</EnumItem>
                <EnumItem>19,50</EnumItem>
                <EnumItem>22,00</EnumItem>
                <EnumItem>24,00</EnumItem>
                <EnumItem>Spesiell begrensning</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tillattForModulvogntog1Og2MedSporingskrav</Name>
      <Definition>Angir om det er tillatt for modulvogntog 1 og 2 med sporingskrav.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bruksklasse_Tømmertransport_TillattForModulvogntog1Og2MedSporingskrav">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>strekningsbeskrivelse</Name>
      <Definition>Beskrivelse av aktuell vegstrekning.  Denne teksten blir henta direkte inn i veglistene.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>maksTotalvektTømmertransport</Name>
      <Definition>Angir maksimal totalvekt for tømmertransport.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bruksklasse_Tømmertransport_MaksTotalvektTømmertransport">
                <EnumItem>50</EnumItem>
                <EnumItem>56</EnumItem>
                <EnumItem>60</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Merknad knyttet til aktuell strekning. Kommer med uthevet tekst i veglistene.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>veglisteGjelderAlltid</Name>
      <Definition>Veglister er forskrift og gjelder alltid foran kartvisning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
