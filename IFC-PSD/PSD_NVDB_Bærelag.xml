<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Bærelag</Name>
  <Definition>Det øverste lag i vegfundamentet.  Deles ofte i nedre og øvre (1).</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>lagtype</Name>
      <Definition>Angir om det er tale om øvre eller nedre bærelag.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_Lagtype">
                <EnumItem>Øvre bærelag</EnumItem>
                <EnumItem>Nedre bærelag</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type vegobjektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_Type">
                <EnumItem>Mekanisk stabiliserte materialer</EnumItem>
                <EnumItem>Forkilt pukk</EnumItem>
                <EnumItem>Knust grus</EnumItem>
                <EnumItem>Knust berg</EnumItem>
                <EnumItem>Knust asfalt</EnumItem>
                <EnumItem>Dypfrest, mekanisk stabilisert</EnumItem>
                <EnumItem>Bitumenstabiliserte materialer</EnumItem>
                <EnumItem>Asfaltert grus</EnumItem>
                <EnumItem>Asfaltert pukk</EnumItem>
                <EnumItem>Penetrert pukk</EnumItem>
                <EnumItem>Bitumesnstabilisert grus</EnumItem>
                <EnumItem>Bitumenanriket grus</EnumItem>
                <EnumItem>Emulsjonsgrus</EnumItem>
                <EnumItem>Emulsjonspukk</EnumItem>
                <EnumItem>Skumgrus</EnumItem>
                <EnumItem>Dypfrest, bitumenstabilisert</EnumItem>
                <EnumItem>Sementstabiliserte materialer</EnumItem>
                <EnumItem>Sementstabilisert pukk</EnumItem>
                <EnumItem>Sementstabilisert grus</EnumItem>
                <EnumItem>Gjenbruksbetong</EnumItem>
                <EnumItem>Ligninstabilert materiale</EnumItem>
                <EnumItem>Dypfrest, ligninstabilisert</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>øvreNominelleSteinstørrelse</Name>
      <Definition>Angir øvre nominell steinstørrelse for bærelagsmaterialet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_ØvreNominelleSteinstørrelse">
                <EnumItem>11</EnumItem>
                <EnumItem>16</EnumItem>
                <EnumItem>19</EnumItem>
                <EnumItem>22</EnumItem>
                <EnumItem>32</EnumItem>
                <EnumItem>63</EnumItem>
                <EnumItem>90</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nedreNominelleSteinstørrelse</Name>
      <Definition>Angir nedre nominell steinstørrelse for bærelagsmaterialet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_NedreNominelleSteinstørrelse">
                <EnumItem>0</EnumItem>
                <EnumItem>16</EnumItem>
                <EnumItem>22</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tykkelse</Name>
      <Definition>Angir tykkelse.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde_Gjennomsnitt</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forbruk</Name>
      <Definition>Angir masseforbruk pr kvm.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>leggedato</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>reseptnummer</Name>
      <Definition>Gir reseptnummer for oppslag i Labsys.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>budsjett</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_Budsjett">
                <EnumItem>Anlegg</EnumItem>
                <EnumItem>Vedlikehold</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>modifisertBindemiddel</Name>
      <Definition>Angir om det er brukt modifisert bindemiddel.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Bærelag_ModifisertBindemiddel">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Gir permanent merknad.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
