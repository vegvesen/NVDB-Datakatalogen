<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Døgnhvileplass</Name>
  <Definition>Et område som er tilrettelagt for hensetting av vogntog i langtransport slik at sjåførene kan innta normal døgnhvile.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Navn på døgnhvileplass.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lokalisertVed</Name>
      <Definition>Angir hvor døgnhvileplass er lokalisert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_LokalisertVed">
                <EnumItem>Rasteplass</EnumItem>
                <EnumItem>Bensinstasjon</EnumItem>
                <EnumItem>Vegkro</EnumItem>
                <EnumItem>Industriområde</EnumItem>
                <EnumItem>Annen virksomhet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skiltetAtkomst</Name>
      <Definition>Angir hvilken skilting det er i forbindelse med døgnhvileplass.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_SkiltetAtkomst">
                <EnumItem>Ja, forhånsvarsling og varsling ved avkjøring</EnumItem>
                <EnumItem>Ja, varsling ved avkjøring</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallOppstillingsplasserVogntog</Name>
      <Definition>Angir hvor mange oppstillingsplasser dimensjonert for vogntog, det er tilrettelagt for på døgnhvileplassen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilrettelagtForModulvogntog</Name>
      <Definition>Angir hvorvidt døgnhvileplassen er tilrettelagt for modulvogntog.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_TilrettelagtForModulvogntog">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kontaktinfo</Name>
      <Definition>Kontaktinfo til eier/ansvarlig som det er fysisk opplyst om (i form av oppslag etc.) på døgnhvileplassen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for drift av døgnhvileplassen. Det skal angis organisasjon/organisasjonsenhet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal_Totalt</Name>
      <Definition>Angir totalt areal av døgnhvileplass, inkludert kjøreareal, grøntareal, bygninger etc.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal_Kjørbart</Name>
      <Definition>Angir areal av kjørbart område innenfor Døgnhvileplassen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vaktordning</Name>
      <Definition>Angir hvilken type vaktordning det er i tilknytning til området.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Vaktordning">
                <EnumItem>Kontinuerlig vakt</EnumItem>
                <EnumItem>Regelmessig vakt</EnumItem>
                <EnumItem>Ingen vakt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kameraovervåkning</Name>
      <Definition>Angir om det er kameraovervåkning av området.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Kameraovervåkning">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>belysning</Name>
      <Definition>Angir om plassen er belyst.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Belysning">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stengtMedBommer</Name>
      <Definition>Angir om plassen er stengt med bommer.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_StengtMedBommer">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>reserveringssystem</Name>
      <Definition>Angir om det er mulig å forhåndsreservere plass.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Reserveringssystem">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>systemS_Mm_LderTilgjengeligKapasitet</Name>
      <Definition>Angir om det er system som melder tilgjengelig kapasitet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_SystemS_Mm_LderTilgjengeligKapasitet">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>farligGods</Name>
      <Definition>Angir hvordan det er tilrettelagt for kjøretøy som transporterer farlig gods.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_FarligGods">
                <EnumItem>Stengt</EnumItem>
                <EnumItem>Ikke tilrettelagt spesielt</EnumItem>
                <EnumItem>Egne atskilte plasser</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>drivstoffyllingsanlegg</Name>
      <Definition>Angir om det er anlegg for fylling av drivstoff.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Drivstoffyllingsanlegg">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vaskeplassForTrailere</Name>
      <Definition>Angir om det er vaskeplass for trailere/vogntog.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_VaskeplassForTrailere">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>snøryddingsrampe</Name>
      <Definition>Angir om det er snøryddingsrampe i tilknytning til døgnhvileplassen, dvs. sted hvor det er lagt til rette for å kunne fjerne snø fra kjøretøyet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Snøryddingsrampe">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilgangTilStrøm</Name>
      <Definition>Angir om det er tilgang til strøm.  F.eks. til aggregater og andre formål.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_TilgangTilStrøm">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>gebyrForStrøm</Name>
      <Definition>Angir hvorvidt det er gebyr for å bruke strøm.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_GebyrForStrøm">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>toalett</Name>
      <Definition>Angir om det er tilgang til toalett.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Toalett">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dusj</Name>
      <Definition>Angir om det er tilgang til dusj.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Dusj">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kiosk</Name>
      <Definition>Angir om det er kiosk i tilknytning til plassen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Kiosk">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kafeteria</Name>
      <Definition>Angir om det er kafeteria i tilknytning til plassen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Kafeteria">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>oppholdsrom</Name>
      <Definition>Angir om det er oppholdsrom for hvilende.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Oppholdsrom">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>trådløstInternett</Name>
      <Definition>Angir om det er tilgjengelig trådløst internett på stedet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_TrådløstInternett">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kollektivtilbudTilSentrum</Name>
      <Definition>Angir om det er kollektivtilbud til sentrum.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_KollektivtilbudTilSentrum">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Begrenset tilbud</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>gangavstandTilServicetilbud</Name>
      <Definition>Angir om det er gangavstand til servicetilbud.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_GangavstandTilServicetilbud">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>terminalLogistikkområde</Name>
      <Definition>Angir om lokaliseringen er i et viktig målpunktområde for godstransport.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_TerminalLogistikkområde">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>årligTilskuddFraStatensVegvesen</Name>
      <Definition>Angir hvor stor årlig bevilgning som tildeles til drift. Inngår i MOTIV-beregninger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prisnivå</Name>
      <Definition>Angir hvilket prisnivå tilskudd fra Statens vegvesen (ET 9466) er oppgitt i (Årstall).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Gir mulighet til å angi tilleggsinformasjon.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, annen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Delt eierskap</EnumItem>
                <EnumItem>Privat</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Døgnhvileplass_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
