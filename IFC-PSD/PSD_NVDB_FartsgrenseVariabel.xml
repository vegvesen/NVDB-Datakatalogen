<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_FartsgrenseVariabel</Name>
  <Definition>Høyeste tillatte hastighet på en vegstrekning innenfor et avgrenset tidsrom.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>fartsgrenseVerdi</Name>
      <Definition>Verdien på fartsgrensen i angitt tidsrom.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FartsgrenseVariabel_FartsgrenseVerdi">
                <EnumItem>20</EnumItem>
                <EnumItem>30</EnumItem>
                <EnumItem>40</EnumItem>
                <EnumItem>50</EnumItem>
                <EnumItem>60</EnumItem>
                <EnumItem>70</EnumItem>
                <EnumItem>80</EnumItem>
                <EnumItem>90</EnumItem>
                <EnumItem>100</EnumItem>
                <EnumItem>110</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type variabel fartsgrense det er.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FartsgrenseVariabel_Type">
                <EnumItem>Vinterfartsgrense</EnumItem>
                <EnumItem>Sommerfartsgrense</EnumItem>
                <EnumItem>Skole</EnumItem>
                <EnumItem>Miljøfartsgrense</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ukedag</Name>
      <Definition>Angir ukedager en variabel fartsgrense gjelder for.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FartsgrenseVariabel_Ukedag">
                <EnumItem>Hverdager</EnumItem>
                <EnumItem>Hverdager u/ lørdag</EnumItem>
                <EnumItem>Lørdag og søndag</EnumItem>
                <EnumItem>Mandag</EnumItem>
                <EnumItem>Tirsdag</EnumItem>
                <EnumItem>Onsdag</EnumItem>
                <EnumItem>Torsdag</EnumItem>
                <EnumItem>Fredag</EnumItem>
                <EnumItem>Lørdag</EnumItem>
                <EnumItem>Søndag</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoFra</Name>
      <Definition>Angir dato (mmdd) som variabel fartsgrense gjelder fra og med i et normalår.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoTil</Name>
      <Definition>Angir dato (mmdd) som variabel fartsgrense gjelder til og med i et normalår.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>klokkeslettFra</Name>
      <Definition>Angir tidspunkt (hhmm) på døgnet som variabel fartsgrense gjelder fra.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>klokkeslettTil</Name>
      <Definition>Angir tidspunkt på døgnet (hhmm) som variabel fartsgrense gjelder til.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedtaksnummer</Name>
      <Definition>Angir vedtaksnummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>gyldigFraDato</Name>
      <Definition>Dato for når fartsgrense ble satt i drift. Den dato skiltene ble "avduket".</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDate"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Kan gi merknad i forhold til variabel fartsgrense. F.eks. spesifikasjon av gyldighet, retningslinjer for nedsetting av fartsgrense etc.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivnummer</Name>
      <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
