<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Ferjesamband</Name>
  <Definition>Overfart som trafikkeres av bilferje. Kan ha to eller flere anløpssteder.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Angir navn på ferjesambandet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>FDB_Ferjesambands_Id</Name>
      <Definition>Unik ID for ferjesamband. Benyttes i Ferjedatabanken og i billetteringssystem.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>neTEx_id</Name>
      <Definition>Unik ID for ferjesambandet. ID skal være i henhold til NeTEx-format og være lik ferjesambandets ID i nasjonal rutebank/Entur. Eksempel: "MOR:Line:48"</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>sambandstype</Name>
      <Definition>Angir hvilken type samband det er, dvs. om det er på stat-, fylke- eller kommunenivå.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Ferjesamband_Sambandstype">
                <EnumItem>Riksvegferjesamband</EnumItem>
                <EnumItem>Fylkesferjesamband</EnumItem>
                <EnumItem>Kommuneferjesamband</EnumItem>
                <EnumItem>Privat ferjesamband</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsstatus</Name>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Ferjesamband_Driftsstatus">
                <EnumItem>Helårssamband</EnumItem>
                <EnumItem>Helårs med sambandsutvidelse sommersesong</EnumItem>
                <EnumItem>Sommersamband</EnumItem>
                <EnumItem>Nedlagt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>referanseTilEksternInfo</Name>
      <Definition>Gir referanse til ekstern informasjon. Angis i form av URL.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftFraDato</Name>
      <Definition>Angir dato (mmdd) når sommersambandet settes i drift i et normalår.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftTilDato</Name>
      <Definition>Angir dato (mmdd) når sommersambandet tas ut av drift et normalår.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
