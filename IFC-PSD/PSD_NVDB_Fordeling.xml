<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_FORDELING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>typeFordeling</Name>
         <Definition>Angir hvilken type fordeling enheten er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_TypeFordeling">
                  <EnumItem>Hovedfordeling</EnumItem>
                  <EnumItem>Tilknytningsskap</EnumItem>
                  <EnumItem>Underfordeling</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bruksområde</Name>
         <Definition>Angir hva som er hovedbruksområdet for fordeling.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Bruksområde">
                  <EnumItem>Annet bruksområde</EnumItem>
                  <EnumItem>Bru</EnumItem>
                  <EnumItem>Bygning</EnumItem>
                  <EnumItem>Ferist</EnumItem>
                  <EnumItem>Ferjeleie</EnumItem>
                  <EnumItem>Pumper</EnumItem>
                  <EnumItem>Rasteplass</EnumItem>
                  <EnumItem>Signalanlegg</EnumItem>
                  <EnumItem>Skilting</EnumItem>
                  <EnumItem>Spesiell belysning</EnumItem>
                  <EnumItem>Trafikkregistreringsstasjon</EnumItem>
                  <EnumItem>Trafikkregulering/overvåking/ITS</EnumItem>
                  <EnumItem>Tunnel</EnumItem>
                  <EnumItem>Veglys</EnumItem>
                  <EnumItem>Vifter</EnumItem>
                  <EnumItem>Værstasjon</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>plassering</Name>
         <Definition>Angir hvor vegobjektet er plassert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Plassering">
                  <EnumItem>I bygg/rom</EnumItem>
                  <EnumItem>I tunnelrommet</EnumItem>
                  <EnumItem>Ute</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>materiale</Name>
         <Definition>Angir hvilket materiale skap/innkapsling er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Materiale">
                  <EnumItem>Aluminium</EnumItem>
                  <EnumItem>Plast</EnumItem>
                  <EnumItem>Stål</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>parkering</Name>
         <Definition>Angir avstand til nærmeste parkeringsmulighet for servicebil.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Parkering">
                  <EnumItem>&lt; 100m</EnumItem>
                  <EnumItem>&lt; 10m</EnumItem>
                  <EnumItem>&lt; 30m</EnumItem>
                  <EnumItem>&gt; 100m</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høydeOverBakken</Name>
         <Definition>Angir høyde fra bakken til underkant skap. Det gis verdi 0,0 for fordelinger som står på egen sokkel/gulv.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>typeEnhet</Name>
         <Definition>Angir hvilken type/kapsling objektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_TypeEnhet">
                  <EnumItem>Annet</EnumItem>
                  <EnumItem>Klemme</EnumItem>
                  <EnumItem>Koblingsboks</EnumItem>
                  <EnumItem>Skap</EnumItem>
                  <EnumItem>Skinne</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>spenning</Name>
         <Definition>Angir spenningen som leveres til enheten.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Spenning">
                  <EnumItem>12</EnumItem>
                  <EnumItem>230</EnumItem>
                  <EnumItem>230/400</EnumItem>
                  <EnumItem>24</EnumItem>
                  <EnumItem>400</EnumItem>
                  <EnumItem>690</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>adgangskrav</Name>
         <Definition>Angir hvilken type adgang fordelingen er tilrettelagt for.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Adgangskrav">
                  <EnumItem>BA1 - Ukyndige personer</EnumItem>
                  <EnumItem>BA4 - Sakkyndige personer</EnumItem>
                  <EnumItem>BA5 - Instruert personell</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>låsesystem</Name>
         <Definition>Angir hvilket låsesystem som er benyttet på skapet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>nettsystem</Name>
         <Definition>Angir hvordan elkraft er distribuert mellom siste transformator før konsument og konsument.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Nettsystem">
                  <EnumItem>IT</EnumItem>
                  <EnumItem>TN-C</EnumItem>
                  <EnumItem>TN-C-S</EnumItem>
                  <EnumItem>TN-S</EnumItem>
                  <EnumItem>TT</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilknyttetOvervåkningssentral</Name>
         <Definition>Angir hvilken overvåkningssentral tavlen er tilknyttet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_TilknyttetOvervåkningssentral">
                  <EnumItem>Annet sentralt styringssystem</EnumItem>
                  <EnumItem>Datek</EnumItem>
                  <EnumItem>Ingen</EnumItem>
                  <EnumItem>VTS</EnumItem>
                  <EnumItem>VTS Midt</EnumItem>
                  <EnumItem>VTS Nord</EnumItem>
                  <EnumItem>VTS Sør</EnumItem>
                  <EnumItem>VTS Vest</EnumItem>
                  <EnumItem>VTS Øst</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>inngårINødsystemet</Name>
         <Definition>Angir om deler av anlegget inngår i nødsystemet i en tunnel.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_InngårINødsystemet">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>jordelektrode</Name>
         <Definition>Angir hvilken type jordelektrode som er benyttet i anlegget.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Jordelektrode">
                  <EnumItem>Annet</EnumItem>
                  <EnumItem>Cu wiret</EnumItem>
                  <EnumItem>Sammenkoplet med annet nett</EnumItem>
                  <EnumItem>Spyd</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>jordfeilbryter</Name>
         <Definition>Angir hvordan jordfeilbrytere er koblet i anlegget.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Jordfeilbryter">
                  <EnumItem>En for hver kurs</EnumItem>
                  <EnumItem>Felles for alle kurser</EnumItem>
                  <EnumItem>Hver mast</EnumItem>
                  <EnumItem>Ingen</EnumItem>
                  <EnumItem>Kombinasjon</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>jordfeilvarsling</Name>
         <Definition>Angir om anlegget har jordfeilvarsling.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Jordfeilvarsling">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallKurser</Name>
         <Definition>Antall faktiske kurser ut fra fordeling.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>overspenningsvernType</Name>
         <Definition>Angir produsent og typebetegnelse for eventuelt overspenningsvern.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>overbelastningsvernType</Name>
         <Definition>Angir produsent og typebetegnelse for eventuelt overbelastningsvern.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>overbelastningsvernStørrelse</Name>
         <Definition>Angir størrelse på eventuelt overbelastningsvern.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kjølingType</Name>
         <Definition>Angir fabrikat og type for eventuell kjøling.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stedsangivelse</Name>
         <Definition>Angir stedsnavn og/eller eventuell beskrivelse av skapets plassering.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>driftsmerking</Name>
         <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>SCADA_Merking</Name>
         <Definition>Driftsmerking rettet spesifikt mot systemet SCADA.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produsent</Name>
         <Definition>Angir navn på tavlebygger.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavn</Name>
         <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivreferanse</Name>
         <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier_Navn</Name>
         <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Fordeling_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
