<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Fordelingstavle</Name>
  <Definition>Tavle/skap/koblingsboks/punkt hvor strøm fordeles til teknisk utstyr. Benyttes bl.a. i forbindelse med tunnelutstyr</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>typeFordeling</Name>
      <Definition>Angir hvilken type fordeling enheten er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_TypeFordeling">
                <EnumItem>Hovedfordeling</EnumItem>
                <EnumItem>Underfordeling</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hva som er hovedbruksområdet for fordelingstavle.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Bruksområde">
                <EnumItem>Veglys</EnumItem>
                <EnumItem>Bygning</EnumItem>
                <EnumItem>Rasteplass</EnumItem>
                <EnumItem>Signalanlegg</EnumItem>
                <EnumItem>Skilting</EnumItem>
                <EnumItem>Ferjeleie</EnumItem>
                <EnumItem>Tunnel</EnumItem>
                <EnumItem>Værstasjon</EnumItem>
                <EnumItem>Trafikkregistreringsstasjon</EnumItem>
                <EnumItem>Pumper</EnumItem>
                <EnumItem>Vifter</EnumItem>
                <EnumItem>Utsmykking (broer etc)</EnumItem>
                <EnumItem>Trafikkregulering/overvåking</EnumItem>
                <EnumItem>Ferist</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>plassering</Name>
      <Definition>Angir hvor vegobjektet er plassert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Plassering">
                <EnumItem>Ute</EnumItem>
                <EnumItem>I tunnel</EnumItem>
                <EnumItem>I bygg</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>materiale</Name>
      <Definition>Angir hvilket materiale skap/innkapsling er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Materiale">
                <EnumItem>Stål</EnumItem>
                <EnumItem>Aluminium</EnumItem>
                <EnumItem>Plast</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>parkering</Name>
      <Definition>Angir avstand til nærmeste parkeringsmulighet for servicebil.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Parkering">
                <EnumItem>&lt; 10m</EnumItem>
                <EnumItem>&lt; 30m</EnumItem>
                <EnumItem>&lt; 100m</EnumItem>
                <EnumItem>&gt; 100m</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>typeEnhet</Name>
      <Definition>Angir hvilken type/kapsling objektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_TypeEnhet">
                <EnumItem>Skap</EnumItem>
                <EnumItem>Koblingsboks</EnumItem>
                <EnumItem>Klemme</EnumItem>
                <EnumItem>Skinne</EnumItem>
                <EnumItem>Annet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spenning</Name>
      <Definition>Angir spenningen som leveres ut fra enheten.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Spenning">
                <EnumItem>12</EnumItem>
                <EnumItem>24</EnumItem>
                <EnumItem>230</EnumItem>
                <EnumItem>400</EnumItem>
                <EnumItem>230/400</EnumItem>
                <EnumItem>690</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>varmeelement</Name>
      <Definition>Angir om det er montert varmeelement i skapet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Varmeelement">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>låsesystem</Name>
      <Definition>Angir hvilket låsesystem som er benyttet på skapet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kapslingsgrad</Name>
      <Definition>Skapets evne til å motstå støv og vann. Første siffer er støv, andre er vann. Går fra 00 til 68.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Kapslingsgrad">
                <EnumItem>IP00</EnumItem>
                <EnumItem>IP01</EnumItem>
                <EnumItem>IP02</EnumItem>
                <EnumItem>IP03</EnumItem>
                <EnumItem>IP04</EnumItem>
                <EnumItem>IP05</EnumItem>
                <EnumItem>IP06</EnumItem>
                <EnumItem>IP07</EnumItem>
                <EnumItem>IP08</EnumItem>
                <EnumItem>IP10</EnumItem>
                <EnumItem>IP11</EnumItem>
                <EnumItem>IP12</EnumItem>
                <EnumItem>IP13</EnumItem>
                <EnumItem>IP14</EnumItem>
                <EnumItem>IP15</EnumItem>
                <EnumItem>IP16</EnumItem>
                <EnumItem>IP17</EnumItem>
                <EnumItem>IP18</EnumItem>
                <EnumItem>IP20</EnumItem>
                <EnumItem>IP21</EnumItem>
                <EnumItem>IP22</EnumItem>
                <EnumItem>IP23</EnumItem>
                <EnumItem>IP24</EnumItem>
                <EnumItem>IP25</EnumItem>
                <EnumItem>IP26</EnumItem>
                <EnumItem>IP27</EnumItem>
                <EnumItem>IP28</EnumItem>
                <EnumItem>IP30</EnumItem>
                <EnumItem>IP31</EnumItem>
                <EnumItem>IP32</EnumItem>
                <EnumItem>IP33</EnumItem>
                <EnumItem>IP34</EnumItem>
                <EnumItem>IP35</EnumItem>
                <EnumItem>IP36</EnumItem>
                <EnumItem>IP37</EnumItem>
                <EnumItem>IP38</EnumItem>
                <EnumItem>IP40</EnumItem>
                <EnumItem>IP41</EnumItem>
                <EnumItem>IP42</EnumItem>
                <EnumItem>IP43</EnumItem>
                <EnumItem>IP44</EnumItem>
                <EnumItem>IP45</EnumItem>
                <EnumItem>IP46</EnumItem>
                <EnumItem>IP47</EnumItem>
                <EnumItem>IP48</EnumItem>
                <EnumItem>IP50</EnumItem>
                <EnumItem>IP51</EnumItem>
                <EnumItem>IP52</EnumItem>
                <EnumItem>IP53</EnumItem>
                <EnumItem>IP54</EnumItem>
                <EnumItem>IP55</EnumItem>
                <EnumItem>IP56</EnumItem>
                <EnumItem>IP57</EnumItem>
                <EnumItem>IP58</EnumItem>
                <EnumItem>IP60</EnumItem>
                <EnumItem>IP61</EnumItem>
                <EnumItem>IP62</EnumItem>
                <EnumItem>IP63</EnumItem>
                <EnumItem>IP64</EnumItem>
                <EnumItem>IP65</EnumItem>
                <EnumItem>IP66</EnumItem>
                <EnumItem>IP67</EnumItem>
                <EnumItem>IP68</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallKurser</Name>
      <Definition>Antall faktiske kurser ut fra fordeling.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>selektivitet</Name>
      <Definition>Angir i hvilken grad vernet som er nærmest feilstedet løser seg ut ved en feil.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Selektivitet">
                <EnumItem>Delvis</EnumItem>
                <EnumItem>Full</EnumItem>
                <EnumItem>Ingen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilknyttetOvervåkningssentral</Name>
      <Definition>Angir hvilken overvåkningssentral tavlen er tilknyttet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_TilknyttetOvervåkningssentral">
                <EnumItem>VTS</EnumItem>
                <EnumItem>VTS Nord</EnumItem>
                <EnumItem>VTS Midt</EnumItem>
                <EnumItem>VTS Vest</EnumItem>
                <EnumItem>VTS Sør</EnumItem>
                <EnumItem>VTS Øst</EnumItem>
                <EnumItem>Annet sentralt styringssystem</EnumItem>
                <EnumItem>Ingen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>overspenningsvernType</Name>
      <Definition>Angir produsent og typebetegnelse for eventuelt overspenningsvern.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>overbelastningsvernType</Name>
      <Definition>Angir produsent og typebetegnelse for eventuelt overbelastningsvern.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>overbelastningsvernStørrelse</Name>
      <Definition>Angir størrelse på eventuelt overbelastningsvern.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kjølingType</Name>
      <Definition>Angir fabrikat og type for eventuell kjøling.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stedsangivelse</Name>
      <Definition>Angir stedsnavn og/eller eventuell beskrivelse av skapets plassering.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsmerking</Name>
      <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SCADA_Merking</Name>
      <Definition>Driftsmerking rettet spesifikt mot systemet SCADA. .</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produsent</Name>
      <Definition>Angir navn på produsent/fabrikant av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produktnavn</Name>
      <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og evt. serienummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Fordelingstavle_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier_Navn</Name>
      <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
