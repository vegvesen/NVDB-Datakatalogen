<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_FremmedeArter</Name>
  <Definition>Område der det vokser "fremmede" arter, dvs. arter som ikke ønskes spredd langs vegene.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>artsnavn</Name>
      <Definition>Angir navn på art.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Artsnavn">
                <EnumItem>Kjempebjørnekjeks</EnumItem>
                <EnumItem>Tromsøpalme</EnumItem>
                <EnumItem>Kjempespringfrø</EnumItem>
                <EnumItem>Russekål</EnumItem>
                <EnumItem>Hagelupin</EnumItem>
                <EnumItem>Park- og hybridslirekne</EnumItem>
                <EnumItem>Legepestrot</EnumItem>
                <EnumItem>Kanadagullris</EnumItem>
                <EnumItem>Ubestemt Slirekne</EnumItem>
                <EnumItem>Kjempeslirekne</EnumItem>
                <EnumItem>Skogskjegg</EnumItem>
                <EnumItem>Rynkerose</EnumItem>
                <EnumItem>Platanlønn</EnumItem>
                <EnumItem>Fagerfredløs</EnumItem>
                <EnumItem>Hvitsteinkløver</EnumItem>
                <EnumItem>Høstberberis</EnumItem>
                <EnumItem>Hagepastinakk</EnumItem>
                <EnumItem>Andre arter</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forekomstNr</Name>
      <Definition>Angir nr på forekomst innenfor hovedparsell.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lokalitet</Name>
      <Definition>Beskrivelse av stedet. (F.eks. bak støyskjerm, nær stikkrenne).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tetthet</Name>
      <Definition>Gir informasjon om plantetetthet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Tetthet">
                <EnumItem>Tett bestand</EnumItem>
                <EnumItem>Spredt</EnumItem>
                <EnumItem>Få</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallIndivid</Name>
      <Definition>Antall individer av uønsket art på den aktuelle strekningen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forekomstFortsetterPåNaboeiendom</Name>
      <Definition>Angir om arten også er spredt utenfor arealet som tilhører vegeier.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_ForekomstFortsetterPåNaboeiendom">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bestandsbredde</Name>
      <Definition>Gjennomsnittsbredde for den uønskede arten innenfor strekningen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal</Name>
      <Definition>Størrelse på arealet innenfor vegarealet der den fremmede arten forekommer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bekjempelse_Type</Name>
      <Definition>Angir hvilke midler arten bekjempes med.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Bekjempelse_Type">
                <EnumItem>Kantklipp</EnumItem>
                <EnumItem>Kjemisk</EnumItem>
                <EnumItem>Mekanisk fjerning</EnumItem>
                <EnumItem>Annet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bekjempelsesgrad</Name>
      <Definition>Angir i hvilken grad den uønskede fremmede arten bekjempes.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Bekjempelsesgrad">
                <EnumItem>Overvåkes</EnumItem>
                <EnumItem>Begrenses</EnumItem>
                <EnumItem>Bekjempes</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>statusBehandling</Name>
      <Definition>Angir hva som er status for behandling av forekomsten.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_StatusBehandling">
                <EnumItem>Ikke tidligere behandlet</EnumItem>
                <EnumItem>Behandling pågår</EnumItem>
                <EnumItem>Bekjempes/overvåkes</EnumItem>
                <EnumItem>Antatt utryddet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>status_SistEndret</Name>
      <Definition>Angir dato for når egenskapen "Status behandling" sist ble endret.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>registrertDato</Name>
      <Definition>Angir dato for når forekomsten ble registrert i NVDB første gang.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Informasjon som kan være nyttig utover det som er sagt om lokalitet. For eksempel naturbeskrivelse (skog, eng, landbruksareal, nær hager). Andre fremmede arter tas inn her.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_FremmedeArter_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
