<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Friksjonsmåling</Name>
  <Definition>Måling av vegoverflatens friksjon.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>totalDist</Name>
      <Definition>Total utkjørt distanse i m.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>timestamp</Name>
      <Definition>Tidspunkt for måling i millisekunder. Gjelder bare fastslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>MWCount</Name>
      <Definition>Akkumulerte pulser ifra målehjul. Gjelder bare variabelslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spor</Name>
      <Definition>Angir i hvilket spor friksjonsmåling er foretatt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Friksjonsmåling_Spor">
                <EnumItem>Indre</EnumItem>
                <EnumItem>Mellom</EnumItem>
                <EnumItem>Ytre</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>friction</Name>
      <Definition>Friksjonskoeffisient [0 - 1.1], maksimum friksjon for hele oppbremsingsperioden. (Der trykket er størst).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>slip_Prosent</Name>
      <Definition>Slipprosent (mellom MW/TW Speed).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>akselerasjon</Name>
      <Definition>Vertikal akselerasjon i G.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>pressure</Name>
      <Definition>Oljetrykk i bar (foran strupe/bremseventil).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>envTemp</Name>
      <Definition>Utetemperatur i grader C.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>oilTemp</Name>
      <Definition>Oljetemperatur i grader C.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir type måling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Friksjonsmåling_Type">
                <EnumItem>Fast slippmåling</EnumItem>
                <EnumItem>Variabel slippmåling</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>rosita_id</Name>
      <Definition>Gir id for måledatafil i Rosita.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>surfTemp</Name>
      <Definition>Overflatetemperatur på vei i grader C.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>TWSpeed</Name>
      <Definition>Hengerhjulshastighet i km/t.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>F60</Name>
      <Definition>Friksjon ved slipphastighet 60 km/t på målehjulet. Eksempel: Dersom målehjulet starter oppbremsingen ved 80 km/t, er F60 friksjonen når målehjulets hastighet er 20 km/t, altså 60 km/t under det oppbremsingen startet ved. Gjelder bare variabelslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>MWSpeed</Name>
      <Definition>Målehjulshastighet i km/t. Gjelder bare fastslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>my_min</Name>
      <Definition>Laveste målte friksjon etter maksimum friksjon. Gjelder bare variabelslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>sp</Name>
      <Definition>Stigningstall for oppbremsingskurven. Gjelder bare variabelslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>R</Name>
      <Definition>Godhetsfaktor mellom [-1, 1] hvor -1 er best. Skal ligge i praksis mellom [0, -1]. Gjelder bare variabelslippmålinger.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>måleformål</Name>
      <Definition>Angir formål med friksjonsmåling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Friksjonsmåling_Måleformål">
                <EnumItem>Generelle målinger</EnumItem>
                <EnumItem>Forsøk</EnumItem>
                <EnumItem>Kalibrering</EnumItem>
                <EnumItem>Referanse-1</EnumItem>
                <EnumItem>Referanse-2</EnumItem>
                <EnumItem>Referanse-3</EnumItem>
                <EnumItem>Initiell-1</EnumItem>
                <EnumItem>Initiell-2</EnumItem>
                <EnumItem>Initiell-3</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
