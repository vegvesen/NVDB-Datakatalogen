<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Gangfelt</Name>
  <Definition>Oppmerket felt for fotgjengere (1)</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>stedsangivelse</Name>
      <Definition>Stedsnavn hvor forekomst er lokalisert. Ev. plassering i relatert til til stedsnavn. Detaljplassering i kryssområder.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>markeringAvStriper</Name>
      <Definition>Angir hvordan striper er markert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_MarkeringAvStriper">
                <EnumItem>Malte striper</EnumItem>
                <EnumItem>Vekslende materialtype</EnumItem>
                <EnumItem>Ikke striper</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>hevet</Name>
      <Definition>Angir om gangfeltet er hevet i forhold til vegplanet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Hevet">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skiltet</Name>
      <Definition>Angir om det er skiltet eller ikke i forbindelse med vegobjektet.  Angir om det er skiltet for gangfelt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Skiltet">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lysregulert</Name>
      <Definition>Angir om vegobjektet er lysregulert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Lysregulert">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>belysning</Name>
      <Definition>Angir om det er belysning i tilknytning til vegobjektet og eventuelt hvilken type belysning det er.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Belysning">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>trafikkøy</Name>
      <Definition>Angir om det er trafikkøy i tilknytning til vegobjektet.  Angir om det er trafikkøy i gangfeltet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Trafikkøy">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nedsenketKantstein</Name>
      <Definition>Angir om det er nedsenket kantstein eller ikke i forbindelse med vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_NedsenketKantstein">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
                <EnumItem>Ikke kantstein</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>belegning_Type</Name>
      <Definition>Angir type belegning for gangfeltet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Belegning_Type">
                <EnumItem>Stein</EnumItem>
                <EnumItem>Betongstein</EnumItem>
                <EnumItem>Betongheller</EnumItem>
                <EnumItem>Skiferheller</EnumItem>
                <EnumItem>Betong</EnumItem>
                <EnumItem>Asfalt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>belegning_Areal</Name>
      <Definition>Angir areal av belegning i forbindelse med gangfelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedtaksnummer</Name>
      <Definition>Angir vedtaksnummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivnummer</Name>
      <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>FKB_ID</Name>
      <Definition>Refererer til FKB-identitet. Benyttes i forbindelse med felles forvaltning av geometri.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Gangfelt_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
