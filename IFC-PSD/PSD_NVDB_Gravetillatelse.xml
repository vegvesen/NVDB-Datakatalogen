<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_GRAVETILLATELSE</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>bakgrunnForGraving</Name>
         <Definition>Angir hvilken type graving/tillatelse det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Gravetillatelse_BakgrunnForGraving">
                  <EnumItem>Akutt vedlikehold</EnumItem>
                  <EnumItem>Nyetablering</EnumItem>
                  <EnumItem>Planlagt vedlikehold</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>graveformål</Name>
         <Definition>Angir formål for graving.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Gravetillatelse_Graveformål">
                  <EnumItem>Annet formål</EnumItem>
                  <EnumItem>Avfall</EnumItem>
                  <EnumItem>Drenering</EnumItem>
                  <EnumItem>Ecom</EnumItem>
                  <EnumItem>Fjernvarme/kjøling</EnumItem>
                  <EnumItem>Olje, gass</EnumItem>
                  <EnumItem>Strømforsyning</EnumItem>
                  <EnumItem>Vann, avløp, overvann</EnumItem>
                  <EnumItem>Vegvedlikehold</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gravested</Name>
         <Definition>Angir hvor det er gitt tillatelse til graving relatert til vegen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Gravetillatelse_Gravested">
                  <EnumItem>I/langs veg</EnumItem>
                  <EnumItem>Kryssing av veg</EnumItem>
                  <EnumItem>Utenfor vegkant</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>framføring</Name>
         <Definition>Angir hvilken type framføring som primært skal benyttes. Dersom det er ulike typer framføring angis den som benyttes ved kryssing av veg. Om det ikke er kryssing av veg benyttes den som er mest framtredende. Angi beskrivelse i tilleggsinformasjon.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Gravetillatelse_Framføring">
                  <EnumItem>Annen framføring</EnumItem>
                  <EnumItem>Boring</EnumItem>
                  <EnumItem>Graving</EnumItem>
                  <EnumItem>Luftspenn</EnumItem>
                  <EnumItem>Microtrenching</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dato_SøknadMottatt</Name>
         <Definition>Angir dato for når søknad ble mottatt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dato_PlanlagtOppstart</Name>
         <Definition>Angir hvilken dato graving har planlagt start/startet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dato_PlanlagtFerdigstillelse</Name>
         <Definition>Angir hvilken dato graving er planlagt/ble ferdigstilt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gravedybde</Name>
         <Definition>Angir planlagt gravedybde.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>ledningseier</Name>
         <Definition>Navn på firma/etat som er ansvarlig for graving og formell søker om gravetillatelsen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>entreprenør</Name>
         <Definition>Navn på firma/entreprenør som skal utføre gravearbeidet på vegne av ledningseier.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivnummer</Name>
         <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>innmålingsdataLevert</Name>
         <Definition>Angir om det er levert innmålingsdata som dokumenterer geografisk plassering av tiltak.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Gravetillatelse_InnmålingsdataLevert">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dato_EgenkontrollskjemaMottatt</Name>
         <Definition>Dato når egenkontrollskjema ble levert.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dato_Garantiutløp</Name>
         <Definition>Angir dato garantitid utløper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
