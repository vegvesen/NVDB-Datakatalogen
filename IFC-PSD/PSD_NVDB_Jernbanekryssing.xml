<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_JERNBANEKRYSSING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>typeJernbanekryssing</Name>
         <Definition>Angir hvilken type vegobjektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Jernbanekryssing_TypeJernbanekryssing">
                  <EnumItem>I plan</EnumItem>
                  <EnumItem>I plan, lysregulert og bommer</EnumItem>
                  <EnumItem>I plan, lysregulert, grind</EnumItem>
                  <EnumItem>I plan, lysregulert, halve bommer</EnumItem>
                  <EnumItem>I plan, lysregulert, hele bommer</EnumItem>
                  <EnumItem>I plan, lysregulert, uten bommer</EnumItem>
                  <EnumItem>I plan, uten lysregulering og bommer</EnumItem>
                  <EnumItem>Veg over</EnumItem>
                  <EnumItem>Veg under</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>særskiltFare</Name>
         <Definition>Særskilte faremoment som trafikantene bør ta hensyn til ved passering av jernbanesporet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Jernbanekryssing_SærskiltFare">
                  <EnumItem>Lange og lave kjøretøy kan henge fast på jernbanesporet</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
