<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Jevnhetsmåling_AvledaPr20M</Name>
  <Definition>Resulterende jevnhetsmåling for en 20-metersstrekning.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>måledato</Name>
      <Definition>Angir måledato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDate"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>jevnhetsformål</Name>
      <Definition>Angir formal med spormåling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Jevnhetsmåling_AvledaPr20M_Jevnhetsformål">
                <EnumItem>Generelle målinger</EnumItem>
                <EnumItem>Referanse-1</EnumItem>
                <EnumItem>Referanse-2</EnumItem>
                <EnumItem>Referanse-3</EnumItem>
                <EnumItem>Forsøksmålinger</EnumItem>
                <EnumItem>Teleløsning</EnumItem>
                <EnumItem>Initiell-1</EnumItem>
                <EnumItem>Initiell-2</EnumItem>
                <EnumItem>Initiell-3</EnumItem>
                <EnumItem>Måleserie a</EnumItem>
                <EnumItem>Måleserie b</EnumItem>
                <EnumItem>Måleserie c</EnumItem>
                <EnumItem>Måleserie d</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utstyrsnummer</Name>
      <Definition>Angir hvilket utstyr som er benytta.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spor_Laser</Name>
      <Definition>Angir hvilket spor dataene gjelder.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Jevnhetsmåling_AvledaPr20M_Spor_Laser">
                <EnumItem>Indre</EnumItem>
                <EnumItem>Mellom</EnumItem>
                <EnumItem>Ytre</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>IRI_Beregningslengde</Name>
      <Definition>Angir IRI-Beregningslengde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>IRI_Jevnhetsindeks</Name>
      <Definition>Angir IRI-Jevnhetsindeks.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>MPD_Middelverdi</Name>
      <Definition>Midlere profilhøyde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>MPD_Standardavvik</Name>
      <Definition>Midlere profilhøyde, standardavvik.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>HV_Median</Name>
      <Definition>Helningsvinkel.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>HV_IQR</Name>
      <Definition>Helningsvinkel, IQR (Inter-quartile range) Innenfor 25-75%.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>rosita_id</Name>
      <Definition>Gir id for måledatafil/profil i Rosita.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
