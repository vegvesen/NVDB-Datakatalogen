<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_KANTFORSTERKNING_TEST</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>slitelag_Type</Name>
         <Definition>Angir hvilken type bærelag som er benyttet. Oppgi kortnavn for massetype og maks steinstørrelse. Eksempel Agb 11.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>slitelag_Tykkelse</Name>
         <Definition>Angir tykkelse av slitelag</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bindlag_Type</Name>
         <Definition>Angir hvilken type bærelag som er benyttet. Oppgi kortnavn for massetype og maks steinstørrelse. Eksempel Agb 11.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bindlag_Tykkelse</Name>
         <Definition>angir tykkelse av bindlag</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bærelag_Type</Name>
         <Definition>Angir hvilken type bærelag som er benyttet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kantforsterkning (test)_Bærelag_Type">
                  <EnumItem>Asfaltert grus</EnumItem>
                  <EnumItem>Asfaltert pukk</EnumItem>
                  <EnumItem>Bitumenanriket grus</EnumItem>
                  <EnumItem>Bitumenstabiliserte materialer</EnumItem>
                  <EnumItem>Bitumesnstabilisert grus</EnumItem>
                  <EnumItem>Dypfrest, bitumenstabilisert</EnumItem>
                  <EnumItem>Dypfrest, ligninstabilisert</EnumItem>
                  <EnumItem>Dypfrest, mekanisk stabilisert</EnumItem>
                  <EnumItem>Emulsjonsgrus</EnumItem>
                  <EnumItem>Emulsjonspukk</EnumItem>
                  <EnumItem>Forkilt pukk</EnumItem>
                  <EnumItem>Gjenbruksbetong</EnumItem>
                  <EnumItem>Knust asfalt</EnumItem>
                  <EnumItem>Knust berg</EnumItem>
                  <EnumItem>Knust grus</EnumItem>
                  <EnumItem>Ligninstabilisert materiale</EnumItem>
                  <EnumItem>Mekanisk stabiliserte materialer</EnumItem>
                  <EnumItem>Penetrert pukk</EnumItem>
                  <EnumItem>Sementstabilisert grus</EnumItem>
                  <EnumItem>Sementstabilisert pukk</EnumItem>
                  <EnumItem>Sementstabiliserte materialer</EnumItem>
                  <EnumItem>Skumgrus</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bærelag_Steinstørrelse</Name>
         <Definition>Angir største steinstørrelse for bærelag.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bærelag_Tykkelse</Name>
         <Definition>Angir tykkelse av bærelag</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>forsterkningslag_Steinstørrelse</Name>
         <Definition>Angir største steinstørrelse for forsterkningslag.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>forsterkningslag_Tykkelse</Name>
         <Definition>Angir tykkelse av forsterkningslag</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
