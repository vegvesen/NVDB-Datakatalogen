<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_KOLLEKTIVKNUTEPUNKT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>navn</Name>
         <Definition>Angir navn på kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>nivå</Name>
         <Definition>Angir hvilket nivå kollektivknutepunktet tilhører.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Nivå">
                  <EnumItem>Lokalt</EnumItem>
                  <EnumItem>Mindre</EnumItem>
                  <EnumItem>Nasjonalt</EnumItem>
                  <EnumItem>Regionalt</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderBuss</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av buss.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderBuss">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderTrikkBybane</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av trikk/bybane.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderTrikkBybane">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>NSR_Stopplace_ID</Name>
         <Definition>Unik ID som refererer til Stopplace (stoppested) i Nasjonalt stoppestedsregister (NSR). Benyttes for datautveksling mellom NVDB og NSR.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderTog</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av tog.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderTog">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderRutebåt</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av rutebåt.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderRutebåt">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderFerje</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av ferje.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderFerje">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderFly</Name>
         <Definition>Angir om kollektivknutepunktet benyttes av fly.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderFly">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderDrosje</Name>
         <Definition>Angir om det er skiltet drosjeholdeplass i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_GjelderDrosje">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallStoppunkt_Test</Name>
         <Definition>Angir hvor mange stoppunkt det er knyttet til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>venterom</Name>
         <Definition>Angir om det er venterom i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Venterom">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Ja, tilgjengelig</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>toalett_Test</Name>
         <Definition>Angir om det er offentlig toalett i tilknytning til kollektivknutepunktet, og om det eventuelt er tilgjengelig for forflytningshemmede.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Toalett_Test">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Ja, tilgjengelig</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>billettsalg</Name>
         <Definition>Angir om det er billettsalg i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Billettsalg">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Ja, automat</EnumItem>
                  <EnumItem>Ja, kiosk/skranke</EnumItem>
                  <EnumItem>Ja, kiosk/skranke og automat</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sykkelparkering_Test</Name>
         <Definition>Angir om det er sykkelparkering i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Sykkelparkering_Test">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>parkering_Test</Name>
         <Definition>Angir om det er parkeringsplass i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Parkering_Test">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>parkeringForForflytningshemmede_Test</Name>
         <Definition>Angir om det er parkering for forflytningshemmede i tilknytning til kollektivknutepunktet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_ParkeringForForflytningshemmede_Test">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sistVerifisert</Name>
         <Definition>Angir data for når egenskapene sist var verifisert.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Kollektivknutepunkt_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
