<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Kontraktsområde</Name>
  <Definition>Kontrakt/avtale om drift/vedlikehold relatert til bestemte veger innenfor et avgrensa område.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Angir navn på kontraktsområdet. Navnet skal settes sammen etter følgende mønster: "NNNN n..........n ÅÅÅÅ-ÅÅÅÅ". "NNNN" er et firesifret nummer knyttet til kontraktsområde, nr &lt; 2100 kan bli tolka som at de to første siffer er fylkesnummer. For siffer mindre enn 1000 skal det gis 0 (f.eks. 0350). "n.....n" er navn på kontraktsområdet. "ÅÅÅÅ-ÅÅÅÅ" angir årstall kontrakten løper fra og til. Eksempel på navn: "1203 Nordhordland 2004-2008.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nr</Name>
      <Definition>Angir nummer på kontraktsområde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type kotraktsområde det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kontraktsområde_Type">
                <EnumItem>Driftskontrakt</EnumItem>
                <EnumItem>OPS-kontrakt</EnumItem>
                <EnumItem>Refusjonsavtale</EnumItem>
                <EnumItem>Elektrokontrakt</EnumItem>
                <EnumItem>Veglyskontrakt</EnumItem>
                <EnumItem>Tunnelkontrakt</EnumItem>
                <EnumItem>Grøntkontrakt</EnumItem>
                <EnumItem>Vegoppmerkingskontrakt</EnumItem>
                <EnumItem>Rekkverkskontrakt</EnumItem>
                <EnumItem>Andrepartskontrakt</EnumItem>
                <EnumItem>Utprøving</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoFra</Name>
      <Definition>Kontrakten løper fra og med angitt dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoTil</Name>
      <Definition>Kontrakten løper til og med angitt dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av kontrakt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kontraktsområde_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Viken fylkeskommune</EnumItem>
                <EnumItem>Innlandet fylkeskommune</EnumItem>
                <EnumItem>Vestfold og Telemark fylkeskommune</EnumItem>
                <EnumItem>Agder fylkeskommune</EnumItem>
                <EnumItem>Rogaland fylkeskommune</EnumItem>
                <EnumItem>Vestland fylkeskommune</EnumItem>
                <EnumItem>Møre og Romsdal fylkeskommune</EnumItem>
                <EnumItem>Trøndelag fylkeskommune</EnumItem>
                <EnumItem>Nordland fylkeskommune</EnumItem>
                <EnumItem>Troms og Finnmark fylkeskommune</EnumItem>
                <EnumItem>Oslo kommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fellesEierskapMed</Name>
      <Definition>Angir eier medeier. Om det er flere enn 2 eiere velges verdi "Flere enn 2 eiere". Informasjon om eierforhold legges da som tekst under tilleggsinformasjon.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kontraktsområde_FellesEierskapMed">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Viken fylkeskommune</EnumItem>
                <EnumItem>Innlandet fylkeskommune</EnumItem>
                <EnumItem>Vestfold og Telemark fylkeskommune</EnumItem>
                <EnumItem>Agder fylkeskommune</EnumItem>
                <EnumItem>Rogaland fylkeskommune</EnumItem>
                <EnumItem>Vestland fylkeskommune</EnumItem>
                <EnumItem>Møre og Romsdal fylkeskommune</EnumItem>
                <EnumItem>Trøndelag fylkeskommune</EnumItem>
                <EnumItem>Nordland fylkeskommune</EnumItem>
                <EnumItem>Troms og Finnmark fylkeskommune</EnumItem>
                <EnumItem>Oslo kommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Flere enn 2 eiere</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
