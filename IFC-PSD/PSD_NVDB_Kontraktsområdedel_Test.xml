<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Kontraktsområdedel_Test</Name>
  <Definition>En eller flere vegstrekninger som inngår i et kontraktsområde.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Angir navn på kontraktsområdet. Navn er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nr</Name>
      <Definition>Angir nummer på kontraktsområde. Nr er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type kotraktsområde det er tale om. Type er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kontraktsområdedel_Test_Type">
                <EnumItem>Driftskontrakt</EnumItem>
                <EnumItem>OPS-kontrakt</EnumItem>
                <EnumItem>Refusjonsavtale</EnumItem>
                <EnumItem>Elektrokontrakt</EnumItem>
                <EnumItem>Veglyskontrakt</EnumItem>
                <EnumItem>Tunnelkontrakt</EnumItem>
                <EnumItem>Grøntkontrakt</EnumItem>
                <EnumItem>Vegoppmerkingskontrakt</EnumItem>
                <EnumItem>Andrepartskontrakt</EnumItem>
                <EnumItem>Utprøving</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoFra</Name>
      <Definition>Kontrakten løper fra og med angitt dato. Dato-fra er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoTil</Name>
      <Definition>Kontrakten løper til og med angitt dato. Dato-til er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon for spesifikk strekning/delområde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Prosjektreferanse er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av kontrakt. Eier er kopiert fra Kontraktsområde (580). Egenskapstype skal ikke oppdateres manuelt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kontraktsområdedel_Test_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Viken fylkeskommune</EnumItem>
                <EnumItem>Innlandet fylkeskommune</EnumItem>
                <EnumItem>Vestfold og Telemark fylkeskommune</EnumItem>
                <EnumItem>Agder fylkeskommune</EnumItem>
                <EnumItem>Rogaland fylkeskommune</EnumItem>
                <EnumItem>Vestland fylkeskommune</EnumItem>
                <EnumItem>Møre og Romsdal fylkeskommune</EnumItem>
                <EnumItem>Trøndelag fylkeskommune</EnumItem>
                <EnumItem>Nordland fylkeskommune</EnumItem>
                <EnumItem>Troms og Finnmark fylkeskommune</EnumItem>
                <EnumItem>Oslo kommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
