<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_KRYSSINGSMULIGHETÅPNING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>bruksområde</Name>
         <Definition>Angir bruksområde for kryssingsmulighet/åpning.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_Bruksområde">
                  <EnumItem>Annet bruksområde</EnumItem>
                  <EnumItem>Kjøreveg g/s-veg</EnumItem>
                  <EnumItem>Midtdeler</EnumItem>
                  <EnumItem>Smal åpning i siderekkverk</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sikringAvÅpning</Name>
         <Definition>Angir hvordan åpning er sikret.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_SikringAvÅpning">
                  <EnumItem>Annen type sikring</EnumItem>
                  <EnumItem>Bom</EnumItem>
                  <EnumItem>Demonterbart rekkverk</EnumItem>
                  <EnumItem>Drifts-/katastrofeåpning</EnumItem>
                  <EnumItem>Ikke fysisk sikring</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavnTypegodkjenning</Name>
         <Definition>Refererer til produktnavn og typegodkjenning (samsvarsgodkjenning) for drifts-/katastrofeåpninger godkjent av Statens vegvesen. Se https://www.vegvesen.no/fag/teknologi/vegutstyr/</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_ProduktnavnTypegodkjenning">
                  <EnumItem>Duo-Gate double (H2 - W3)</EnumItem>
                  <EnumItem>E-CADO (H2-W2)</EnumItem>
                  <EnumItem>Gate-Guard (H2-W7)</EnumItem>
                  <EnumItem>Monoline nød- driftsåpning (N2-W4)</EnumItem>
                  <EnumItem>Oslobommen (N2-W5)</EnumItem>
                  <EnumItem>SAB-Rekkverk Åpning (H2-W6)</EnumItem>
                  <EnumItem>SafeLine M nød-/driftsåpning (H1-W3)</EnumItem>
                  <EnumItem>SoloGuard-Rekkverk Åpning (H1-W1)</EnumItem>
                  <EnumItem>Tidligere godkjent type</EnumItem>
                  <EnumItem>Ukjent</EnumItem>
                  <EnumItem>Vario-Guard MUF (H2-W5)</EnumItem>
                  <EnumItem>VEVA 3 (H2-W7)</EnumItem>
                  <EnumItem>Vik eo drifts-/katastrofeåpning (H2-W4)</EnumItem>
                  <EnumItem>Vik eo drifts-/katastrofeåpning (L2-W4)</EnumItem>
                  <EnumItem>Vik eo drifts-/katastrofeåpning (N2-W1)</EnumItem>
                  <EnumItem>VIK-EP nødåpning (H1/L1-W3)</EnumItem>
                  <EnumItem>VIK-EP nødåpning (N2-W3)</EnumItem>
                  <EnumItem>VS-Stalpro (H1-W3)</EnumItem>
                  <EnumItem>Nordic M H1 QO (H1-W4)</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>styrkeklasse</Name>
         <Definition>Angir hvilken styrkeklasse vegobjektet har.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_Styrkeklasse">
                  <EnumItem>H1</EnumItem>
                  <EnumItem>H2</EnumItem>
                  <EnumItem>H3</EnumItem>
                  <EnumItem>H4a</EnumItem>
                  <EnumItem>H4b</EnumItem>
                  <EnumItem>L1</EnumItem>
                  <EnumItem>L2</EnumItem>
                  <EnumItem>L3</EnumItem>
                  <EnumItem>L4a</EnumItem>
                  <EnumItem>L4b</EnumItem>
                  <EnumItem>N1</EnumItem>
                  <EnumItem>N2</EnumItem>
                  <EnumItem>T1</EnumItem>
                  <EnumItem>T2</EnumItem>
                  <EnumItem>T3</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bredde</Name>
         <Definition>Angir bredde av kryssing/åpning. For åpninger på midtdeler måles bredden i vegens lengderetning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_KryssingsmulighetÅpning_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
