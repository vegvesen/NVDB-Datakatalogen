<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Kulturminne_Vegminne</Name>
  <Definition>Kulturminne (jf. definisjonen på et kulturminne fra Kulturminneloven § 2) som er spor etter menneskelig virksomhet knyttet til samferdsel og veg.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>kategori</Name>
      <Definition>Angir hvilken hovedkategori vegminnet tilhører. Det er avhengighet til ET "Enkeltminneart".</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_Kategori">
                <EnumItem>Veg</EnumItem>
                <EnumItem>Bru</EnumItem>
                <EnumItem>Vegutstyr</EnumItem>
                <EnumItem>Bygning</EnumItem>
                <EnumItem>Vegrelatert kulturminne</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>enkeltminneart</Name>
      <Definition>Angir hvilken enkeltminneart vegminne tilhører. Begrepet er definert i Askeladden.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_Enkeltminneart">
                <EnumItem>Vegfar</EnumItem>
                <EnumItem>Rideveg</EnumItem>
                <EnumItem>Postveg</EnumItem>
                <EnumItem>Høgfjellsveg</EnumItem>
                <EnumItem>Kongeveg</EnumItem>
                <EnumItem>Kjerreveg-fransk prinsipp</EnumItem>
                <EnumItem>Chaussé</EnumItem>
                <EnumItem>Mellomriksveg</EnumItem>
                <EnumItem>Motorveg</EnumItem>
                <EnumItem>Veg, 1. generasjons bilveg (før 1960)</EnumItem>
                <EnumItem>Veg, 2-felts bilveg</EnumItem>
                <EnumItem>Forbindelsesveg</EnumItem>
                <EnumItem>Bevegelig bru</EnumItem>
                <EnumItem>Bjelkebru</EnumItem>
                <EnumItem>Buebru</EnumItem>
                <EnumItem>Fagverksbru / Hengverksbru</EnumItem>
                <EnumItem>Flytebru</EnumItem>
                <EnumItem>Fritt frambygd bru / Kassebru</EnumItem>
                <EnumItem>Hellebru</EnumItem>
                <EnumItem>Hengebru</EnumItem>
                <EnumItem>Hvelvbru</EnumItem>
                <EnumItem>Kavlbru</EnumItem>
                <EnumItem>Mastebru</EnumItem>
                <EnumItem>Platebru</EnumItem>
                <EnumItem>Skråstagbru</EnumItem>
                <EnumItem>Sprengverkbru</EnumItem>
                <EnumItem>Allé</EnumItem>
                <EnumItem>Kryss/Rundkjøring</EnumItem>
                <EnumItem>Vegknute</EnumItem>
                <EnumItem>Rekkverk</EnumItem>
                <EnumItem>Rasteplass</EnumItem>
                <EnumItem>Støttemur</EnumItem>
                <EnumItem>Støyskjerm</EnumItem>
                <EnumItem>Vegkunst</EnumItem>
                <EnumItem>Vegmerker/skilt</EnumItem>
                <EnumItem>Båtdrag / Trallebane</EnumItem>
                <EnumItem>Båtvorr/Steinkai</EnumItem>
                <EnumItem>Ferjekai</EnumItem>
                <EnumItem>Ferjekaibru</EnumItem>
                <EnumItem>Landkar/Pilar/Pontong</EnumItem>
                <EnumItem>Gangstein/Vadestein</EnumItem>
                <EnumItem>Varde/Varderekke</EnumItem>
                <EnumItem>Tunnel</EnumItem>
                <EnumItem>Tunnelportal</EnumItem>
                <EnumItem>Skredoverbygg</EnumItem>
                <EnumItem>Kulvert</EnumItem>
                <EnumItem>Stikkrenne</EnumItem>
                <EnumItem>Bygate</EnumItem>
                <EnumItem>Gang- og sykkelveg</EnumItem>
                <EnumItem>Lokalveg</EnumItem>
                <EnumItem>Miljøgate</EnumItem>
                <EnumItem>Bomstasjon</EnumItem>
                <EnumItem>Oppsynsmannsbrakke</EnumItem>
                <EnumItem>Garasje</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Navn på vegminne.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelseAvVegminnet</Name>
      <Definition>Beskrivelse av vegminne.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datering</Name>
      <Definition>Sannsynlig årstall eller tidsperiode vegminnet er datert til.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vernestatus</Name>
      <Definition>Juridisk vern, hjemlet i lov, forskrift, plan mv.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_Vernestatus">
                <EnumItem>Statlig listeført</EnumItem>
                <EnumItem>Automatisk fredet</EnumItem>
                <EnumItem>Forskriftsfredet</EnumItem>
                <EnumItem>Vedtaksfredet</EnumItem>
                <EnumItem>Vernet etter plan- og bygningsloven</EnumItem>
                <EnumItem>Uklar vernestatus</EnumItem>
                <EnumItem>Ikke vernet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>verneplannummer</Name>
      <Definition>Viser til objektnummer i "Vegvalg - Nasjonal verneplan for veger, bruer og vegrelaterte kulturminner" (Statens vegvesen, 2002).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>askeladden_ID</Name>
      <Definition>Gir referanse til unik ID i Askeladden.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>linkTilKulturminnesøk</Name>
      <Definition>Gir link til Kulturminnesøk, dvs. innsynsløsning for Askeladden. Gir mer detaljert informasjon om forekomst.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>linkTilFredningspresiseringer</Name>
      <Definition>Link til dokument som beskriver hva fredning innebærer i praksis for gitt vegobjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>status</Name>
      <Definition>Angir hvilken status vegminnet har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_Status">
                <EnumItem>I trafikk</EnumItem>
                <EnumItem>Ute av bruk/nedlagt</EnumItem>
                <EnumItem>Fjernet/borte</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>restriksjonGrøftKantrensk</Name>
      <Definition>Angir om det skal tas spesielle hensyn ved grøft/kantrensk.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_RestriksjonGrøftKantrensk">
                <EnumItem>Utføres etter særskilt beskrivelse</EnumItem>
                <EnumItem>Utføres som avtalt i driftskontrakt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>restriksjonKantklipp</Name>
      <Definition>Angir om det skal tas spesielle hensyn ved kantklipp.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_RestriksjonKantklipp">
                <EnumItem>Utføres etter særskilt beskrivelse</EnumItem>
                <EnumItem>Utføres som avtalt i driftskontrakt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>andreRestriksjoner</Name>
      <Definition>Beskriver andre restriksjoner knyttet til drift og vedlikehold utover grøft/kantrensk og kantklipp.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>sistKontrollert</Name>
      <Definition>Angir hvilken status vegminnet har.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDate"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Angir informasjon fortrinnsvis knyttet til forvaltning, drift og vedlikehold.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av kulturminne.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kulturminne_Vegminne_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Stat, øvrig</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Ukjent</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
