<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Kum</Name>
  <Definition>Konstruksjon som benyttes i forbindelse med kopling av rørledninger og/eller inntak av vann gjennom rist/sluk. Merknad: Kum for kopling av trekkerør og kabler er definert som egen vegobjekttype Trekkekum.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type vegobjektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Type">
                <EnumItem>Standard kum</EnumItem>
                <EnumItem>Hjelpesluk</EnumItem>
                <EnumItem>Standard kum m sandfang</EnumItem>
                <EnumItem>Firkantkum</EnumItem>
                <EnumItem>Gategutt</EnumItem>
                <EnumItem>Spesialkum</EnumItem>
                <EnumItem>Spesialkum, prefabrikkert</EnumItem>
                <EnumItem>Spesialkum plasstøpt</EnumItem>
                <EnumItem>Spesialkum, murt</EnumItem>
                <EnumItem>Spesialkum, infiltreringskum</EnumItem>
                <EnumItem>Kum sprengt i fjell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hva vegobjektet brukes til, hvilken funksjon det har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Bruksområde">
                <EnumItem>Drenering</EnumItem>
                <EnumItem>Inspeksjonskum</EnumItem>
                <EnumItem>Inntak/utløp stikkrenne</EnumItem>
                <EnumItem>Spillvannskum</EnumItem>
                <EnumItem>Fallkum</EnumItem>
                <EnumItem>Vannkum</EnumItem>
                <EnumItem>Pumpekum</EnumItem>
                <EnumItem>Brannkum</EnumItem>
                <EnumItem>Fjernvarme</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>materialtype</Name>
      <Definition>Angir primær materialtype for vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Materialtype">
                <EnumItem>Betong</EnumItem>
                <EnumItem>Plast</EnumItem>
                <EnumItem>Naturstein</EnumItem>
                <EnumItem>Teglstein, murstein</EnumItem>
                <EnumItem>Betongstein</EnumItem>
                <EnumItem>Rustfritt stål</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>plassering</Name>
      <Definition>Angir hvor kummen er plassert.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Plassering">
                <EnumItem>I grøft</EnumItem>
                <EnumItem>I vegbane</EnumItem>
                <EnumItem>På vegskulder</EnumItem>
                <EnumItem>I g/s-veg</EnumItem>
                <EnumItem>På fortau</EnumItem>
                <EnumItem>I trafikkdeler</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lokkRist_Type</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_LokkRist_Type">
                <EnumItem>Tett lokk, støpjern</EnumItem>
                <EnumItem>Tett lokk, betong</EnumItem>
                <EnumItem>Tett lokk, plast</EnumItem>
                <EnumItem>Tett lokk, tre</EnumItem>
                <EnumItem>Støpjernslokk m hel rist</EnumItem>
                <EnumItem>Støpjernslokk m halv rist</EnumItem>
                <EnumItem>Kuppelrist, flat</EnumItem>
                <EnumItem>Kuppelrist, lav</EnumItem>
                <EnumItem>Kuppelrist, høy</EnumItem>
                <EnumItem>Slukrist, i gateplan</EnumItem>
                <EnumItem>Slukrist, vertikal</EnumItem>
                <EnumItem>Betonglokk m liten rist</EnumItem>
                <EnumItem>Skrårist</EnumItem>
                <EnumItem>Åpen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spesiellBelegningRundtKum</Name>
      <Definition>Angir om det er spesiell belegning i et belte/firkant rundt kum for å lette vedlikehold og gjøre kum mer synlig.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_SpesiellBelegningRundtKum">
                <EnumItem>Asfalt</EnumItem>
                <EnumItem>Stein</EnumItem>
                <EnumItem>Betong</EnumItem>
                <EnumItem>Ingen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>diameter</Name>
      <Definition>Angir indre diameter av kum.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>diameter_Åpning</Name>
      <Definition>Angir diameter på kummens åpning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dybde</Name>
      <Definition>Angir dybde målt fra topp kumlokk til innvendig bunn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dybdeTilUtløp</Name>
      <Definition>Angir dybde fra topp kum til underkant (innvendig rør) av utløp.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde1_Firkantkum</Name>
      <Definition>Angir bredde 1 av firkantkum.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde2_Firkantkum</Name>
      <Definition>Angir bredde 2 av firkantkum.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstandFraVegkant</Name>
      <Definition>Angir avstand fra vegkant til senter kumlokk.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stige</Name>
      <Definition>Angir hva slags stige det er i kummen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Stige">
                <EnumItem>Stige</EnumItem>
                <EnumItem>Innstøpte trinn</EnumItem>
                <EnumItem>Ikke stige</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>harFlyteramme</Name>
      <Definition>Angir om kummen har flyteramme eller ikke.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_HarFlyteramme">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avløp</Name>
      <Definition>Angir hvor avløpet fra kummen ledes.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Avløp">
                <EnumItem>Rørledning til resipient</EnumItem>
                <EnumItem>Avløp til offentlig system</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dykker</Name>
      <Definition>Angir om det er dykker i tilknytning til kummen. Dykker er en konstruksjon som skal hindre slitasje på kummen når det føres mye vann inn i kummen på et nivå høyere enn bunnen på kummen. Dykker kan også brukes for å føre inn en slange med vann eller damp for tining av frosne kummer eller ledninger.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Dykker">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>oljeutskilling</Name>
      <Definition>Kum har system for utskilling av olje.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Oljeutskilling">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Ja, uten koalesensfilter</EnumItem>
                <EnumItem>Ja, med koalesensfilter</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stengeventil</Name>
      <Definition>Angir hvorvidt det er stengeventil eller ikke i kum.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Stengeventil">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallRørInn</Name>
      <Definition>Angir hvor mange rør som kommer inn mot kummen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallHjelpesluker</Name>
      <Definition>Angir hvor mange hjelpesluker det er i tilknytning til kummen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kumskjerm</Name>
      <Definition>Angir om det er satt opp en skjerm/skjold ved kummen inn mot skråningen for å hindre at det glir masser fra skråning ned i kummen eller blokkerer kumlokket. Det kan også angis hvilket materiale denne skjermen er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Kumskjerm">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Ja, plast</EnumItem>
                <EnumItem>Ja, betong</EnumItem>
                <EnumItem>Ja, metall, stål</EnumItem>
                <EnumItem>Ja, tre</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>oppslammingstid</Name>
      <Definition>Angir hvor lang tid det normalt tar før en kum blir full av slam.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Oppslammingstid">
                <EnumItem>&lt; 1år</EnumItem>
                <EnumItem>1-2 år</EnumItem>
                <EnumItem>&gt; 2år</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>inngårIDrenssystem</Name>
      <Definition>Angir om kum inngår i drenssystemet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_InngårIDrenssystem">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>valideringsdato_Test</Name>
      <Definition>Dato vegobjektet sist ble validert/påvist. (Til testformål).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>valideringskode_Test</Name>
      <Definition>Kode som angir hvilke type validering som er gjort. Foreløpig til testformål.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsmerking</Name>
      <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produsent</Name>
      <Definition>Angir produktnavn/typebetegnelse, kan også ta med typenummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produktnavn</Name>
      <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>leverandør</Name>
      <Definition>Angir navn på firma som har levert vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivnummer</Name>
      <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>FKB_ID</Name>
      <Definition>Refererer til FKB-identitet. Benyttes i forbindelse med felles forvaltning av geometri.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier_Navn</Name>
      <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Kum_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
