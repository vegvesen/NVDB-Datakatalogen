<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Lysarmatur</Name>
  <Definition>Lyskilde med innfatning.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>lyskildeType</Name>
      <Definition>Angir hvilken type lyskildene er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_LyskildeType">
                <EnumItem>Kvikksølv</EnumItem>
                <EnumItem>Natrium, høytrykk</EnumItem>
                <EnumItem>Natrium, lavtrykk</EnumItem>
                <EnumItem>Lysstoffrør</EnumItem>
                <EnumItem>Glødelampe</EnumItem>
                <EnumItem>QL</EnumItem>
                <EnumItem>LED</EnumItem>
                <EnumItem>Metallhalogen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>effekt</Name>
      <Definition>Angir samlet effekt pr lysarmatur. For damplamper angis lyskildeeffekten og for LED oppgis systemeffekten.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>armaturtype</Name>
      <Definition>Angir hvilken type armatur det er. Avledes av Belysningspunkt.Bruksområde.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Armaturtype">
                <EnumItem>Veglys</EnumItem>
                <EnumItem>Tunnellys</EnumItem>
                <EnumItem>Parklys</EnumItem>
                <EnumItem>Lyskaster</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>høydeOverVeg</Name>
      <Definition>Angir lysarmaturens høyde over veg.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antallLyskilderPerArmatur</Name>
      <Definition>Angir hvor mange lyskilder det er per lysarmatur.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_AntallLyskilderPerArmatur">
                <EnumItem>1</EnumItem>
                <EnumItem>2</EnumItem>
                <EnumItem>3</EnumItem>
                <EnumItem>4</EnumItem>
                <EnumItem>5</EnumItem>
                <EnumItem>6</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>inngårINødsystemet</Name>
      <Definition>Angir om lysarmatur er en del av nødsystemet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_InngårINødsystemet">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>optikk</Name>
      <Definition>Angir optikk i lysarmatur.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Optikk">
                <EnumItem>Reflektor</EnumItem>
                <EnumItem>Linse</EnumItem>
                <EnumItem>Reflektor og linse</EnumItem>
                <EnumItem>Direkte lys</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>konstantLysutbytte</Name>
      <Definition>Angir at armaturen har korreksjon slik at lysutbytte er konstant gjennom hele levetiden (CLO - Constant Light Output).</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_KonstantLysutbytte">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilt</Name>
      <Definition>Angir hvor mye armatur er vippet opp i forhold til horisontalplanet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Tilt">
                <EnumItem>Horisontalt montert</EnumItem>
                <EnumItem>0-10 grader oppvipp</EnumItem>
                <EnumItem>Over 10 grader oppvipp</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dimming</Name>
      <Definition>Angir om det er mulighet for regulering (dimming) av lyset. Ikke aktuelt for lys ved gangfelt etc.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Dimming">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utstyrsklasse</Name>
      <Definition>Beskriver hvordan el-sikkerhet (Jording og isolering) er ivaretatt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Utstyrsklasse">
                <EnumItem>Ujorda</EnumItem>
                <EnumItem>Jorda</EnumItem>
                <EnumItem>Dobbeltisolert</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsstrøm_Udimmet</Name>
      <Definition>Angir driftsstrøm for lyskilden.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lysflux</Name>
      <Definition>Total lysmengde fra lysarmaturen, definert som lysmengde målt mot øyets følsomhetskurve.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>LED_AntallDioder</Name>
      <Definition>Angir antall dioder per lampearmatur.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>LED_EffektPerStk</Name>
      <Definition>Ved 25 grader C, 700 mA.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avskjerming_Type</Name>
      <Definition>Angir hvilken type avskjerming det er på armaturen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Avskjerming_Type">
                <EnumItem>Skjerm, plan</EnumItem>
                <EnumItem>Skjerm, buet</EnumItem>
                <EnumItem>Skjerm, halvbuet</EnumItem>
                <EnumItem>Skjerm, dyptrukket</EnumItem>
                <EnumItem>Åpen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avskjerming_Materiale</Name>
      <Definition>Angir hvordan glasset på armaturen er utformet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Avskjerming_Materiale">
                <EnumItem>Plant</EnumItem>
                <EnumItem>Buet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fargetemperatur</Name>
      <Definition>Angir fargetemperatur.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fargegjengivelsesindeks</Name>
      <Definition>Angir Fargegjengivelsesindeks, (Ra-indeks) for lysarmaturen (0-100).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utskiftbareLyskilder</Name>
      <Definition>Angir om selve lyskilden(e) kan skiftes ut.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_UtskiftbareLyskilder">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lyskildeSistSkiftet</Name>
      <Definition>Angir tidspunkt for når lyskilde sist ble skiftet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>armaturnummer</Name>
      <Definition>Entydig nummer innenfor gitt anlegg/område.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kommunikasjonsenhet</Name>
      <Definition>Navn/adresse på kommunikasjonsenhet som lysarmatur er knyttet til. Kan være VTP (Veglystennpunkt eller konsentrator/segmentkontroller.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsmerking</Name>
      <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produsent_Armatur</Name>
      <Definition>Angir navn på produsent/fabrikant av armatur.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Produsent_Armatur">
                <EnumItem>Aeg</EnumItem>
                <EnumItem>Thorn</EnumItem>
                <EnumItem>Philips</EnumItem>
                <EnumItem>Siemens</EnumItem>
                <EnumItem>Siteco</EnumItem>
                <EnumItem>Ifa</EnumItem>
                <EnumItem>Noral</EnumItem>
                <EnumItem>Seelux</EnumItem>
                <EnumItem>Jernkonst</EnumItem>
                <EnumItem>Urbis</EnumItem>
                <EnumItem>AEC Illuminazione</EnumItem>
                <EnumItem>Defa</EnumItem>
                <EnumItem>Multilux</EnumItem>
                <EnumItem>Heper</EnumItem>
                <EnumItem>Osram</EnumItem>
                <EnumItem>Fagerhult</EnumItem>
                <EnumItem>Euroskilt</EnumItem>
                <EnumItem>Schreder</EnumItem>
                <EnumItem>Bega</EnumItem>
                <EnumItem>Solar Light</EnumItem>
                <EnumItem>Meyer</EnumItem>
                <EnumItem>Tight</EnumItem>
                <EnumItem>Targetti</EnumItem>
                <EnumItem>Multilux</EnumItem>
                <EnumItem>Glamox</EnumItem>
                <EnumItem>Strill</EnumItem>
                <EnumItem>Lyskomponenter</EnumItem>
                <EnumItem>EnergyOptimal</EnumItem>
                <EnumItem>Louis Poulsen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produsent_Driver</Name>
      <Definition>Angir navn på produsent/fabrikant av driver.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produsent_Lyskilde</Name>
      <Definition>Angir navn på produsent/fabrikant av lyskilde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produktnavn_Armatur</Name>
      <Definition>Angir produktnavn for lysarmaturen. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>leverandør</Name>
      <Definition>Angir navn på firma som har levert vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier_Navn</Name>
      <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Lysarmatur_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
