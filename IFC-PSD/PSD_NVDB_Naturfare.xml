<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_NATURFARE</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>typeNaturfare</Name>
         <Definition>Angir hvilken type naturfare (problem) det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_TypeNaturfare">
                  <EnumItem>Bølger</EnumItem>
                  <EnumItem>Flom</EnumItem>
                  <EnumItem>Isgang</EnumItem>
                  <EnumItem>Sandflukt</EnumItem>
                  <EnumItem>Snøfokk</EnumItem>
                  <EnumItem>Stormflo</EnumItem>
                  <EnumItem>Vind</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>navn</Name>
         <Definition>Angir navn på strekningen der naturfaren inntrer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjentakelsesintervall</Name>
         <Definition>Angir hvor ofte denne type naturfare (problem) statistisk sett oppstår på strekningen. Det defineres som at problemet har inntruffet, ikke bare om vegen blir stengt, men også om det er behov for økt beredskap og enkelte restriksjoner som f.eks. nedsatt hastighet, sperring av ett kjørefelt m.m. Gjentaksintervall er styrt av hvor stor del av året problemet er tilstede. Det kreves derfor færre hendelser av vinterproblemet snøfokk enn helårsproblemet bølger for å gi et høyt gjentaksintervall.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Gjentakelsesintervall">
                  <EnumItem>1-2 ganger årlig</EnumItem>
                  <EnumItem>Flere ganger per måned</EnumItem>
                  <EnumItem>Flere ganger per år</EnumItem>
                  <EnumItem>Hvert 2.-5. år</EnumItem>
                  <EnumItem>Hvert 5.-50. år</EnumItem>
                  <EnumItem>Sjeldnere enn hvert 50. år</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>naturfarevurdering</Name>
         <Definition>Angir om det skal gjennomføres naturfarevurdering på strekningen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Naturfarevurdering">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vindhastighet</Name>
         <Definition>Angir hvor kraftig vind som generelt skal til før problemet oppstår.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Vindhastighet">
                  <EnumItem>Frisk bris</EnumItem>
                  <EnumItem>Full storm</EnumItem>
                  <EnumItem>Liten kuling</EnumItem>
                  <EnumItem>Liten storm</EnumItem>
                  <EnumItem>Orkan</EnumItem>
                  <EnumItem>Sterk kuling</EnumItem>
                  <EnumItem>Sterk storm</EnumItem>
                  <EnumItem>Stiv kuling</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vindretning_Kritisk</Name>
         <Definition>Angir hvilken vindretning som generelt gir utfordringer.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Vindretning_Kritisk">
                  <EnumItem>N</EnumItem>
                  <EnumItem>NV</EnumItem>
                  <EnumItem>NØ</EnumItem>
                  <EnumItem>S</EnumItem>
                  <EnumItem>SV</EnumItem>
                  <EnumItem>SØ</EnumItem>
                  <EnumItem>V</EnumItem>
                  <EnumItem>Ø</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vindretning_Sekundær</Name>
         <Definition>Angir hvilken sekundær vindretning som generelt gir utfordringer.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Vindretning_Sekundær">
                  <EnumItem>Alle</EnumItem>
                  <EnumItem>N</EnumItem>
                  <EnumItem>NV</EnumItem>
                  <EnumItem>NØ</EnumItem>
                  <EnumItem>S</EnumItem>
                  <EnumItem>SV</EnumItem>
                  <EnumItem>SØ</EnumItem>
                  <EnumItem>V</EnumItem>
                  <EnumItem>Ø</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stormflohøyde</Name>
         <Definition>Erfaringstall for stormflohøyde, dvs. vannstand (cm) over middels tidevannshøyde.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>styrtflom</Name>
         <Definition>Angir om flommen er som følge av styrtregn (kraftig regn under 3 timers varighet). Styrtregn forekommer i hovedsak om sommeren.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Styrtflom">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kilde</Name>
         <Definition>Angir hvem som har bidratt med informasjon om problemet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturfare_Kilde">
                  <EnumItem>Andre</EnumItem>
                  <EnumItem>Byggherre</EnumItem>
                  <EnumItem>Entreprenør</EnumItem>
                  <EnumItem>Lokalkjent</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Tilleggsopplysninger som er relevante for naturfareberedskapen på vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
