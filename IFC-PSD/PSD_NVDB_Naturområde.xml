<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_NATUROMRÅDE</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type område det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturområde_Type">
                  <EnumItem>Eng</EnumItem>
                  <EnumItem>Ferskvanns-/våtmarksområder</EnumItem>
                  <EnumItem>Kulturlandskap</EnumItem>
                  <EnumItem>Masseplanter</EnumItem>
                  <EnumItem>Naturmark, impediment</EnumItem>
                  <EnumItem>Skog/tregruppe</EnumItem>
                  <EnumItem>Verdifull naturtype</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>formål</Name>
         <Definition>Angir formål med å skjøtte området.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturområde_Formål">
                  <EnumItem>Bevare naturområde/kulturlandskap</EnumItem>
                  <EnumItem>Hensyn til naboeiendom</EnumItem>
                  <EnumItem>Hindre lokalt glatt veg</EnumItem>
                  <EnumItem>Matområde, vilt</EnumItem>
                  <EnumItem>Sikre utsikt fra vegen, sikkerhet, estetikk.</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>aktuellArbeidsoperasjon</Name>
         <Definition>Angir aktuelt arbeidsoperasjon for området.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturområde_AktuellArbeidsoperasjon">
                  <EnumItem>Busker/kratt fjernes</EnumItem>
                  <EnumItem>Busker/kratt holdes nede</EnumItem>
                  <EnumItem>Eng/gress klippes</EnumItem>
                  <EnumItem>Gress/kratt fjernes</EnumItem>
                  <EnumItem>Krever ikke tiltak, kun hensyn</EnumItem>
                  <EnumItem>Skog fjernes</EnumItem>
                  <EnumItem>Skog tynnes</EnumItem>
                  <EnumItem>Trær beskjæres</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>hyppighetTiltak</Name>
         <Definition>Angir hvor ofte tiltak skal gjennomføres.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturområde_HyppighetTiltak">
                  <EnumItem>Annet hvert år</EnumItem>
                  <EnumItem>En gang i løpet av 5 år</EnumItem>
                  <EnumItem>Hvert år</EnumItem>
                  <EnumItem>Sjeldnere enn 5 år</EnumItem>
                  <EnumItem>To ganger i løpet av 5 år</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir areal av område som skal ryddes.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>avstandFraVeg</Name>
         <Definition>Angir minste avstand mellom veg og vegetasjonsområde.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper, f.eks. hvilken type område dette er.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Naturområde_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
