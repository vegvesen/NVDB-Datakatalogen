<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Nedbøyningsmåleserie</Name>
  <Definition>Serie med nedbøyningsmålinger.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>formål</Name>
      <Definition>Angir formål med nedbøyningsmåling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedbøyningsmåleserie_Formål">
                <EnumItem>Sommer</EnumItem>
                <EnumItem>Teleløsning</EnumItem>
                <EnumItem>Parallellmåling</EnumItem>
                <EnumItem>Referansestrekning</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>måledato</Name>
      <Definition>Dato for måling.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spor</Name>
      <Definition>Angir hvor nedbøyning er foretatt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedbøyningsmåleserie_Spor">
                <EnumItem>Ytre spor</EnumItem>
                <EnumItem>Mellom spor</EnumItem>
                <EnumItem>Indre</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utstyrstype</Name>
      <Definition>Angir hvilken type utstyr som er benyttet ved nedbøyningsmåling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedbøyningsmåleserie_Utstyrstype">
                <EnumItem>Dynaflect</EnumItem>
                <EnumItem>Fallodd</EnumItem>
                <EnumItem>Lacroix</EnumItem>
                <EnumItem>Benkelmann</EnumItem>
                <EnumItem>TSD</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utstyrsid</Name>
      <Definition>Angir Id for utstyrstype.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>operatør</Name>
      <Definition>Angir hvem som er ansvarlig operatør ved dataregistrering.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelse</Name>
      <Definition>Beskrivelse som er aktuell for hele måleserie.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>måledatafilnavn</Name>
      <Definition>Gir filnavn til rådatafil som ligger på o:\landsdekkende..</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
