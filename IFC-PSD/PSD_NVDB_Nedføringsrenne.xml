<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Nedføringsrenne</Name>
  <Definition>Grøft som fører vann fra topp skjæring ned til kum/stikkrenne.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>utforming</Name>
      <Definition>Angir hvilken utforming nedføringsrenne har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedføringsrenne_Utforming">
                <EnumItem>Loddrett</EnumItem>
                <EnumItem>Skrå</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>materiale</Name>
      <Definition>Angir hvilken type materiale vegobjektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedføringsrenne_Materiale">
                <EnumItem>Stein</EnumItem>
                <EnumItem>Betong</EnumItem>
                <EnumItem>Berg</EnumItem>
                <EnumItem>Plast</EnumItem>
                <EnumItem>Stål</EnumItem>
                <EnumItem>Asfalt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lengde</Name>
      <Definition>Angir total lengde av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dybde</Name>
      <Definition>Angir dybde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde_Topp</Name>
      <Definition>Angir gjennomsnittlig bredde (topp) for hele vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bredde_Bunn</Name>
      <Definition>Angir gjennomsnittlig bredde (bunn) for hele vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedføringsrenne_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Nedføringsrenne_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
