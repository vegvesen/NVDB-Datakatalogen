<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Omkjøringsruteinnsats_Punkt</Name>
  <Definition>Punkt i vegnettet der det kreves innsats i forbindelse med klargjøring av ei omkjøringsrute for omkjøringstrafikk.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>innsatstype</Name>
      <Definition>Angir hvilken type innsats som er aktuell på stedet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Omkjøringsruteinnsats_Punkt_Innsatstype">
                <EnumItem>Dirigering, Aktiver manuell</EnumItem>
                <EnumItem>Signalanlegg, Aktiver automatisk</EnumItem>
                <EnumItem>Signalhode, Aktiver styringVTS</EnumItem>
                <EnumItem>Skiltplate, Aktiver manuell (Skilt på løsfot…)</EnumItem>
                <EnumItem>Skiltplate, Dekk til, Aktiver manuell</EnumItem>
                <EnumItem>Skiltplate, Fast omskilting</EnumItem>
                <EnumItem>Variabelt skilt, Aktiver styringVTS</EnumItem>
                <EnumItem>Variabelt skilt, Aktiver manuell</EnumItem>
                <EnumItem>Vegbom, Aktiver styringVTS</EnumItem>
                <EnumItem>Vegbom, Aktiver manuell</EnumItem>
                <EnumItem>Venteplass, Aktiver manuell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skiltnummer_MidlertidigSkilt</Name>
      <Definition>Angir skiltnummer til midlertidig skilt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Omkjøringsruteinnsats_Punkt_Skiltnummer_MidlertidigSkilt">
                <EnumItem>713.V - Vanlig vegviser, Venstre</EnumItem>
                <EnumItem>713.H - Vanlig vegviser, Høyre </EnumItem>
                <EnumItem>723.41 - Omkjøringsrute for store kjøretøy</EnumItem>
                <EnumItem>741 - Omkjøring for bestemte kjøretøygrupper </EnumItem>
                <EnumItem>743.1 - Midlertidig omkjøring. Uten tekst</EnumItem>
                <EnumItem>743.2 - Midlertidig omkjøring. Med tekst</EnumItem>
                <EnumItem>Annet skiltnummer</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skilttekst_MidlertidigSkilt</Name>
      <Definition>Angir tekst på midlertidig skilt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>sesong</Name>
      <Definition>Angir hvilken sesong innsats er relevant.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Omkjøringsruteinnsats_Punkt_Sesong">
                <EnumItem>Vinter</EnumItem>
                <EnumItem>Sommer</EnumItem>
                <EnumItem>Uavhengig av sesong</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>referanseTilEksisterendeVegobjekt</Name>
      <Definition>Gir referanse til eksisterende vegobjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Angir tilleggsinformasjon.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
