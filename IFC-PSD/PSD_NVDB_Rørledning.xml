<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_RØRLEDNING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>bruksområde</Name>
         <Definition>Angir hva vegobjektet brukes til, hvilken funksjon det har.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Bruksområde">
                  <EnumItem>Fjernkjøling</EnumItem>
                  <EnumItem>Fjernvarme</EnumItem>
                  <EnumItem>Gassledning</EnumItem>
                  <EnumItem>Kulvert</EnumItem>
                  <EnumItem>Oljeledning</EnumItem>
                  <EnumItem>Vann</EnumItem>
                  <EnumItem>Vann, avløp, felles</EnumItem>
                  <EnumItem>Vann, drenering</EnumItem>
                  <EnumItem>Vann, hovedledning</EnumItem>
                  <EnumItem>Vann, infiltrering i grunnen</EnumItem>
                  <EnumItem>Vann, overvann</EnumItem>
                  <EnumItem>Vann, pumpeledning</EnumItem>
                  <EnumItem>Vann, spillvann</EnumItem>
                  <EnumItem>Vann, stikkledning</EnumItem>
                  <EnumItem>Ventilasjon</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>materialtype</Name>
         <Definition>Angir primær materialtype for vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Materialtype">
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>Glassfiber</EnumItem>
                  <EnumItem>Metall, stål</EnumItem>
                  <EnumItem>Metall, stål, duktilt støpjern</EnumItem>
                  <EnumItem>Metall, stål, galvanisert</EnumItem>
                  <EnumItem>Metall, stål, korrugert</EnumItem>
                  <EnumItem>Naturstein/fjell, murstein</EnumItem>
                  <EnumItem>Plast</EnumItem>
                  <EnumItem>Plast, abs-rør</EnumItem>
                  <EnumItem>Plast, DV</EnumItem>
                  <EnumItem>Plast, npf8001</EnumItem>
                  <EnumItem>Plast, pe</EnumItem>
                  <EnumItem>Plast, pel</EnumItem>
                  <EnumItem>Plast, pp</EnumItem>
                  <EnumItem>Plast, pvc-rør</EnumItem>
                  <EnumItem>Teglstein, murstein</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tverrsnittform</Name>
         <Definition>Angir hvilken form tverrsnittet av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Tverrsnittform">
                  <EnumItem>Ellipse</EnumItem>
                  <EnumItem>Firkanta</EnumItem>
                  <EnumItem>Sirkulært</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>diameter</Name>
         <Definition>Angir diameter av rørledning. Det angis ytre eller indre diameter ut fra hva som er standard for aktuell rørtype.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tykkelse</Name>
         <Definition>Angir standard tykkelse av godset i rørledningen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lengde</Name>
         <Definition>Angir lengde av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>elementlengde</Name>
         <Definition>Angir normal lengde av enkeltelement. Lengde av enkeltrør.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>helningFall</Name>
         <Definition>Angir fall på rørledning i lengderetning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>harInnløpsrist</Name>
         <Definition>Angir om det er innløpsrist i tilknytning til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_HarInnløpsrist">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>farge</Name>
         <Definition>Angir hovedfarge til vegobjektet. Mest aktuelt for plastrør.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skjøtemetode</Name>
         <Definition>Angir hvordan rørene er sammenkoplet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Skjøtemetode">
                  <EnumItem>Butt i butt</EnumItem>
                  <EnumItem>Fals</EnumItem>
                  <EnumItem>Flens</EnumItem>
                  <EnumItem>Helstøpt</EnumItem>
                  <EnumItem>Krympeplast</EnumItem>
                  <EnumItem>Muffe med pakning</EnumItem>
                  <EnumItem>Muffe uten pakning</EnumItem>
                  <EnumItem>Sveist</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>rehabilitering</Name>
         <Definition>Angir om vegobjektet har vært rehabilitert og hvilken form for rehabilitering.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Rehabilitering">
                  <EnumItem>Delvis utskifting</EnumItem>
                  <EnumItem>Epoxy</EnumItem>
                  <EnumItem>Oppblåsbar indre plaststrømpe</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produsent</Name>
         <Definition>Angir navn på produsent/fabrikant av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavn</Name>
         <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivnummer</Name>
         <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier_Navn</Name>
         <Definition>Navn på eier av vegobjektet. Merknad: Det skal angis organisasjonsnavn, firmanavn eller gårds- og bruksnummer, ikke personnavn.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Rørledning_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
