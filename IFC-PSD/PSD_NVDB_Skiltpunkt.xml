<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_SKILTPUNKT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>oppsettingsutstyr</Name>
         <Definition>Angir hvilket oppsettingsutstyr skiltplatene er festet på.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Oppsettingsutstyr">
                  <EnumItem>1-rør på bunnplate</EnumItem>
                  <EnumItem>2-rør på bunnplate</EnumItem>
                  <EnumItem>3-rør på bunnplate</EnumItem>
                  <EnumItem>Annen vegeiers skiltpunkt</EnumItem>
                  <EnumItem>Bakkemontert bøyle</EnumItem>
                  <EnumItem>Gittermast</EnumItem>
                  <EnumItem>Gittermast, firkant 200mm</EnumItem>
                  <EnumItem>Gittermast, firkant 250mm</EnumItem>
                  <EnumItem>Gittermast, firkant 380mm</EnumItem>
                  <EnumItem>Gittermast, trekant 200mm</EnumItem>
                  <EnumItem>Gittermast, trekant 250mm</EnumItem>
                  <EnumItem>Halvportal</EnumItem>
                  <EnumItem>Helportal</EnumItem>
                  <EnumItem>Henger i tak/kabelbro</EnumItem>
                  <EnumItem>Kontaktledningsmast</EnumItem>
                  <EnumItem>Lysmast</EnumItem>
                  <EnumItem>Rekkverk/støtpute</EnumItem>
                  <EnumItem>Signalstolpe</EnumItem>
                  <EnumItem>Skiltbjelke/gitterdrager i tak</EnumItem>
                  <EnumItem>Stolpe 60,3 mm (2")</EnumItem>
                  <EnumItem>Stolpe 88,9 mm (3")</EnumItem>
                  <EnumItem>Søyle, liten</EnumItem>
                  <EnumItem>Søyle, liten, rund 110 mm</EnumItem>
                  <EnumItem>Søyle, liten, åttekantet 120mm</EnumItem>
                  <EnumItem>Søyle, middels</EnumItem>
                  <EnumItem>Søyle, middels, åttekantet 150 mm</EnumItem>
                  <EnumItem>Søyle, stor</EnumItem>
                  <EnumItem>Søyle, stor, rund 180 mm</EnumItem>
                  <EnumItem>Søyle, stor, åttekantet 190 mm</EnumItem>
                  <EnumItem>Trestolpe</EnumItem>
                  <EnumItem>Vegbom</EnumItem>
                  <EnumItem>Vegg</EnumItem>
                  <EnumItem>Wirestrekk</EnumItem>
                  <EnumItem>Søyle, firkantet</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavn_Skiltmast</Name>
         <Definition>Angir produktnavn for ettergivende skiltmast. Verdier er hentet fra oversikt over godkjente produkttyper på vegvesen.no.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Produktnavn_Skiltmast">
                  <EnumItem>HIMAST H1000</EnumItem>
                  <EnumItem>HIMAST H2000</EnumItem>
                  <EnumItem>HIMAST H3000</EnumItem>
                  <EnumItem>HIMAST H500</EnumItem>
                  <EnumItem>Jerol 114</EnumItem>
                  <EnumItem>Jerol 168</EnumItem>
                  <EnumItem>Jerol 219</EnumItem>
                  <EnumItem>Jerol 273</EnumItem>
                  <EnumItem>Lattix 4420</EnumItem>
                  <EnumItem>Lattix 4425</EnumItem>
                  <EnumItem>Lattix 4438</EnumItem>
                  <EnumItem>Lattix D4412</EnumItem>
                  <EnumItem>Norsafe NM 120</EnumItem>
                  <EnumItem>Norsafe NM 150</EnumItem>
                  <EnumItem>Norsafe NM 190C</EnumItem>
                  <EnumItem>Norsafe NM 190D</EnumItem>
                  <EnumItem>Norsafe NM 290</EnumItem>
                  <EnumItem>Norsafe NM 89</EnumItem>
                  <EnumItem>Opplandsmast toløper</EnumItem>
                  <EnumItem>Opplandsmast treløper</EnumItem>
                  <EnumItem>SafeHit1</EnumItem>
                  <EnumItem>SafeHit2</EnumItem>
                  <EnumItem>SafeHit3</EnumItem>
                  <EnumItem>SafeHit4</EnumItem>
                  <EnumItem>T&amp;V</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallOppsettingsutstyr</Name>
         <Definition>Angir antall oppsettingsutstyr det er i forbindelse med vegobjektet. Antall skiltstolper/søyler knyttet til skiltpunktet. Eventuelle støttestolper skal ikke medregnes.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallStøttestolper</Name>
         <Definition>Angir hvor mange støttestolper det er i tilknytning til skiltpunktet. Støttestolpene er skråstilte og benyttes for å avstive skiltpunkt i forhold til bl.a. vindlast.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høyde_MastStolpe</Name>
         <Definition>Gir høyde (lengde) av selve masten/stolpen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høydeOverVegkant</Name>
         <Definition>Angir høyde fra vegkant til underkant nederste skiltplate som ikke er underskilt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>avstandFraDekkekant</Name>
         <Definition>Angir hvilken avstand det er fra dekkekant til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>avskjæringsledd</Name>
         <Definition>Angir om det er avskjæringsledd på skiltpunktets oppsettingsutstyr. Med avskjæringsledd vil stolpe/mast knekke eller bøyes i dette leddet ved påkjørsel.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Avskjæringsledd">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>brakett</Name>
         <Definition>Angir om det er benyttet brakett for festing av skiltet. Brakett benyttes gjerne for skiltpunkt som er festet på vegg, bru, m.m.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Brakett">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bøyle</Name>
         <Definition>Angir om skiltplaten(e) er festet inn i en metallbøyle (som igjen er festet på oppsettingsutstyret).</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Bøyle">
                  <EnumItem>Forgrening</EnumItem>
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Ja, midtmontert</EnumItem>
                  <EnumItem>Ja, sidemontert</EnumItem>
                  <EnumItem>Ja, toppmontert</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skiltholder</Name>
         <Definition>Angir om skiltpunktet har skiltholder.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Skiltholder">
                  <EnumItem>Ja, buet</EnumItem>
                  <EnumItem>Ja, rett</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>RAL_Kode</Name>
         <Definition>Angir lakkfargekode for stolpe/mast. Bare aktuelt for lakkerte stolper/master.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fundamentering</Name>
         <Definition>Angir hvordan vegobjektet er fundamentert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Fundamentering">
                  <EnumItem>I/på fjellfundament</EnumItem>
                  <EnumItem>I/på jordfundament</EnumItem>
                  <EnumItem>Ingen</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fundamentdetaljer</Name>
         <Definition>Detaljinformasjon om skiltfundament. Kan være informasjon om produsent, produktnavn, typebetegnelse, størrelse, m.m.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallFundament</Name>
         <Definition>Angir hvor mange fundament det er i tilknytning til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kampanje_Egnet</Name>
         <Definition>Angir type kampanje(r) som skiltpunktet er tilrettelagt for.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Kampanje_Egnet">
                  <EnumItem>Bilbelte</EnumItem>
                  <EnumItem>Bilbelte/Fart</EnumItem>
                  <EnumItem>Del vegen</EnumItem>
                  <EnumItem>Fart</EnumItem>
                  <EnumItem>Stopp og sov (historisk)</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kampanje_Pågående</Name>
         <Definition>Angir type kampanje som pågår i periode angitt under ET 10894 "Kampanjeperiode".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Kampanje_Pågående">
                  <EnumItem>Bilbelte</EnumItem>
                  <EnumItem>Del vegen</EnumItem>
                  <EnumItem>Fart</EnumItem>
                  <EnumItem>Ingen</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kampanjeperiode</Name>
         <Definition>Angir hvilken periode gjeldende kampanje gjelder for.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bebyggelse</Name>
         <Definition>Angir hvilken type bebyggelse vegobjektet er omgitt av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Bebyggelse">
                  <EnumItem>By</EnumItem>
                  <EnumItem>Landlig</EnumItem>
                  <EnumItem>Tettbebygd</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>klima</Name>
         <Definition>Angir hvilken type klima det er der vegobjektet er plassert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Klima">
                  <EnumItem>Innland</EnumItem>
                  <EnumItem>Innland, 300 til 600</EnumItem>
                  <EnumItem>Innland, &lt; 300</EnumItem>
                  <EnumItem>Innland, &gt; 600</EnumItem>
                  <EnumItem>Kystklima</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>miljø</Name>
         <Definition>Angir om vegobjektet er lokalisert til et område med kjemisk forurensning.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Miljø">
                  <EnumItem>Ikke kjemisk</EnumItem>
                  <EnumItem>Kjemisk forurensning</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skiltpunkt_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
