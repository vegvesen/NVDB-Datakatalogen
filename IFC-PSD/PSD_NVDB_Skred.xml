<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Skred</Name>
  <Definition>Masser som løsner</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>skredDato</Name>
      <Definition>Angir dato da skredet gikk.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skredKlokkeslett</Name>
      <Definition>Angir klokkeslett da skredet gikk.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>typeSkred</Name>
      <Definition>Angir hvilken type skred det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_TypeSkred">
                <EnumItem>Stein</EnumItem>
                <EnumItem>Jord/løsmasse</EnumItem>
                <EnumItem>Snø</EnumItem>
                <EnumItem>Is</EnumItem>
                <EnumItem>Is/stein</EnumItem>
                <EnumItem>Flomskred (vann+stein+jord)</EnumItem>
                <EnumItem>Sørpeskred (vann+snø+stein)</EnumItem>
                <EnumItem>Utglidning av veg</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>løsneområde</Name>
      <Definition>Angir løsneområde for skredet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_Løsneområde">
                <EnumItem>Tunnelmunning (historisk)</EnumItem>
                <EnumItem>Vegskjæring</EnumItem>
                <EnumItem>Ur</EnumItem>
                <EnumItem>Fjell/dalside</EnumItem>
                <EnumItem>Inne i tunnel</EnumItem>
                <EnumItem>Vegbane/-fylling</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stedsangivelse</Name>
      <Definition>Stedsnavn hvor forekomst er lokalisert. Evt plassering i forhold til stedsnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>volumAvSkredmasserPåVeg</Name>
      <Definition>Angir volum av skredmasser på vegen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VolumAvSkredmasserPåVeg">
                <EnumItem>&lt; 1 kbm</EnumItem>
                <EnumItem>&lt;10 kbm</EnumItem>
                <EnumItem>&lt; 100 kbm</EnumItem>
                <EnumItem>&lt; 1000 kbm</EnumItem>
                <EnumItem>&gt; 1000 kbm</EnumItem>
                <EnumItem>Ikke vurdert</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>volumAvTotaleSkredmasser</Name>
      <Definition>Anslag over hvor stort det totale skredet har vært.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>blokkertVeglengde</Name>
      <Definition>Angir lengde av veg som var blokkert av skredet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_BlokkertVeglengde">
                <EnumItem>Kun i grøft</EnumItem>
                <EnumItem>&lt; 10 m</EnumItem>
                <EnumItem>10 - 50 m</EnumItem>
                <EnumItem>50 - 100 m</EnumItem>
                <EnumItem>&gt; 100 m</EnumItem>
                <EnumItem>Stoppet i sikringstiltak (ny)</EnumItem>
                <EnumItem>Passerte veg uten å legge igjen skredmasser (ny)</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>blokkertGangSykkelveg</Name>
      <Definition>Angir om gang/sykkelveg er blitt blokkert av skredet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_BlokkertGangSykkelveg">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>høydeforskjellVegUtløsningssted</Name>
      <Definition>Angir høydeforskjell mellom skredets utløsningssted og vegen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_HøydeforskjellVegUtløsningssted">
                <EnumItem>0 - 50 m (historisk)</EnumItem>
                <EnumItem>0 - 5 m (ny)</EnumItem>
                <EnumItem>5 - 20 m (ny)</EnumItem>
                <EnumItem>20 - 50 m (ny)</EnumItem>
                <EnumItem>50 - 200 m</EnumItem>
                <EnumItem>&gt; 200 m</EnumItem>
                <EnumItem>Nedenfor veg</EnumItem>
                <EnumItem>Ikke vurdert</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>grøftebreddePåSkredside</Name>
      <Definition>Angir grøftebredde på skredside.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vesentligeSkader</Name>
      <Definition>Angir om skredet totalt sett har gitt vesentlige skader eller ikke.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VesentligeSkader">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåPerson</Name>
      <Definition>Angir om skredet har medført skade på person.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåPerson">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåKjøretøy</Name>
      <Definition>Angir om skredet har medført skade på kjøretøy.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåKjøretøy">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåVegdekkeVegkropp</Name>
      <Definition>Angir om skredet har medført skade på vegdekke og/eller vegkropp.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåVegdekkeVegkropp">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåBru</Name>
      <Definition>Angir om skredet har medført skade på bru.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåBru">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåRekkverk</Name>
      <Definition>Angir om skredet har medført skade på rekkverk.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåRekkverk">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadePåDrenering</Name>
      <Definition>Angir om skredet har medført skade på drenering.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_SkadePåDrenering">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>antattHovedårsak</Name>
      <Definition>Angir hva som er antatt hovedårsak til skredet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_AntattHovedårsak">
                <EnumItem>Fryse- og tineprosesser</EnumItem>
                <EnumItem>Rotsprengning</EnumItem>
                <EnumItem>Langvarig forvitring/kryp</EnumItem>
                <EnumItem>Menneskelig aktivitet</EnumItem>
                <EnumItem>Undergraving av skråningsfot</EnumItem>
                <EnumItem>Ytre påvirkning fra steinsprang/steinskred</EnumItem>
                <EnumItem>Dreneringsforhold</EnumItem>
                <EnumItem>Ukontrollert erosjon i elv/bekk</EnumItem>
                <EnumItem>Stort snøfall</EnumItem>
                <EnumItem>Vind og snøfokk</EnumItem>
                <EnumItem>Skavlfall</EnumItem>
                <EnumItem>Rask temperaturstigning</EnumItem>
                <EnumItem>Soloppvarming</EnumItem>
                <EnumItem>Vedvarende svake lag</EnumItem>
                <EnumItem>Glidning mot bakken</EnumItem>
                <EnumItem>Mildvær</EnumItem>
                <EnumItem>Regn og smeltevann</EnumItem>
                <EnumItem>Ukjent</EnumItem>
                <EnumItem>Kontrollert utløst</EnumItem>
                <EnumItem>Annet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stengning</Name>
      <Definition>Gir informasjon om eventuell stengning av vegen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_Stengning">
                <EnumItem>Ingen stengning</EnumItem>
                <EnumItem>Stengning av del av vegbane</EnumItem>
                <EnumItem>Stengt for tungtrafikk</EnumItem>
                <EnumItem>Stengt for personbiler</EnumItem>
                <EnumItem>Stengt for all trafikk</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>værforholdPåVegen</Name>
      <Definition>Angir hvilke værforhold det var på vegen da skredet gikk.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VærforholdPåVegen">
                <EnumItem>Ingen nedbør</EnumItem>
                <EnumItem>Regn</EnumItem>
                <EnumItem>Regn, lite</EnumItem>
                <EnumItem>Regn, middels</EnumItem>
                <EnumItem>Regn, mye</EnumItem>
                <EnumItem>Snø</EnumItem>
                <EnumItem>Snø, lite</EnumItem>
                <EnumItem>Snø, middels</EnumItem>
                <EnumItem>Snø, mye</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vindretning</Name>
      <Definition>Angir hvilken vindretning det var på vegen da skredet gikk.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_Vindretning">
                <EnumItem>S</EnumItem>
                <EnumItem>SSV</EnumItem>
                <EnumItem>SV</EnumItem>
                <EnumItem>VSV</EnumItem>
                <EnumItem>V</EnumItem>
                <EnumItem>VNV</EnumItem>
                <EnumItem>NV</EnumItem>
                <EnumItem>NNV</EnumItem>
                <EnumItem>N</EnumItem>
                <EnumItem>NNØ</EnumItem>
                <EnumItem>NØ</EnumItem>
                <EnumItem>ØNØ</EnumItem>
                <EnumItem>Ø</EnumItem>
                <EnumItem>ØSØ</EnumItem>
                <EnumItem>SØ</EnumItem>
                <EnumItem>SSØ</EnumItem>
                <EnumItem>Vindstille</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>temperaturPåVeg</Name>
      <Definition>Angir lufttemperatur på/ved vegen der skredet har inntruffet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bilder</Name>
      <Definition>Angir om det finnes bilder av skredet eller ikke.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_Bilder">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>registreringsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for registrert informasjon om skredet.  Det skal angis organisasjon/organisasjonsenhet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
