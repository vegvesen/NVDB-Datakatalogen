<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Skred_VarslingOvervåkning</Name>
  <Definition>Område som blir overvåket med hensyn på skred.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>typeNaturfare</Name>
      <Definition>Angir hvilken type naturfare overvåkning/varsling gjelder for.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VarslingOvervåkning_TypeNaturfare">
                <EnumItem>Snøskred</EnumItem>
                <EnumItem>Steinsprang/skred</EnumItem>
                <EnumItem>Jordskred</EnumItem>
                <EnumItem>Flom</EnumItem>
                <EnumItem>Bølger</EnumItem>
                <EnumItem>Flere skredtyper</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>overvåkningstype</Name>
      <Definition>Angir type overvåkning.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VarslingOvervåkning_Overvåkningstype">
                <EnumItem>Geofon</EnumItem>
                <EnumItem>Radar</EnumItem>
                <EnumItem>Rystelsesmåling</EnumItem>
                <EnumItem>Varslingsgjerde</EnumItem>
                <EnumItem>Infralyd</EnumItem>
                <EnumItem>Annen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>varslingPåVeg</Name>
      <Definition>Angir type varsling som er satt opp synlig for trafikanter.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VarslingOvervåkning_VarslingPåVeg">
                <EnumItem>Lys</EnumItem>
                <EnumItem>Bommer</EnumItem>
                <EnumItem>Ingen</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VarslingOvervåkning_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Skred_VarslingOvervåkning_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
