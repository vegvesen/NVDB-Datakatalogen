<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_SKREDTEKNISKOBJEKT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Beskriver hvilken type utstyr det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredteknisk objekt_Type">
                  <EnumItem>Annet utstyr</EnumItem>
                  <EnumItem>Gassdetonator</EnumItem>
                  <EnumItem>Geofon</EnumItem>
                  <EnumItem>Infralydsensor</EnumItem>
                  <EnumItem>Mast</EnumItem>
                  <EnumItem>Radar</EnumItem>
                  <EnumItem>Sprengningstårn</EnumItem>
                  <EnumItem>Styreskap</EnumItem>
                  <EnumItem>Teknisk bygg</EnumItem>
                  <EnumItem>Målebolt</EnumItem>
                  <EnumItem>Prisme</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fundamentering</Name>
         <Definition>Beskriver hvordan utstyret er fundamentert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredteknisk objekt_Fundamentering">
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>I fjell</EnumItem>
                  <EnumItem>I løsmasser</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høyde</Name>
         <Definition>Angir utstyrets egenhøyde fra topp fundament til topp utstyr.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>adkomst</Name>
         <Definition>Beskriver hvordan man får adkomst til vegobjektet for vedlikehold.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredteknisk objekt_Adkomst">
                  <EnumItem>Bil</EnumItem>
                  <EnumItem>Gravemaskin</EnumItem>
                  <EnumItem>Helikopter</EnumItem>
                  <EnumItem>Hjullaster</EnumItem>
                  <EnumItem>Kran</EnumItem>
                  <EnumItem>Tau</EnumItem>
                  <EnumItem>Til fots</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>beskrivelse</Name>
         <Definition>Beskrivelse av utstyret.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produsent</Name>
         <Definition>Angir navn på produsent/fabrikant av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredteknisk objekt_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredteknisk objekt_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
