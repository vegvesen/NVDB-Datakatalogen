<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_SKREDUTSATTVEG</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>navn</Name>
         <Definition>Gjenkjennbart navn på strekningen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skredtype1</Name>
         <Definition>Angir vanligste skredtype som gir skred på veg.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredutsatt veg_Skredtype1">
                  <EnumItem>Flomskred</EnumItem>
                  <EnumItem>Is/steinnedfall</EnumItem>
                  <EnumItem>Isnedfall</EnumItem>
                  <EnumItem>Jord/løsmasseskred</EnumItem>
                  <EnumItem>Snøskred</EnumItem>
                  <EnumItem>Steinsprang</EnumItem>
                  <EnumItem>Sørpeskred</EnumItem>
                  <EnumItem>Utglidning av veg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skredtype2</Name>
         <Definition>Angir nest vanligste skredtype som gir skred på veg.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredutsatt veg_Skredtype2">
                  <EnumItem>Flomskred</EnumItem>
                  <EnumItem>Is/steinnedfall</EnumItem>
                  <EnumItem>Isnedfall</EnumItem>
                  <EnumItem>Jord/løsmasseskred</EnumItem>
                  <EnumItem>Snøskred</EnumItem>
                  <EnumItem>Steinsprang</EnumItem>
                  <EnumItem>Sørpeskred</EnumItem>
                  <EnumItem>Utglidning av veg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skredtype3</Name>
         <Definition>Angir tredje vanligste skredtype som gir skred på veg.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredutsatt veg_Skredtype3">
                  <EnumItem>Flere enn tre skredtyper</EnumItem>
                  <EnumItem>Flomskred</EnumItem>
                  <EnumItem>Is/steinnedfall</EnumItem>
                  <EnumItem>Isnedfall</EnumItem>
                  <EnumItem>Jord/løsmasseskred</EnumItem>
                  <EnumItem>Snøskred</EnumItem>
                  <EnumItem>Steinsprang</EnumItem>
                  <EnumItem>Sørpeskred</EnumItem>
                  <EnumItem>Utglidning av veg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjentakelsesintervall</Name>
         <Definition>Angir hvor ofte det statistisk sett oppstår skred eller skredfare slik at veg må stenges.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredutsatt veg_Gjentakelsesintervall">
                  <EnumItem>1-2 ganger årlig</EnumItem>
                  <EnumItem>Flere ganger per måned</EnumItem>
                  <EnumItem>Flere ganger per år</EnumItem>
                  <EnumItem>Hver 2.-5. år</EnumItem>
                  <EnumItem>Sjeldnere enn hvert 5. år</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skredfarevurdering</Name>
         <Definition>Angir om det skal gjøres skredfarevurdering på strekningen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Skredutsatt veg_Skredfarevurdering">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
