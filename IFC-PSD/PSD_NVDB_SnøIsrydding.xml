<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_SnøIsrydding</Name>
  <Definition>Sted/område hvor det er behov for å snø-/isrydding utover det som tas med brøytekjøretøy.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type snørydding det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_SnøIsrydding_Type">
                <EnumItem>Snørydding</EnumItem>
                <EnumItem>Snørydding, manglende snøopplager</EnumItem>
                <EnumItem>Snørydding, sikt</EnumItem>
                <EnumItem>Snørydding, trafikkdeler/øy</EnumItem>
                <EnumItem>Snørydding, fortau</EnumItem>
                <EnumItem>Snørydding, leskur</EnumItem>
                <EnumItem>Snørydding, trafikklomme</EnumItem>
                <EnumItem>Snørydding, sideanlegg/naboeiendom</EnumItem>
                <EnumItem>Snørydding, skjæring/mur</EnumItem>
                <EnumItem>Snørydding, nedfresing av brøytekanter</EnumItem>
                <EnumItem>Snørydding, fokksnø</EnumItem>
                <EnumItem>Snørydding, fresefelt</EnumItem>
                <EnumItem>Isrensk i bergskjæring/skråning</EnumItem>
                <EnumItem>Isrensk i tunnel</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelse</Name>
      <Definition>Tilleggsbeskrivelse. Kan f.eks. være bakgrunn for snøryddingsbehov, om snø må kjøres bort, Hvor stort omfang snørydding har osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bortkjøring</Name>
      <Definition>Angir om det er nødvendig å laste snø/is over på annet kjøretøy (lastebil/traktor) og kjøre det bort fra stedet eller ikke.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_SnøIsrydding_Bortkjøring">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>hyppighet</Name>
      <Definition>Angir anslag for hvor ofte det er behov for å foreta rydding per vintersesong.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>makshøydeIs</Name>
      <Definition>Angir hvor høyt over veg det er nødvendig med isrensk.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal</Name>
      <Definition>Angir areal av område som skal snø-/isryddes.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
