<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_STATISTIKK_GENERELL</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>kildeVegobjekttypeID</Name>
         <Definition>Id til den vegobjekttypen som objektet inneholder/skal inneholde begrenset statistisk informasjon for.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kildeEgenskapstypeID</Name>
         <Definition>Id til den egenskapstypen som den statistiske informasjonen gjelder for.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>geografiskOppløsning</Name>
         <Definition>Angir hva statistikken omfatter.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Statistikk, generell_GeografiskOppløsning">
                  <EnumItem>Fylke</EnumItem>
                  <EnumItem>Generert</EnumItem>
                  <EnumItem>HP</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Landet</EnumItem>
                  <EnumItem>PMS-parsell</EnumItem>
                  <EnumItem>Region</EnumItem>
                  <EnumItem>Veg pr avdeling</EnumItem>
                  <EnumItem>Veg pr fylke</EnumItem>
                  <EnumItem>Veg pr kommune</EnumItem>
                  <EnumItem>Veg pr land</EnumItem>
                  <EnumItem>Veg pr region</EnumItem>
                  <EnumItem>Vegavdeling</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stedsangivelse_Fra_Til</Name>
         <Definition>Angir stedsnavn for start og slutt strekning. Rekkefølge som navnene settes i har ikke betydning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fordelingIIntervaller</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>konfidensnivå_50Prosent</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>konfidensnivå_90Prosent</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>middelverdi</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>min_Mm_Verdi</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>maksverdi</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>andelMedData</Name>
         <Definition>Del av veglengden som har informasjon i den egenskapstypen statistikken er beregna ut fra.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fordelingIVerdier</Name>
         <Definition/>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>veglengde</Name>
         <Definition>Målt veglengde som lokasjonsattributten dekker. Ev. lengde av kjørefelt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
