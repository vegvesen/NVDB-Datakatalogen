<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Statistikk_Vegbredde</Name>
  <Definition>Aggregert informasjon ifbm PMS-rapport 2625.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>opprettetDato</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stedsangivelse_Fra_Til</Name>
      <Definition>Angir stedsnavn for start og slutt strekning. Rekkefølge som navnene settes i har ikke betydning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>geografiskOppløsning</Name>
      <Definition>Angir hva statistikken omfatter.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Statistikk_Vegbredde_GeografiskOppløsning">
                <EnumItem>Landet</EnumItem>
                <EnumItem>Region</EnumItem>
                <EnumItem>Fylke</EnumItem>
                <EnumItem>Vegavdeling</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Veg pr land</EnumItem>
                <EnumItem>Veg pr region</EnumItem>
                <EnumItem>Veg pr fylke</EnumItem>
                <EnumItem>Veg pr avdeling</EnumItem>
                <EnumItem>Veg pr kommune</EnumItem>
                <EnumItem>HP</EnumItem>
                <EnumItem>PMS-parsell</EnumItem>
                <EnumItem>1000-meter</EnumItem>
                <EnumItem>Generert</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>min_Mm_Dato_Grunnlag</Name>
      <Definition>Datagrunnlaget for statistikkobjektet er ikke eldre enn gitt dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>maks_Mm_Dato_Grunnlag</Name>
      <Definition>Datagrunnlaget for statistikkobjektet er ikke nyere enn gitt dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>midlereDekkebredde</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>midlereKjørebanebredde</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>midlereVegbredde</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekkeareal</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>måltProsent</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lengde</Name>
      <Definition>Angir total lengde av strekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekkebreddefordeling</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kjørebanebreddefordeling</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vegbreddefordeling</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
