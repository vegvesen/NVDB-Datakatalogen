<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Stedsnavn</Name>
  <Definition>Angir stedsnavn.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>egennavn</Name>
      <Definition>Angir selve stedsnavnet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>navnetype</Name>
      <Definition>Angir hvilken navnetype navnet er av.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>stedsnavnsregister_Id</Name>
      <Definition>Gir referanse til stedsnavnsregister.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nivå</Name>
      <Definition>Angir om det er et navn på overordna-, lokalt- eller detaljert nivå.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Stedsnavn_Nivå">
                <EnumItem>Nivå 7</EnumItem>
                <EnumItem>Nivå 8</EnumItem>
                <EnumItem>Nivå 9</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>opprinnelse</Name>
      <Definition>Angir hvor stedsnavnet har sin opprinnelse.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Stedsnavn_Opprinnelse">
                <EnumItem>VDB</EnumItem>
                <EnumItem>Holdeplassregister</EnumItem>
                <EnumItem>Stedsnavnsregisteret</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eksternReferanse</Name>
      <Definition>Feltet kan benyttes for referanse til stedsnavn tilknyttet Holdeplasser.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>VDB_Register</Name>
      <Definition>Angir hvilket VDB-register dette navnet er brukt i.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
