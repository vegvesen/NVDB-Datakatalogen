<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_STIKKRENNEKULVERT</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>bruksområde</Name>
         <Definition>Angir hva stikkrenne kulvert primært brukes til.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Bruksområde">
                  <EnumItem>Biologisk mangfold</EnumItem>
                  <EnumItem>Landbruk</EnumItem>
                  <EnumItem>Vann</EnumItem>
                  <EnumItem>Voll, vanngjennomløp</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>navn</Name>
         <Definition>Angir navn knyttet til stikkrenne/kulvert.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilknyttetLukkaDren</Name>
         <Definition>Angir om stikkrenne er tilknytta lukka drenering. Vannet ledes inn i et lukket dreneringssystem.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_TilknyttetLukkaDren">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjennomløpForElvBekk</Name>
         <Definition>Angir om elv/bekk renner gjennom stikkrenne/kulvert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_GjennomløpForElvBekk">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>materialtype</Name>
         <Definition>Angir materialtype.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Materialtype">
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>Eternitt</EnumItem>
                  <EnumItem>Naturstein</EnumItem>
                  <EnumItem>Plast</EnumItem>
                  <EnumItem>Stål</EnumItem>
                  <EnumItem>Tre</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>belegningStikkrenne</Name>
         <Definition>Angir om det er egen belegning i bunn stikkrenne av annet materiale enn stikkrenne for øvrig. Benyttes for sikring mot erosjon og/eller bremsing av vannhastighet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_BelegningStikkrenne">
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>Ingen belegning</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tverrsnittsform</Name>
         <Definition>Angir hvilken type tverrsnitt gjennomløpskonstruksjon har.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Tverrsnittsform">
                  <EnumItem>Ellipseform</EnumItem>
                  <EnumItem>Flatbunnet med hvelv</EnumItem>
                  <EnumItem>Rektangulær</EnumItem>
                  <EnumItem>Sirkulær</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>typeInnløp</Name>
         <Definition>Angir hvilken type innløp det er i ei stikkrenne.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_TypeInnløp">
                  <EnumItem>Kum over stikkrenne</EnumItem>
                  <EnumItem>Åpent i grøft</EnumItem>
                  <EnumItem>Åpent i grøft med støtteskjold</EnumItem>
                  <EnumItem>Åpent med frontmur</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>belegningInnløp</Name>
         <Definition>Angir om det er spesiell belegning i området rundt innløpet av stikkrenna.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_BelegningInnløp">
                  <EnumItem>Asfalt</EnumItem>
                  <EnumItem>Ingen/stedlige masser</EnumItem>
                  <EnumItem>Stein</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>typeUtløp</Name>
         <Definition>Angir hvilken type utløp det er i ei stikkrenne.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_TypeUtløp">
                  <EnumItem>I bekk/elv</EnumItem>
                  <EnumItem>I skråning/terreng</EnumItem>
                  <EnumItem>Kum</EnumItem>
                  <EnumItem>Åpen grøft</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>belegningUtløp</Name>
         <Definition>Angir om det er spesiell belegning i området rundt utløpet av stikkrenna.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_BelegningUtløp">
                  <EnumItem>Asfalt</EnumItem>
                  <EnumItem>Ingen/stedlige masser</EnumItem>
                  <EnumItem>Stein</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>fundamentering</Name>
         <Definition>Angir hvordan stikkrenne/kulvert er fundamentert.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Fundamentering">
                  <EnumItem>Bunnplate</EnumItem>
                  <EnumItem>Fjellfot</EnumItem>
                  <EnumItem>Grus</EnumItem>
                  <EnumItem>Leire</EnumItem>
                  <EnumItem>Pukk</EnumItem>
                  <EnumItem>Stedlige masser</EnumItem>
                  <EnumItem>Sålefundament</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prefabrikkert</Name>
         <Definition>Angir om gjennomløp er plassprodusert eller prefabrikkert. Bare aktuelt for stikkrenne/kulvert av betong.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Prefabrikkert">
                  <EnumItem>Plassprodusert</EnumItem>
                  <EnumItem>Prefabrikkert</EnumItem>
                  <EnumItem>Prefabrikkert, ikke NS3121</EnumItem>
                  <EnumItem>Prefabrikkert, NS3121</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lengde</Name>
         <Definition>Angir lengde av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>diameter_Innvendig</Name>
         <Definition>Angir innvendig diameter av gjennomløp. Benyttes fortrinnsvis for sirkulære tverrsnitt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bredde_Innvendig</Name>
         <Definition>Angir innvendig bredde av gjennomløpskonstruksjon. Ikke aktuell for sirkulære tverrsnitt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høyde_Innvendig</Name>
         <Definition>Angir innvendig høyde av gjennomløpskonstruksjon. Tar ikke hensyn til ev. igjenfylling i bunn av konstruksjon.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>retning</Name>
         <Definition>Angir hvilken retning i forhold til metrering vegobjektet har. Angir klokkeretning som vannet renner i, 12 angir at vannet renner parallelt med vegen i metreringsretningen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Retning">
                  <EnumItem>1</EnumItem>
                  <EnumItem>10</EnumItem>
                  <EnumItem>11</EnumItem>
                  <EnumItem>12</EnumItem>
                  <EnumItem>2</EnumItem>
                  <EnumItem>3</EnumItem>
                  <EnumItem>4</EnumItem>
                  <EnumItem>5</EnumItem>
                  <EnumItem>6</EnumItem>
                  <EnumItem>7</EnumItem>
                  <EnumItem>8</EnumItem>
                  <EnumItem>9</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vinkel</Name>
         <Definition>Angir om vinkel mellom stikkrenna og veg som stikkrenna krysser er rett eller skrå.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Vinkel">
                  <EnumItem>Rett</EnumItem>
                  <EnumItem>Skrå</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>helningFall</Name>
         <Definition>Angir fall på stikkrenne. Angis alltid som positiv verdi.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tykkelseOverfylling</Name>
         <Definition>Angir tykkelse overfylling av rørledning. Det vil si gjennomsnittlig tykkelse fra topp av rørledning til topp dekke.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>overfyllingUtløp</Name>
         <Definition>Angir tykkelsen på overfylling ved utløp. Det vil si tykkelse fra topp av stikkrenne til topp dekke.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>overfyllingInnløp</Name>
         <Definition>Angir tykkelsen på overfylling ved innløp. Det vil si tykkelse fra topp av stikkrenne til topp dekke.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>harInnløpsrist</Name>
         <Definition>Angir om det er innløpsrist i tilknytning til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_HarInnløpsrist">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>varmekabler</Name>
         <Definition>Angir om det er varmekabler eller ikke i tilknytning til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Varmekabler">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallTininger</Name>
         <Definition>Angir hvor mange ganger stikkrenna vanligvis må tines i løpet av en vinter.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>rehabilitering</Name>
         <Definition>Angir hvilken type rehabilitering som er gjort.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Rehabilitering">
                  <EnumItem>Delvis utskifting</EnumItem>
                  <EnumItem>Ikke rehabilitert</EnumItem>
                  <EnumItem>Innvendig glassfiberstrømpe</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>spesielleProblem</Name>
         <Definition>Angir eventuelle spesielle problem knyttet til stikkrennen. Dette er problem som stadig gjentar seg.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_SpesielleProblem">
                  <EnumItem>Kollaps</EnumItem>
                  <EnumItem>Masser, innløp</EnumItem>
                  <EnumItem>Masser, innløp og utløp</EnumItem>
                  <EnumItem>Masser, utløp</EnumItem>
                  <EnumItem>Vegetasjonstetting, innløp</EnumItem>
                  <EnumItem>Vegetasjonstetting, innløp og utløp</EnumItem>
                  <EnumItem>Vegetasjonstetting, utløp</EnumItem>
                  <EnumItem>Kapasitet</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>spesielleHensynFiskevandring</Name>
         <Definition>Angir om det skal tas spesielle hensyn i forhold til fiskevandring. For mer info se datterobjekt av type "Fysisk inngrep i vannforekomst".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_SpesielleHensynFiskevandring">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>driftsmerking</Name>
         <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavn_Rør</Name>
         <Definition>Angir produktnavn for rør. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper, kan f.eks. være spesielle forhold knyttet til oppbygging, utdyping av spesielle problem, m.m.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivnummer</Name>
         <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivreferanse</Name>
         <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>FKB_ID</Name>
         <Definition>Refererer til FKB-identitet. Benyttes i forbindelse med felles forvaltning av geometri.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_StikkrenneKulvert_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>høyde_Passasje</Name>
         <Definition>Angir innvendig høyde når det er tatt hensyn til eventuelle hindringer, f.eks. masser i bunn, oppheng i tak.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
