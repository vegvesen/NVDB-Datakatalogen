<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Styreapparat</Name>
  <Definition>Apparatur i tilknytning til teknisk utstyr.  Apparatur står vanligvis i skap.  Inneholder f.eks PLS, modem, batteri/strøm, telefon med mer.  Benyttes f.eks i tilknytning til signalanlegg.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir type styreapparat.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Type">
                <EnumItem>Signal</EnumItem>
                <EnumItem>Signal, C800V</EnumItem>
                <EnumItem>Signal, EC-1</EnumItem>
                <EnumItem>Signal, ELC</EnumItem>
                <EnumItem>Signal, ELC-2</EnumItem>
                <EnumItem>Signal, ELC-3</EnumItem>
                <EnumItem>Signal, ELC-2/3</EnumItem>
                <EnumItem>Signal, EPC</EnumItem>
                <EnumItem>Signal, EPC-4</EnumItem>
                <EnumItem>Signal, EPC-2</EnumItem>
                <EnumItem>Signal, FC</EnumItem>
                <EnumItem>Signal, FC-2000</EnumItem>
                <EnumItem>Signal, FC-2008</EnumItem>
                <EnumItem>Signal, FC-2016</EnumItem>
                <EnumItem>Signal, FC-400</EnumItem>
                <EnumItem>Signal, FCA</EnumItem>
                <EnumItem>Signal, FCA-400</EnumItem>
                <EnumItem>Signal, FCA-4000</EnumItem>
                <EnumItem>Signal, FCA-042</EnumItem>
                <EnumItem>Signal, FCA-082</EnumItem>
                <EnumItem>Signal, FCA-162</EnumItem>
                <EnumItem>Signal, FCA-2</EnumItem>
                <EnumItem>Signal, FR-80</EnumItem>
                <EnumItem>Signal, ITC-1</EnumItem>
                <EnumItem>Signal, ITC-2</EnumItem>
                <EnumItem>Signal, ITC-2 Mini</EnumItem>
                <EnumItem>Signal, ITC-3</EnumItem>
                <EnumItem>Signal, JCF</EnumItem>
                <EnumItem>Signal, JCF-150</EnumItem>
                <EnumItem>Signal, JCF-250</EnumItem>
                <EnumItem>Signal, KL</EnumItem>
                <EnumItem>Signal, KL-5000</EnumItem>
                <EnumItem>Signal, KLT-5000</EnumItem>
                <EnumItem>Signal, LC</EnumItem>
                <EnumItem>Signal, LC/FCA</EnumItem>
                <EnumItem>Signal, MQ</EnumItem>
                <EnumItem>Signal, MR</EnumItem>
                <EnumItem>Signal, NH</EnumItem>
                <EnumItem>Signal, PLS</EnumItem>
                <EnumItem>Signal, POFYR</EnumItem>
                <EnumItem>Signal, SC-5400</EnumItem>
                <EnumItem>Signal, SC-5408</EnumItem>
                <EnumItem>Signal, SC-8800</EnumItem>
                <EnumItem>Signal, SC-9016</EnumItem>
                <EnumItem>Signal, SC-9108</EnumItem>
                <EnumItem>Signal, SE</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hovedbruksområde for styreapparat.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Bruksområde">
                <EnumItem>Signalanlegg</EnumItem>
                <EnumItem>Skilt</EnumItem>
                <EnumItem>Vegbom</EnumItem>
                <EnumItem>Ventilasjon</EnumItem>
                <EnumItem>Annet utstyr</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>energikilde</Name>
      <Definition>Angir hvilken type energikilde som benyttes.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Energikilde">
                <EnumItem>Nettselskap</EnumItem>
                <EnumItem>Batteri</EnumItem>
                <EnumItem>Batteri/solcellepanel</EnumItem>
                <EnumItem>Batteri/vindgenerator</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kommunikasjon</Name>
      <Definition>Angir hvilken type kommunikasjon det er i tilknytning til stasjonen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Kommunikasjon">
                <EnumItem>Modem/oppringt</EnumItem>
                <EnumItem>Mobiltelefon</EnumItem>
                <EnumItem>Fast linje</EnumItem>
                <EnumItem>Manuell "tapping" av data</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>abonnementstype</Name>
      <Definition>Angir hvilken type abonnement det er på telefonforbindelsen (uoff).</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Abonnementstype">
                <EnumItem>Bredbånd/fiber</EnumItem>
                <EnumItem>ISDN</EnumItem>
                <EnumItem>ISDN, Basis</EnumItem>
                <EnumItem>ISDN, Plus</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>typeIO</Name>
      <Definition>Angir om det er digital eller analog tilkopling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_TypeIO">
                <EnumItem>Analog</EnumItem>
                <EnumItem>Digital</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>telefonnummer1</Name>
      <Definition>Angir telefonnr.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>telefonnummer2</Name>
      <Definition>Angir telefonnr.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>typeCPU_Kort</Name>
      <Definition>Angir type CPU-kort.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>igangsettingsdato</Name>
      <Definition>Angir hvilket år utstyret ble satt i drift i det systemet det er en del av (f.eks signalanlegget).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produksjonsår</Name>
      <Definition>Angir hvilket år utstyret ble produsert.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>driftsmerking</Name>
      <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>SCADA_Merking</Name>
      <Definition>Driftsmerking rettet spesifikt mot systemet SCADA.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>produktnavn</Name>
      <Definition>Angir produktnavn for vegobjektet. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og evt. serienummer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Styreapparat_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
