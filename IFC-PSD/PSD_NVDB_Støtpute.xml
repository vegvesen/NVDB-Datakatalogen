<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_STØTPUTE</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>produktnavnTypegodkjenning</Name>
         <Definition>Refererer til produktnavn og typegodkjenning (samsvarsgodkjenning) for støtputer godkjent av Statens vegvesen. Se http://www.vegvesen.no/Fag/Teknologi/Rekkverk+og+master/Sok+etter+godkjent+produkt.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_ProduktnavnTypegodkjenning">
                  <EnumItem>APM 100 - S100</EnumItem>
                  <EnumItem>APM 110 - S110</EnumItem>
                  <EnumItem>APM 50 - S50</EnumItem>
                  <EnumItem>APM 80 - S80</EnumItem>
                  <EnumItem>Brakemaster - uspes</EnumItem>
                  <EnumItem>CrashGuard 3S S80</EnumItem>
                  <EnumItem>CrashGuard 6S S110</EnumItem>
                  <EnumItem>Ermes S110</EnumItem>
                  <EnumItem>Ermes S80</EnumItem>
                  <EnumItem>EuroTRACC 80 Wide - S80</EnumItem>
                  <EnumItem>EuroTRACC S110 - S110</EnumItem>
                  <EnumItem>EuroTRACC S110 Wide - S110</EnumItem>
                  <EnumItem>EuroTRACC S80 - S80</EnumItem>
                  <EnumItem>QuadGuard - uspes</EnumItem>
                  <EnumItem>QuadGuard CEN - S100</EnumItem>
                  <EnumItem>QuadGuard CEN - S110 </EnumItem>
                  <EnumItem>QuadGuard CEN - S50</EnumItem>
                  <EnumItem>QuadGuard CEN - S80</EnumItem>
                  <EnumItem>Quest CEN - S100</EnumItem>
                  <EnumItem>Quest CEN - S110</EnumItem>
                  <EnumItem>Quest CEN - S80</EnumItem>
                  <EnumItem>SMA 100 - S100</EnumItem>
                  <EnumItem>SMA 110 - S110</EnumItem>
                  <EnumItem>SMA 50 - S50</EnumItem>
                  <EnumItem>SMA 80 - S80</EnumItem>
                  <EnumItem>SMA 80/1 - S80/1</EnumItem>
                  <EnumItem>TAU 100 - S100</EnumItem>
                  <EnumItem>TAU 60 - S50</EnumItem>
                  <EnumItem>TAU 80 - S80</EnumItem>
                  <EnumItem>TAU B - S110</EnumItem>
                  <EnumItem>TAU TUBE P100 - S100</EnumItem>
                  <EnumItem>TAU TUBE P110 - S110</EnumItem>
                  <EnumItem>TAU TUBE P50 - S50</EnumItem>
                  <EnumItem>TAU TUBE P80 - S80</EnumItem>
                  <EnumItem>WALT - S80/1</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sikkerhetsklasse</Name>
         <Definition>Angir hvilken sikkerhetsklasse støtpute er dimensjonert for. Jfr. Rekkverksnormal HB N101 (231) kap. 6.3.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Sikkerhetsklasse">
                  <EnumItem>S-100</EnumItem>
                  <EnumItem>S-110</EnumItem>
                  <EnumItem>S-50</EnumItem>
                  <EnumItem>S-80</EnumItem>
                  <EnumItem>S-80-1</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>avledningsevne</Name>
         <Definition>Støtputer deles inn i to typer etter deres evne til å fange opp og stanse eller avlede kjøretøyet ved en sidepåkjørsel. Kilde: Rekkverksnormal HB N101 (231).</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Avledningsevne">
                  <EnumItem>Avledende</EnumItem>
                  <EnumItem>Ikke avledende</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bevegelsesklasse</Name>
         <Definition>Angir bevegelsesklasse.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Bevegelsesklasse">
                  <EnumItem>Z1</EnumItem>
                  <EnumItem>Z2</EnumItem>
                  <EnumItem>Z3</EnumItem>
                  <EnumItem>Z4</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>utbøyningsklasse</Name>
         <Definition>Angir utbøyningsklasse.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Utbøyningsklasse">
                  <EnumItem>D1</EnumItem>
                  <EnumItem>D2</EnumItem>
                  <EnumItem>D3</EnumItem>
                  <EnumItem>D4</EnumItem>
                  <EnumItem>D5</EnumItem>
                  <EnumItem>D6</EnumItem>
                  <EnumItem>D7</EnumItem>
                  <EnumItem>D8</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skaderisiko</Name>
         <Definition>Angir skaderisiko.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Skaderisiko">
                  <EnumItem>A</EnumItem>
                  <EnumItem>B</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>inspeksjonsdato</Name>
         <Definition>Dato for når støtpute sist ble inspisert.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>driftsmerking</Name>
         <Definition>Identitet/navn på forekomst, normalt synlig på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Støtpute_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
