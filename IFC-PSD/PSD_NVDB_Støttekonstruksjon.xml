<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Støttekonstruksjon</Name>
  <Definition>Byggverk som brukes for å overvinne nivåforskjell hvor vanlig skråning vil ta for stor plass.  S. brukes også som sikringstiltak.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>areal</Name>
      <Definition>Angir areal av synlig del av støttekonstruksjonen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type vegobjektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Type">
                <EnumItem>Naturstein, murt, støpt</EnumItem>
                <EnumItem>Naturstein, murt, tørrmur</EnumItem>
                <EnumItem>Betong</EnumItem>
                <EnumItem>Betong, plasstøpt</EnumItem>
                <EnumItem>Betong, prefabrikkerte element</EnumItem>
                <EnumItem>Betongstein, murt, tørrmur</EnumItem>
                <EnumItem>Betongstein, murt, støpt</EnumItem>
                <EnumItem>Steinkurver</EnumItem>
                <EnumItem>Armert jord</EnumItem>
                <EnumItem>Tre/Sognemur</EnumItem>
                <EnumItem>Spunt</EnumItem>
                <EnumItem>Slissevegg</EnumItem>
                <EnumItem>Bjelkestengsel</EnumItem>
                <EnumItem>Sekantvegg</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>bruksområde</Name>
      <Definition>Angir hva vegobjektet brukes til, hvilken funksjon det har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Bruksområde">
                <EnumItem>Støtte opp skjæringsskråning</EnumItem>
                <EnumItem>Støtte opp fyllingsskråning</EnumItem>
                <EnumItem>Vingemur</EnumItem>
                <EnumItem>Sikre fjellskjæring</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fundamentering</Name>
      <Definition>Angir fundamenteringsmetode for støttekonstruksjonen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Fundamentering">
                <EnumItem>Betongsåle</EnumItem>
                <EnumItem>Trykkimpregnert tresåle</EnumItem>
                <EnumItem>Fjellfot</EnumItem>
                <EnumItem>Løsmasse/fjell</EnumItem>
                <EnumItem>På peler</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forblending</Name>
      <Definition>Type forblending.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Forblending">
                <EnumItem>Betongmur</EnumItem>
                <EnumItem>Ingen forblending</EnumItem>
                <EnumItem>Tørrmur</EnumItem>
                <EnumItem>Torvblokker</EnumItem>
                <EnumItem>Tre</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>konstruksjonsbeskrivelse</Name>
      <Definition>Beskrivelse av relevante forhold som ikke dekkes av andre egenskaper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>konstruksjonshøyde_Maksimal</Name>
      <Definition>Største høyde fra bunn til topp av støttekonstruksjonen. Inkluderer både synlig og tildekket del.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>konstruksjonshøyde_Minimum</Name>
      <Definition>Minste høyde fra bunn til topp av støttekonstruksjonen. Inkluderer både synlig og tildekket del.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>høyde_Synlig_Maksimal</Name>
      <Definition>Angir største synlige høyde for støttekonstruksjon.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>høyde_Synlig_Gjennomsnitt</Name>
      <Definition>Angir gjennomsnittlig synlig høyde for støttekonstruksjonen.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tykkelse_Gjennomsnitt</Name>
      <Definition>Angir støttekonstruksjonens gjennomsnittstykkelse. Inkluderer ikke eventuell forblending.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lengde</Name>
      <Definition>Angir lengde av støttekonstruksjon.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>aktivForankring</Name>
      <Definition>Angir hvor vidt støttekonstruksjonen har en aktiv støttefunksjon etter at anlegget er ferdig.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_AktivForankring">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>brutus_Id</Name>
      <Definition>Gir referanse til Brutus.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>etableringsår</Name>
      <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>arkivreferanse</Name>
      <Definition>Gir referanse/link til ytterligere informasjon om vegobjektet. Fortrinnsvis til vegeiers eget arkivsystem. Kan være til mappe/sak med tilgang til ulik informasjon eller direkte til et dokument.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Støttekonstruksjon_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
