<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_SVINGERESTRIKSJON</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>gjelderHverdagerULørdagFraKl</Name>
         <Definition>Svingerestriksjon gjelder på hverdager unntatt lørdag fra angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderHverdagerULørdagTilKl</Name>
         <Definition>Svingeforbud gjelder på hverdager unntatt lørdag til angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderLørdagFraKl</Name>
         <Definition>Svingeforbud gjelder på lørdager fra angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderLørdagTilKl</Name>
         <Definition>Svingeforbud gjelder på lørdager til angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderSøndagFraKl</Name>
         <Definition>Svingeforbud gjelder på søndager fra angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderSøndagTilKl</Name>
         <Definition>Svingeforbud gjelder på søndager til angitt klokkeslett.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjelderUkedag</Name>
         <Definition>Svingeforbud gjelder kun angitt ukedag.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Svingerestriksjon_GjelderUkedag">
                  <EnumItem>Fredag</EnumItem>
                  <EnumItem>Mandag</EnumItem>
                  <EnumItem>Onsdag</EnumItem>
                  <EnumItem>Tirsdag</EnumItem>
                  <EnumItem>Torsdag</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gyldigFraDato</Name>
         <Definition>Dato for når svingerestriksjonen ble innført. Den dato skilt ble "avduket"</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>merknad</Name>
         <Definition>Kan gi permanent merknad. F.eks. "høyresving forbudt".</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedtaksnummer</Name>
         <Definition>Angir vedtaksnummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivnummer</Name>
         <Definition>Gir referanse til relevant sak i vegeiers arkivsystem.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
