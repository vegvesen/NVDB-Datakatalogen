<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Sykkelrute_TurFritid</Name>
  <Definition>Fastlagt rute for sykling, primært for tur/fritidssykling.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>kategori</Name>
      <Definition>Angir hvilken hovedkategori sykkelruta tilhører.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkelrute_TurFritid_Kategori">
                <EnumItem>Nasjonal/europeisk sykkelrute</EnumItem>
                <EnumItem>Regional sykkelrute</EnumItem>
                <EnumItem>Lokal sykkelrute</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>navn</Name>
      <Definition>Gir navn til Sykkelvegrute.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nummer</Name>
      <Definition>Angir nummer på sykkelrute.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelse</Name>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>nasjonalSykkelrute</Name>
      <Definition>Angir nummer/navn på nasjonal sykkelrute.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkelrute_TurFritid_NasjonalSykkelrute">
                <EnumItem>1 - Kystruta</EnumItem>
                <EnumItem>2 - Kanalruta</EnumItem>
                <EnumItem>3 - Fjord og fjell</EnumItem>
                <EnumItem>4 - Rallarvegen</EnumItem>
                <EnumItem>5 - Numedalsruta</EnumItem>
                <EnumItem>6 - Sognefjellsvegen</EnumItem>
                <EnumItem>7 - Pilegrimsruta</EnumItem>
                <EnumItem>8 - Trollheimen</EnumItem>
                <EnumItem>9 - Villmarksruta</EnumItem>
                <EnumItem>10 - Nordkapp - Lindesnes</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>euroVeloSykkelrute</Name>
      <Definition>Angir nummer/navn på EuroVelosykkelrute.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkelrute_TurFritid_EuroVeloSykkelrute">
                <EnumItem>1 - Atlantic Coast Route</EnumItem>
                <EnumItem>3 - Pilgrims Route</EnumItem>
                <EnumItem>7 - Sun Route</EnumItem>
                <EnumItem>11 - East Europe Route</EnumItem>
                <EnumItem>12 - North Sea Cycle Route</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skiltet</Name>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkelrute_TurFritid_Skiltet">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
                <EnumItem>Delvis</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kvalitetssikretDato</Name>
      <Definition>Dato informasjon sist var kvalitetssikret.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kvalitetssikretAv</Name>
      <Definition>Angir hvem som utførte siste kvalitetssikring.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
