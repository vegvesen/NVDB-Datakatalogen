<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Sykkeltilbud_Riksvegrute</Name>
  <Definition>Informasjon om sykkeltilbud langs riksvegrute.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>riksvegrute</Name>
      <Definition>Angir hvilken riksvegrute aktuelt sykkeltilbud gjelder for.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkeltilbud_Riksvegrute_Riksvegrute">
                <EnumItem>Rute 1: E6 Riksgrensen/Svinesund - Oslo</EnumItem>
                <EnumItem>Rute 2a: E18 Riksgrensen/Ørje - Oslo</EnumItem>
                <EnumItem>Rute 2b: E16 Riksgrensen - Hønefoss og Rv35 Hønefoss - Hokksund</EnumItem>
                <EnumItem>Rute 3: E18 Oslo - Kristiansand - Stavanger</EnumItem>
                <EnumItem>Rute 4a: E39 Stavanger - Bergen - Ålesund</EnumItem>
                <EnumItem>Rute 4b: E39 Ålesund - Trondheim</EnumItem>
                <EnumItem>Rute 4c: Rv 9 Kristiansand - Haukeligrend og Rv 13/55 Jøsendal - Sogndal</EnumItem>
                <EnumItem>Rute 5a: E134 Drammen - Haugesund</EnumItem>
                <EnumItem>Rute 5b: Rv 7 Hønefoss - Bu og Rv52 Gol - Borlaug</EnumItem>
                <EnumItem>Rute 5c: E16 Sandvika - Bergen</EnumItem>
                <EnumItem>Rute 6a: E6 Oslo - Trondheim</EnumItem>
                <EnumItem>Rute 6b: Rv3 Kolomoen - Ulsberg</EnumItem>
                <EnumItem>Rute 6c: Rv15 Otta - Måløy</EnumItem>
                <EnumItem>Rute 6d: E136 Dombås - Ålesund</EnumItem>
                <EnumItem>Rute 6e: Rv70 Oppdal - Kristiansund</EnumItem>
                <EnumItem>Rute 7: E6 Trondheim - Fauske</EnumItem>
                <EnumItem>Rute 8a: E6 Fauske - Nordkjosbotn</EnumItem>
                <EnumItem>Rute 8b: E6 Nordkjosbotn - Kirkenes</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>sykkeltilbud</Name>
      <Definition>Angir hvilket tilbud det er til syklende.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkeltilbud_Riksvegrute_Sykkeltilbud">
                <EnumItem>Gang- og sykkelveg</EnumItem>
                <EnumItem>Sykkelveg</EnumItem>
                <EnumItem>Sykkelekspressveg</EnumItem>
                <EnumItem>Sykkelfelt</EnumItem>
                <EnumItem>Blandet trafikk </EnumItem>
                <EnumItem>Sykkeltilbud på lokalt vegnett</EnumItem>
                <EnumItem>Sykling forbudt eller ikke anbefalt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelse</Name>
      <Definition>Beskrivelse av strekning.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vurderingAvTilbud</Name>
      <Definition>Vurdering av kvalitet på tilbud. Vurdering foretas av regionale sykkelkoordinatorer. Vegnormaler legges til grunn i vurderingen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Sykkeltilbud_Riksvegrute_VurderingAvTilbud">
                <EnumItem>Tilfredsstillende</EnumItem>
                <EnumItem>Ikke tilfredsstillende</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kvalitetssikretDato</Name>
      <Definition>Dato informasjon sist var kvalitetssikret.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kvalitetssikretAv</Name>
      <Definition>Angir hvem som utførte siste kvalitetssikring.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
