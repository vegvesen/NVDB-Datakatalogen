<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Testobjekttype</Name>
  <Definition>Denne objekttypen benyttes ifbm testing av NVDB.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>test_struct_liste_mbilde</Name>
      <Definition>Egenskapstype til test av liste av "structer". Liste inneholder et sett av bilder med tilhørende bildetekst.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>farge_liste</Name>
      <Definition>Angir alle farger som er brukt (for testformål).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fargeliste_TV_tekst</Name>
      <Definition>Test av liste med farger hvor det er predefinerte verdier for farge.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Testobjekttype_Fargeliste_TV_tekst">
                <EnumItem>Gul</EnumItem>
                <EnumItem>Blå</EnumItem>
                <EnumItem>Grå</EnumItem>
                <EnumItem>Rød</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fargelistenr_TV_tall</Name>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Testobjekttype_Fargelistenr_TV_tall">
                <EnumItem>1</EnumItem>
                <EnumItem>2</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>test_dato</Name>
      <Definition>Test av dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>test_dato_liste</Name>
      <Definition>Test liste med dato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>ansvarlig_strukt</Name>
      <Definition>Angir person som er ansvarlig (for testformål).</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
