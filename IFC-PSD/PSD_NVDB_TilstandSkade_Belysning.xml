<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Belysning</Name>
  <Definition>Gir informasjon om tilstand og eventuell skade på belysning.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>tilstand</Name>
      <Definition>Angir tilstand til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Belysning_Tilstand">
                <EnumItem>Ok</EnumItem>
                <EnumItem>Tiltaksbehov</EnumItem>
                <EnumItem>Mangler</EnumItem>
                <EnumItem>Funksjonell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadetype</Name>
      <Definition>Angir hvilken type skade det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Belysning_Skadetype">
                <EnumItem>Skjev mast</EnumItem>
                <EnumItem>Knust armatur</EnumItem>
                <EnumItem>Mørk</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDate"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltak</Name>
      <Definition>Angir hvilket tiltak som bør foretas på vegobjektet som skade er koplet mot.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltakNår</Name>
      <Definition>Angir tidsperiode fra skade er registrert til tiltaket bør være utført.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Belysning_TiltakNår">
                <EnumItem>Straks</EnumItem>
                <EnumItem>14 dager</EnumItem>
                <EnumItem>3 mnd</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
