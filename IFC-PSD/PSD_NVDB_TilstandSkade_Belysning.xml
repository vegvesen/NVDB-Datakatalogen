<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_TILSTANDSKADE_BELYSNING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>tilstand</Name>
         <Definition>Angir tilstand til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Tilstandskade, belysning_Tilstand">
                  <EnumItem>Funksjonell</EnumItem>
                  <EnumItem>Mangler</EnumItem>
                  <EnumItem>Ok</EnumItem>
                  <EnumItem>Tiltaksbehov</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skadetype</Name>
         <Definition>Angir hvilken type skade det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Tilstandskade, belysning_Skadetype">
                  <EnumItem>Knust armatur</EnumItem>
                  <EnumItem>Mørk</EnumItem>
                  <EnumItem>Skjev mast</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datoKontrollert</Name>
         <Definition>Angir dato for når kontroll ble foretatt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tiltak</Name>
         <Definition>Angir hvilket tiltak som bør foretas på vegobjektet som skade er koplet mot.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tiltakNår</Name>
         <Definition>Angir tidsperiode fra skade er registrert til tiltaket bør være utført.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Tilstandskade, belysning_TiltakNår">
                  <EnumItem>14 dager</EnumItem>
                  <EnumItem>3 mnd</EnumItem>
                  <EnumItem>Straks</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
