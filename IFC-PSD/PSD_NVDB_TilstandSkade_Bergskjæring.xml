<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Bergskjæring</Name>
  <Definition>Angir tilstand og skade knyttet til en bergskjæring.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>observertNedfall</Name>
      <Definition>Angir om det er observert nedfall fra skjæring, og indikasjon på omfang.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Bergskjæring_ObservertNedfall">
                <EnumItem>Nei</EnumItem>
                <EnumItem>Ja, 0-1 kbm</EnumItem>
                <EnumItem>Ja, 1-5 kbm</EnumItem>
                <EnumItem>Ja, &gt; 5 kbm</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fareForNedfall</Name>
      <Definition>Angir om det er fare for fremtidig nedfall fra bergskjæring.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Bergskjæring_FareForNedfall">
                <EnumItem>Nei</EnumItem>
                <EnumItem>Ja</EnumItem>
                <EnumItem>Ja, inntil 5 kbm</EnumItem>
                <EnumItem>Ja, inntil 10 kbm</EnumItem>
                <EnumItem>Ja, mer enn 10 kbm</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vannISkjæring</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Bergskjæring_VannISkjæring">
                <EnumItem>Rennende vann</EnumItem>
                <EnumItem>Fuktig</EnumItem>
                <EnumItem>Tørt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilstand</Name>
      <Definition>Angir tilstand på gitt tidspunkt. EU-kategorier.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Bergskjæring_Tilstand">
                <EnumItem>0-God kvalitet</EnumItem>
                <EnumItem>1-Middels kvalitet</EnumItem>
                <EnumItem>2-Dårlig kvalitet</EnumItem>
                <EnumItem>3-Svært dårlig kvalitet</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltakNår</Name>
      <Definition>Angir tidsperiode fra skade er registrert til tiltaket bør være utført.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Bergskjæring_TiltakNår">
                <EnumItem>Straks</EnumItem>
                <EnumItem>14 dager</EnumItem>
                <EnumItem>3 mnd</EnumItem>
                <EnumItem>1 år</EnumItem>
                <EnumItem>1-2 år</EnumItem>
                <EnumItem>&gt; 2år</EnumItem>
                <EnumItem>på sikt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kontrollertAv</Name>
      <Definition>Angir hvem som er ansvarlig for kontroll.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
