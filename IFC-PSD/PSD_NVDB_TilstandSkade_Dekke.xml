<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Dekke</Name>
  <Definition>Gir informasjon om tilstand og eventuell skade på vegdekke.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>antall</Name>
      <Definition>Vegobjektet kan representere flere objekt av samme type. Vi angir her hvor mange objekt dette objektet representerer.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadetype</Name>
      <Definition>Angir hvilken type skade det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Dekke_Skadetype">
                <EnumItem>Område med krakkelering</EnumItem>
                <EnumItem>Langsgående dekkesprekk</EnumItem>
                <EnumItem>Tversgående dekkesprekk</EnumItem>
                <EnumItem>Overflateskade</EnumItem>
                <EnumItem>Sporslitasje</EnumItem>
                <EnumItem>Hull</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>årsakTilSkadeMangel</Name>
      <Definition>Angir årsak til en skade eller mangel.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Dekke_ÅrsakTilSkadeMangel">
                <EnumItem>Dårlige grunnforhold</EnumItem>
                <EnumItem>Dårlig vegfundament</EnumItem>
                <EnumItem>Telefarlig materiale</EnumItem>
                <EnumItem>Dårlig drenering</EnumItem>
                <EnumItem>Sporslitasje</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>registreringsdato</Name>
      <Definition>Angir dato da vegobjektet ble registrert. Datoformat: ååååmmdd.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>alvorlighetsgrad</Name>
      <Definition>Angir hvor alvorlig en skade/tilstand er.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Dekke_Alvorlighetsgrad">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
                <EnumItem>Kritisk</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>utbredelse</Name>
      <Definition>Angir hvor utbredt en skade/tilstand er.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Dekke_Utbredelse">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
