<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Grøft</Name>
  <Definition>Gir informasjon om tilstand og eventuell skade på åpen grøft.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>tilstand</Name>
      <Definition>Angir tilstand til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Grøft_Tilstand">
                <EnumItem>Ok</EnumItem>
                <EnumItem>Tiltaksbehov</EnumItem>
                <EnumItem>Mangler</EnumItem>
                <EnumItem>Funksjonell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadetype</Name>
      <Definition>Angir hvilken type skade det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Grøft_Skadetype">
                <EnumItem>Tilgrodd</EnumItem>
                <EnumItem>Tettet pga ras</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDate"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltak</Name>
      <Definition>Angir hvilket tiltak som bør foretas på vegobjektet som skade er koplet mot.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Grøft_Tiltak">
                <EnumItem>Grøfterensk</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltakNår</Name>
      <Definition>.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
