<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Punkt</Name>
  <Definition>Gir informasjon om tilstand og eventuell skade.  Normalt knyttet til et spesifikt punktobjekt.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>tilstand</Name>
      <Definition>Angir tilstand til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Punkt_Tilstand">
                <EnumItem>Ok</EnumItem>
                <EnumItem>Tiltaksbehov</EnumItem>
                <EnumItem>Mangler</EnumItem>
                <EnumItem>Funksjonell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadetype</Name>
      <Definition>Angir hvilken type skade det er tale om.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltak</Name>
      <Definition>Angir hvilket tiltak som bør foretas på vegobjektet som skade er koplet mot.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Punkt_Tiltak">
                <EnumItem>Skiftes</EnumItem>
                <EnumItem>Fjernes</EnumItem>
                <EnumItem>Utbedres på stedet</EnumItem>
                <EnumItem>Ryddes</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltakNår</Name>
      <Definition>Angir tidsperiode fra skade er registrert til tiltaket bør være utført.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Punkt_TiltakNår">
                <EnumItem>Straks</EnumItem>
                <EnumItem>14 dager</EnumItem>
                <EnumItem>3 mnd</EnumItem>
                <EnumItem>1 år</EnumItem>
                <EnumItem>1-2 år</EnumItem>
                <EnumItem>&gt; 2år</EnumItem>
                <EnumItem>på sikt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>test_Tilstandsindeks</Name>
      <Definition>Angir tilstand. Til testformål.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
