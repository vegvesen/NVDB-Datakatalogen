<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_TilstandSkade_Skiltpunkt</Name>
  <Definition>Gir informasjon om tilstand og eventuell skade på skiltpunkt.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>tilstand</Name>
      <Definition>Angir tilstand til vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Skiltpunkt_Tilstand">
                <EnumItem>Ok</EnumItem>
                <EnumItem>Tiltaksbehov</EnumItem>
                <EnumItem>Mangler</EnumItem>
                <EnumItem>Funksjonell</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>skadetype</Name>
      <Definition>Angir hvilken type skade det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Skiltpunkt_Skadetype">
                <EnumItem>Brøyteskade</EnumItem>
                <EnumItem>Hærverk</EnumItem>
                <EnumItem>Påkjørsel</EnumItem>
                <EnumItem>Stor nedsmussing</EnumItem>
                <EnumItem>Skjevhet</EnumItem>
                <EnumItem>Defekt/ute av funksjon</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>datoKontrollert</Name>
      <Definition>Angir dato for når kontroll ble foretatt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tiltak</Name>
      <Definition>Angir hvilket tiltak som bør foretas på vegobjektet som skade er koplet mot.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_TilstandSkade_Skiltpunkt_Tiltak">
                <EnumItem>Skiftes</EnumItem>
                <EnumItem>Flyttes</EnumItem>
                <EnumItem>Fjernes</EnumItem>
                <EnumItem>Forsterkes</EnumItem>
                <EnumItem>Rettes</EnumItem>
                <EnumItem>Siktrydding</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>test_Tilstandsindeks</Name>
      <Definition>Angir tilstand. Til testformål.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>tilleggsinformasjon</Name>
      <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
