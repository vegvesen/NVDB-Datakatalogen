<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_TOALETTANLEGG</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type toalett det er i toalettanlegget.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_Type">
                  <EnumItem>Tørrklosett</EnumItem>
                  <EnumItem>Vannklosett</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallKlosett</Name>
         <Definition>Angir hvor mange klosett/toalettskåler/pissoarer det er til sammen på toalettanlegget.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>universeltUtformet</Name>
         <Definition>Angir om det finnes universelt utformet toalett på toalettanlegget.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_UniverseltUtformet">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>avløpTil</Name>
         <Definition>Angir hvor avløpet føres. (Ikke aktuelt for tørrklosett).</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_AvløpTil">
                  <EnumItem>Lukket tank</EnumItem>
                  <EnumItem>Offentlig nett</EnumItem>
                  <EnumItem>Privat nett</EnumItem>
                  <EnumItem>Slamavskiller med utslipp til infiltrasjonsbasseng</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vinterstengt</Name>
         <Definition>Angir om toalettanlegg er vinterstengt.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_Vinterstengt">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vinterstengt_FraDato</Name>
         <Definition> Angir måned og dato (mmdd) for når toalettanlegget vinterstenges i et normalår.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vinterstengt_TilDato</Name>
         <Definition>Angir måned og dato (mmdd) for når toalettanlegget er vinterstengt til i et normalår.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>privatAvtale</Name>
         <Definition>Angir om det finnes avtale med privat eier om bruk av toalett.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_PrivatAvtale">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produsent</Name>
         <Definition>Angir navn på produsent/fabrikant av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>produktnavn</Name>
         <Definition>Angir produktnavn for toalettene. Produktnavn kan inneholde modellnavn, typebetegnelse, typenummer og ev. serienummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>leverandør</Name>
         <Definition>Angir navn på firma som har levert vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper, f.eks. utfyllende informasjon om toalettanlegg. Kan f.eks. være ang. tilgjengelighet, eierforhold osv.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Toalettanlegg_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
