<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_TRAFIKKØY</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type vegobjektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Type">
                  <EnumItem>Delende trafikkøy med venstresvingefelt</EnumItem>
                  <EnumItem>Delende trafikkøy uten venstresvingefelt</EnumItem>
                  <EnumItem>Dråpeøy</EnumItem>
                  <EnumItem>Parallelldeleøy, rundkjøring</EnumItem>
                  <EnumItem>Sentraløy, rundkjøring</EnumItem>
                  <EnumItem>Trafikkøy v filterfelt</EnumItem>
                  <EnumItem>Trafikkøy v fotgjengerovergang</EnumItem>
                  <EnumItem>Trafikkøy v snuplass</EnumItem>
                  <EnumItem>Trekantdeleøy, rundkjøring</EnumItem>
                  <EnumItem>Trekantøy</EnumItem>
                  <EnumItem>Trompetdeleøy, rundkjøring</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>utforming</Name>
         <Definition>Utforming av vegobjekt.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Utforming">
                  <EnumItem>Fysisk m kantstein</EnumItem>
                  <EnumItem>Malt</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>belegning</Name>
         <Definition>Angir hva vegobjektet er belagt med.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Belegning">
                  <EnumItem>Asfalt</EnumItem>
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>Betong, farget</EnumItem>
                  <EnumItem>Betong, mønstret</EnumItem>
                  <EnumItem>Betong, mønstret og farget</EnumItem>
                  <EnumItem>Betongheller</EnumItem>
                  <EnumItem>Betongstein</EnumItem>
                  <EnumItem>Granitt</EnumItem>
                  <EnumItem>Grønt/beplantning</EnumItem>
                  <EnumItem>Pukk</EnumItem>
                  <EnumItem>Skiferheller</EnumItem>
                  <EnumItem>Stein</EnumItem>
                  <EnumItem>Stein, brostein</EnumItem>
                  <EnumItem>Stein, faststøpt i betong</EnumItem>
                  <EnumItem>Stein, rundstein</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>belegning_OverkjørbartAreal</Name>
         <Definition>Angir hvilken type belegning det er på overkjørbart areal.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Belegning_OverkjørbartAreal">
                  <EnumItem>Asfalt</EnumItem>
                  <EnumItem>Betong</EnumItem>
                  <EnumItem>Betongstein</EnumItem>
                  <EnumItem>Stein, brostein</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir totalt areal av vegobjektet. For fysiske øyer skal arealet regnes til ytterkant kantstein. Ev. overkjørbart areal skal medregnes. For malte øyer skal arealet regnes til ytterkant maling.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal_Overkjørbart</Name>
         <Definition>Angir areal av den delen av trafikkøyen som er overkjørbar. Dette arealet er tiltenkt framkommelighet for store kjøretøy ev. spesialkjøretøy.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>FKB_ID</Name>
         <Definition>Refererer til FKB-identitet. Benyttes i forbindelse med felles forvaltning av geometri.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Trafikkøy_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
