  <IfcVersion/>
  <Name>Pset_NVDB_UlykkesinvolvertPerson</Name>
  <Definition>Personer involvert i trafikkulykke.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>passasjerNummer</Name>
      <Definition>Passasjernummer, f.eks. A0.  Satt sammen av ENHETS-bokstav og et fortløpende nummer. Fører=0, resten er fortløpende.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kjønnIUlykke</Name>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_UlykkesinvolvertPerson_KjønnIUlykke">
                <EnumItem>Mann</EnumItem>
                <EnumItem>Kvinne</EnumItem>
                <EnumItem>Ukjent</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fødselsdato</Name>
      <Definition>Fødselsdato.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
