      <Definition>Alder.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>alderskategori</Name>
      <Definition>Angir hvilken aldersgruppe ulykkesinvolvert person tilhører.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_UlykkesinvolvertPerson_Alderskategori">
                <EnumItem>0-5 år</EnumItem>
                <EnumItem>6-15</EnumItem>
                <EnumItem>16-17</EnumItem>
                <EnumItem>18-19</EnumItem>
                <EnumItem>20-24</EnumItem>
                <EnumItem>25-34</EnumItem>
                <EnumItem>35-44</EnumItem>
                <EnumItem>45-54</EnumItem>
                <EnumItem>55-64</EnumItem>
                <EnumItem>65-74</EnumItem>
                <EnumItem>75-84</EnumItem>
                <EnumItem>85 år eller eldre</EnumItem>
                <EnumItem>Uoppgitt alder</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
