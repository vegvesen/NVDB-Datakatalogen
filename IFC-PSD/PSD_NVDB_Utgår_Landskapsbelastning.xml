         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Landskapsbelastning (Historisk)_Landskapstema">
                  <EnumItem>Kulturmiljø</EnumItem>
                  <EnumItem>Landskapsbilde</EnumItem>
