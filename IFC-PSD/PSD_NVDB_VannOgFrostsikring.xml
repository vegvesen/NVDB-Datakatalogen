<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_VANNOGFROSTSIKRING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type vegobjektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vann- og frostsikring_Type">
                  <EnumItem>Betongelementer i vegger og tak</EnumItem>
                  <EnumItem>Betongelementer i vegger, sprøytebetong på knølmontert PE-skum i tak</EnumItem>
                  <EnumItem>Betongelementer i vegger, sprøytebetong på PE-skum i tak</EnumItem>
                  <EnumItem>Lettbetongelementer i vegger og tak</EnumItem>
                  <EnumItem>Lette konstruksjoner, platehvelv, aluminium</EnumItem>
                  <EnumItem>Lette konstruksjoner, platehvelv, stål</EnumItem>
                  <EnumItem>PE-skum, utildekket</EnumItem>
                  <EnumItem>Sprøytebetong, fiberarmert på knølmontert PE-skum</EnumItem>
                  <EnumItem>Sprøytebetonghvelv, nettarmert på membran</EnumItem>
                  <EnumItem>Sprøytebetonghvelv, nettarmert på PE-skum</EnumItem>
                  <EnumItem>Tunnelduk</EnumItem>
                  <EnumItem>Betongelementer i vegger, sprøytebetong på membran i tak</EnumItem>
                  <EnumItem>Annet</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir areal av vann- og frostsikring. Det kan beregnes som produkt av lengde og omriss knyttet til tunnelprofil (se N100).</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lengde</Name>
         <Definition>Angir lengde av vann-/frostsikring.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallElement</Name>
         <Definition>Angir hvor mange enkeltelement en konstruksjon er satt sammen av.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tykkelse</Name>
         <Definition>Angir total tykkelse av hvelvkonstruksjon.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sprøytebetong_HarMikrofiber</Name>
         <Definition>Angir om det er mikrofiber i sprøytebetong (brannsikring).</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vann- og frostsikring_Sprøytebetong_HarMikrofiber">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sprøytebetong_HarLettilslag</Name>
         <Definition>Angir om det er det er lettilslag i sprøytebetong (f.eks. cafco).</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vann- og frostsikring_Sprøytebetong_HarLettilslag">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>sprøytebetong_Tykkelse</Name>
         <Definition>Angir gjennomsnittlig tykkelse av sprøytebetong.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vann- og frostsikring_Sprøytebetong_Tykkelse">
                  <EnumItem>2 cm</EnumItem>
                  <EnumItem>6 cm</EnumItem>
                  <EnumItem>8 cm</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dokumentasjonBrannfarlighet</Name>
         <Definition>Gir referanse til dokumentasjon om brannfarlighet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>brannfarlig</Name>
         <Definition>Angir om tunnelhvelv er brannfarlig eller ikke.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vann- og frostsikring_Brannfarlig">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>etableringsår</Name>
         <Definition>Angir hvilket år vegobjektet ble etablert på stedet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
