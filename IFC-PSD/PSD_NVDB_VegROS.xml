<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_VegROS</Name>
  <Definition>Punkt/strekning i vegnettet hvor det er økt risiko og sårbarhet for langvarige fremkommelighetsbrudd. Merknad: Skal ha minst 10 meters utstrekning.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>a01_Bruddhendelse</Name>
      <Definition>Angir type bruddhendelse.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A01_Bruddhendelse">
                <EnumItem>Veg stengt</EnumItem>
                <EnumItem>Tunnel stengt</EnumItem>
                <EnumItem>Bru stengt</EnumItem>
                <EnumItem>Ferje/ferjekai stengt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a02_BruddårsakSpesifikk</Name>
      <Definition>Angir hva som er årsak til bruddhendelse.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A02_BruddårsakSpesifikk">
                <EnumItem>Bilberging (Vogntog)</EnumItem>
                <EnumItem>Brann (i objekter på/nær vegen)</EnumItem>
                <EnumItem>Dreneringssvikt</EnumItem>
                <EnumItem>Farlig gods ulykke</EnumItem>
                <EnumItem>Flom</EnumItem>
                <EnumItem>Fundamenteringssvikt</EnumItem>
                <EnumItem>Høyde - lengde - bredde - vekt</EnumItem>
                <EnumItem>Skred kvikkleire</EnumItem>
                <EnumItem>Skred jord</EnumItem>
                <EnumItem>Skred snø</EnumItem>
                <EnumItem>Skred stein</EnumItem>
                <EnumItem>Nedfall fra fjellskjæring</EnumItem>
                <EnumItem>Stormflo</EnumItem>
                <EnumItem>Strømbrudd</EnumItem>
                <EnumItem>Ulykkespunkt/-strekning</EnumItem>
                <EnumItem>Utglidning</EnumItem>
                <EnumItem>Vind</EnumItem>
                <EnumItem>Snø</EnumItem>
                <EnumItem>Annen årsak</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a03_Sted</Name>
      <Definition>Stedsnavn eller strekningsnavn.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a04_Beskrivelse</Name>
      <Definition>Beskrivelse av hendelse og særskilte forhold ved denne.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a05_BruddårsakHovedgruppe</Name>
      <Definition>Angir hovedgruppe bruddfare.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A05_BruddårsakHovedgruppe">
                <EnumItem>Natur</EnumItem>
                <EnumItem>Trafikk</EnumItem>
                <EnumItem>Konstruksjon</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a06_BruddårsakUndergruppe</Name>
      <Definition>Angir undergruppe bruddfare.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A06_BruddårsakUndergruppe">
                <EnumItem>Skred</EnumItem>
                <EnumItem>Flom</EnumItem>
                <EnumItem>Vær</EnumItem>
                <EnumItem>Ulykke</EnumItem>
                <EnumItem>Konstruksjonsfeil</EnumItem>
                <EnumItem>Teknisk svikt</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a10_Risiko</Name>
      <Definition>Angir grad av fare for og alvorlighet av et framkommelighetsbrudd på dette stedet/denne strekningen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A10_Risiko">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a11_Trussel</Name>
      <Definition>Samleparameter for enkeltparametrene Sannsynlighet, Omfang og Varighet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A11_Trussel">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a12_Verdi</Name>
      <Definition>Samleparameter for enkeltparametrene ÅDT, Andel tunge, TBK og Viktighet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A12_Verdi">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a13_Sårbarhet</Name>
      <Definition>Samleparameter for enkeltparametrene Omkjøring og Beredskap.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A13_Sårbarhet">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a14_Usikkerhet</Name>
      <Definition>Angir grad av usikkerhet i vurderingene som ligger til grunn for fastsettelse av risiko.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A14_Usikkerhet">
                <EnumItem>Liten</EnumItem>
                <EnumItem>Middels</EnumItem>
                <EnumItem>Stor</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a15_FagligGrunnlag</Name>
      <Definition>Angir det faglige grunnlaget for vurderingen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_VegROS_A15_FagligGrunnlag">
                <EnumItem>Nivå1</EnumItem>
                <EnumItem>Nivå 2</EnumItem>
                <EnumItem>Nivå 3</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a20_TiltakInvesteringUtbedring</Name>
      <Definition>Hvis aktuell, beskriv tiltak. Ved flere tiltak, angi tiltakene med 1. , 2. , osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a21_TiltakDriftVedlikehold</Name>
      <Definition>Hvis aktuell, beskriv tiltak. Ved flere tiltak, angi tiltakene med 1. , 2. , osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a22_TiltakBeredskap</Name>
      <Definition>Hvis aktuell, beskriv tiltak. Ved flere tiltak, angi tiltakene med 1. , 2. , osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a23_TiltakKartleggingAnalyse</Name>
      <Definition>Hvis aktuell, beskriv tiltak. Ved flere tiltak, angi tiltakene med 1. , 2. , osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a50_Merknad</Name>
      <Definition>Merknad. Kan benyttes ifm. revisjon eller oppdatering eller for å angi spesielle grunner til at dette er et VegROSpunkt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a60_Arkivnummer</Name>
      <Definition>Referanse til Mime.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>a70_Metadata</Name>
      <Definition>Angir hvor det finnes metadata. URL eller filkatalog.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
