<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_VEGANLEGG</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>typeProsjekt</Name>
         <Definition>Angir hvilken type prosjekt dette er.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_TypeProsjekt">
                  <EnumItem>Anlegg for gående/syklende</EnumItem>
                  <EnumItem>Annen type prosjekt</EnumItem>
                  <EnumItem>Grøntanlegg, miljø</EnumItem>
                  <EnumItem>Ny veg</EnumItem>
                  <EnumItem>Sideanlegg</EnumItem>
                  <EnumItem>Signal, skilt, oppmerking</EnumItem>
                  <EnumItem>Skredsikring</EnumItem>
                  <EnumItem>Trafikksikkerhetstiltak</EnumItem>
                  <EnumItem>Tunneloppgradering</EnumItem>
                  <EnumItem>Utbedring av veg</EnumItem>
                  <EnumItem>Vedlikeholdstiltak veg</EnumItem>
                  <EnumItem>Vegbelysning</EnumItem>
                  <EnumItem>Brufornying</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>navn</Name>
         <Definition>Angir fullstendig navn på veganlegget.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>beskrivelse</Name>
         <Definition>Gir kort beskrivelse av veganlegget.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>status</Name>
         <Definition>Angir hvilken status veganlegg har. NB! Egenskapstypen har til dels sammenheng med egenskapstype 2031 "Dato byggestart" og 2032 "Dato ferdigstilt".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_Status">
                  <EnumItem>Avbrutt</EnumItem>
                  <EnumItem>Ferdigstilt</EnumItem>
                  <EnumItem>Planstadiet</EnumItem>
                  <EnumItem>Under bygging</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datoByggestart</Name>
         <Definition>Angir startdato for bygging av veganlegget. Dato fram i tid ansees som estimert tidspunkt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datoFerdigstilt</Name>
         <Definition>Dato for ferdigstillelse av anlegget. Veganlegget regnes som ferdigstilt når det blir overført til  driftsenhet/-organisasjon. Dato fram i tid ansees som estimert tidspunkt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datoTrafikkåpning</Name>
         <Definition>Dato for når det åpnes for trafikk på veganlegget.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>medførerVegnettsendring</Name>
         <Definition>Angir om prosjektet medfører endringer i vegnettet. Dette gjelder endring i horisontalplan, vertikalplan, vegtype, vegstatus m.m.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_MedførerVegnettsendring">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Må avklares</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>digitaltNavigerbartVegnett_DatoOk</Name>
         <Definition>Angir dato for når vegnett med tilhørende fagdata er oppdatert i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kategori1_DatoGrunnlagLevert</Name>
         <Definition>Angir dato for når grunnlag for kategori 1 data til NVDB sist ble overlevert Statens vegvesen. Skal angis av vegforvalter.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kategori1_DatoOKINVDB</Name>
         <Definition>Angir dato da kategori 1 data var oppdatert og OK i NVDB. Statens vegvesen melder fra til vegforvalter. Vegforvalter legger inn informasjon.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kategori2_DatoOK</Name>
         <Definition>Angir dato for når kategori 2-data var OK i NVDB. Legges inn av aktuell vegforvalter.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kategori3_Leveranse</Name>
         <Definition>Gir informasjon om hvorvidt det legges inn data for kategori 3 i NVDB.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_Kategori3_Leveranse">
                  <EnumItem>Delvis</EnumItem>
                  <EnumItem>Ikke avklart</EnumItem>
                  <EnumItem>På nivå med Objektliste1</EnumItem>
                  <EnumItem>Tilnærmet ikke</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>merknadDataleveranse</Name>
         <Definition>Merknad som gir informasjon omkring dataleveranser.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>datafangstprosjekt</Name>
         <Definition>Gir informasjon om eventuelle datafangstprosjekt tilhørende veganlegget.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>linkTilProsjektinformasjon</Name>
         <Definition>Gir URL til informasjonsside om veganlegget/prosjektet i vegforvalters egne system.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>arkivreferanse</Name>
         <Definition>Gir link til arkivreferanse.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>FKBDataLevertDato</Name>
         <Definition>Gir dato for når FKB-dataleveranse for veganlegget ble levert.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kvalitetTidspunkt</Name>
         <Definition>Angir hvor nøyaktig angitt tidspunkt for vegåpning er.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_KvalitetTidspunkt">
                  <EnumItem>&lt; 5 dager</EnumItem>
                  <EnumItem>&lt; 5 år</EnumItem>
                  <EnumItem>&lt; Halvår</EnumItem>
                  <EnumItem>&lt; Måned</EnumItem>
                  <EnumItem>&lt; År</EnumItem>
                  <EnumItem>&gt; 5 år</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>kvalitetStedfesting</Name>
         <Definition>Angir hvor nøyaktig stedfestingen av veganlegget er.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_KvalitetStedfesting">
                  <EnumItem>&lt; 1 km</EnumItem>
                  <EnumItem>&lt; 10 meter</EnumItem>
                  <EnumItem>&lt; 100 meter</EnumItem>
                  <EnumItem>&lt; 5 meter</EnumItem>
                  <EnumItem>&lt; 50 meter</EnumItem>
                  <EnumItem>&lt; 500 meter</EnumItem>
                  <EnumItem>&gt; 1 km</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>relevantForEkomportalen</Name>
         <Definition>Angir om vegobjektet er relevant for overføring til Ekomportalen eller ikke.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_RelevantForEkomportalen">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er prosjekteier.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_Eier">
                  <EnumItem>Agder fylkeskommune</EnumItem>
                  <EnumItem>Innlandet fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Møre og Romsdal fylkeskommune</EnumItem>
                  <EnumItem>Nordland fylkeskommune</EnumItem>
                  <EnumItem>Oslo kommune</EnumItem>
                  <EnumItem>Rogaland fylkeskommune</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Troms og Finnmark fylkeskommune</EnumItem>
                  <EnumItem>Trøndelag fylkeskommune</EnumItem>
                  <EnumItem>Vestfold og Telemark fylkeskommune</EnumItem>
                  <EnumItem>Vestland fylkeskommune</EnumItem>
                  <EnumItem>Viken fylkeskommune</EnumItem>
                  <EnumItem>Akershus fylkeskommune</EnumItem>
                  <EnumItem>Buskerud fylkeskommune</EnumItem>
                  <EnumItem>Finnmark fylkeskommune</EnumItem>
                  <EnumItem>Telemark fylkeskommune</EnumItem>
                  <EnumItem>Troms fylkeskommune</EnumItem>
                  <EnumItem>Vestfold fylkeskommune</EnumItem>
                  <EnumItem>Østfold fylkeskommune</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjennomføringsavtale</Name>
         <Definition>Angir at prosjektet gjennomføres/styres av annen virksomhet/organisasjon enn vegeier.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Veganlegg_Gjennomføringsavtale">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>gjennomføringsavtaleMed</Name>
         <Definition>Angir hvem det er inngått gjennomføringsavtale med. Det skal oppgis navn på firma/organisasjon.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
