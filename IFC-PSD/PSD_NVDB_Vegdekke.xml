<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Vegdekke</Name>
  <Definition>Den øverste del av overbygningen, består vanligvis av et slitelag og et bindlag. (Definisjonsliste Statens vegvesens håndbøker).</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>lagtype</Name>
      <Definition>Angir om vegdekke opprinnelig er lagt som bindlag eller slitelag.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Lagtype">
                <EnumItem>Slitelag</EnumItem>
                <EnumItem>Bindlag</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>massetype</Name>
      <Definition>Angir hvilken type vegobjektet er av.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Massetype">
                <EnumItem>Grus</EnumItem>
                <EnumItem>Tynndekke</EnumItem>
                <EnumItem>Asfalt (historisk)</EnumItem>
                <EnumItem>Asfaltert grus (historisk)</EnumItem>
                <EnumItem>Støpeasfalt</EnumItem>
                <EnumItem>Topeka</EnumItem>
                <EnumItem>Skjelettasfalt</EnumItem>
                <EnumItem>Asfaltbetong</EnumItem>
                <EnumItem>Drensasfalt</EnumItem>
                <EnumItem>Asfaltgrusbetong</EnumItem>
                <EnumItem>Mykasfalt</EnumItem>
                <EnumItem>Asfaltert finpukk</EnumItem>
                <EnumItem>Asfaltløsningsgrus (historisk)</EnumItem>
                <EnumItem>Emulsjonsgrus, tett</EnumItem>
                <EnumItem>Asfaltskumgrus</EnumItem>
                <EnumItem>Oljegrus (historisk)</EnumItem>
                <EnumItem>Enkel overflatebehandling</EnumItem>
                <EnumItem>Dobbel overflatebehandling</EnumItem>
                <EnumItem>Enkel overflatebehandling grus</EnumItem>
                <EnumItem>Dobbel overflatebehandling grus (historisk)</EnumItem>
                <EnumItem>Gjenbruksasfalt</EnumItem>
                <EnumItem>Forsegling</EnumItem>
                <EnumItem>Slamasfalt</EnumItem>
                <EnumItem>Betong</EnumItem>
                <EnumItem>Betongdekke, uarmert</EnumItem>
                <EnumItem>Valsebetong</EnumItem>
                <EnumItem>Betongelementer</EnumItem>
                <EnumItem>Belegningsstein</EnumItem>
                <EnumItem>Brostein/Gatestein</EnumItem>
                <EnumItem>Stålgitter (bru)</EnumItem>
                <EnumItem>Tre (bru)</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>øvreNominelleSteinstørrelse</Name>
      <Definition>Angir øvre nominelle steinstørrelse for asfaltmateriale.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_ØvreNominelleSteinstørrelse">
                <EnumItem>2</EnumItem>
                <EnumItem>4</EnumItem>
                <EnumItem>6</EnumItem>
                <EnumItem>8</EnumItem>
                <EnumItem>11</EnumItem>
                <EnumItem>12</EnumItem>
                <EnumItem>14</EnumItem>
                <EnumItem>16</EnumItem>
                <EnumItem>22</EnumItem>
                <EnumItem>28</EnumItem>
                <EnumItem>32</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekketykkelse</Name>
      <Definition>Angir representativ dekketykkelse i millimeter.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekkebredde_Gjennomsnitt</Name>
      <Definition>Angir gjennomsnittlig bredde for dekket i meter.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>masseforbruk</Name>
      <Definition>Angir masseforbruk i kg/kvm.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekkeleggingsdato</Name>
      <Definition>Angir dato for dekkelegging.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcDateTime"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>reseptnummer</Name>
      <Definition>Gir reseptnummer for oppslag i Labsys.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>budsjett</Name>
      <Definition>Angir hvilket budsjett dekkelegging går over.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Budsjett">
                <EnumItem>Anlegg</EnumItem>
                <EnumItem>Vedlikehold</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>areal</Name>
      <Definition>Angir arealet av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spesiellPlassering</Name>
      <Definition>Angir om dekke har spesiell plassering.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_SpesiellPlassering">
                <EnumItem>På bru</EnumItem>
                <EnumItem>I tunnel</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>dekkevariant</Name>
      <Definition>Angir detaljer om dekketypen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Dekkevariant">
                <EnumItem>Åpen</EnumItem>
                <EnumItem>Tett</EnumItem>
                <EnumItem>Drenerende</EnumItem>
                <EnumItem>Gjenbruksmasse</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spesialasfalt</Name>
      <Definition>Angir bruk av ev.spesialasfalt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Spesialasfalt">
                <EnumItem>Asfaltert finpukk</EnumItem>
                <EnumItem>Chemcrete</EnumItem>
                <EnumItem>Sipernat</EnumItem>
                <EnumItem>Trinidadasf.</EnumItem>
                <EnumItem>Gilsonite</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>spesialSteinmateriale</Name>
      <Definition>Angir bruk av ev. spesial steinmateriale.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_SpesialSteinmateriale">
                <EnumItem>Slagg</EnumItem>
                <EnumItem>Lys stein</EnumItem>
                <EnumItem>Dynagrip</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lavtemperaturasfalt</Name>
      <Definition>Angir om utlagt dekke er produsert som lavtemperaturasfalt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Lavtemperaturasfalt">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>repaver</Name>
      <Definition>«halvsålings»-metode med stedlig gjenbruk av eksisterende asfalt som det legges et tynt slitelag på (50 kg/m2). Ingen klebing da det legges varmt mot varmt.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Repaver">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>avstrøing</Name>
      <Definition>Angir om det er utført avstrøing.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Avstrøing">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forsegling</Name>
      <Definition>Angir om det er utført forsegling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Forsegling">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forvarming</Name>
      <Definition>Angir om det er utført forvarming.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Forvarming">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>forkiling</Name>
      <Definition>Angir om det er utført forkiling.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Forkiling">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>anriking</Name>
      <Definition>Angir om det er utført anriking.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Anriking">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>fibertilsetning</Name>
      <Definition>Angir ev. fibertilsetning.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Fibertilsetning">
                <EnumItem>Cellulose</EnumItem>
                <EnumItem>Mineral</EnumItem>
                <EnumItem>Stål</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>gummitilsetning</Name>
      <Definition>Angir ev. gummitilsetning.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Gummitilsetning">
                <EnumItem>Gummipulver</EnumItem>
                <EnumItem>Revet gummi</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>modifisertBindemiddel</Name>
      <Definition>Angir om det er brukt modifisert bindemiddel.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_ModifisertBindemiddel">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>polypropylen</Name>
      <Definition>Angir om det er benyttet polypropylen.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Polypropylen">
                <EnumItem>Ja</EnumItem>
                <EnumItem>Nei</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>betongfasthet</Name>
      <Definition>Angir betongfasthet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Betongfasthet">
                <EnumItem>C35</EnumItem>
                <EnumItem>C45</EnumItem>
                <EnumItem>C70</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Gir permanent merknad.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>kostnad</Name>
      <Definition>Angir endelig kostnad for dekke.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcInteger"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektInternObjekt_ID</Name>
      <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegdekke_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
