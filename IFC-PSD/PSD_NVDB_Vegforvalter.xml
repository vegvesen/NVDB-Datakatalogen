         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegforvalter_Vegforvalter">
                  <EnumItem>Agder fylkeskommune </EnumItem>
                  <EnumItem>Innlandet fylkeskommune</EnumItem>
                  <EnumItem>Kommune </EnumItem>
                  <EnumItem>Møre og Romsdal fylkeskommune </EnumItem>
                  <EnumItem>Nordland fylkeskommune </EnumItem>
                  <EnumItem>Nye Veier AS </EnumItem>
                  <EnumItem>Oslo kommune </EnumItem>
                  <EnumItem>Rogaland fylkeskommune </EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Trøndelag fylkeskommune </EnumItem>
                  <EnumItem>Vestland fylkeskommune </EnumItem>
                  <EnumItem>Akershus fylkeskommune</EnumItem>
                  <EnumItem>Buskerud fylkeskommune</EnumItem>
                  <EnumItem>Finnmark fylkeskommune</EnumItem>
                  <EnumItem>Telemark fylkeskommune</EnumItem>
                  <EnumItem>Troms fylkeskommune</EnumItem>
                  <EnumItem>Vestfold fylkeskommune</EnumItem>
                  <EnumItem>Østfold fylkeskommune</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>merknad</Name>
         <Definition>Kan beskrive spesielle avtaler mellom fylker, avtaler mellom f.eks. Nye Veier AS og en fylkeskommune, eller annet som er relevant for angitt vegstrekning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
