<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_VEGOPPMERKING_TVERRGÅENDE</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>type</Name>
         <Definition>Angir hvilken type vegobjektet er av.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_Type">
                  <EnumItem>Delesymbol (1038)</EnumItem>
                  <EnumItem>Fartsgrense (1054)</EnumItem>
                  <EnumItem>Fartshump (1027)</EnumItem>
                  <EnumItem>Gangfelt (1024)</EnumItem>
                  <EnumItem>Gangsymbol (1037)</EnumItem>
                  <EnumItem>Mangler</EnumItem>
                  <EnumItem>Parkeringsfelt (1028)</EnumItem>
                  <EnumItem>Parkeringssymbol (1040)</EnumItem>
                  <EnumItem>Pil, feltskifte, (1034.F)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.H)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.R)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.RH)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.RHV)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.RV)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.V)</EnumItem>
                  <EnumItem>Pil, kjøreretn, (1034.VH)</EnumItem>
                  <EnumItem>Pil, minirundkjøring (1034)</EnumItem>
                  <EnumItem>Rumlefelt</EnumItem>
                  <EnumItem>Skinnekjøretøy (1044)</EnumItem>
                  <EnumItem>Skravurområde, fyll (1014)</EnumItem>
                  <EnumItem>Skravurområde, skrålinjer (1014)</EnumItem>
                  <EnumItem>Skravurområde, vinkler (1014)</EnumItem>
                  <EnumItem>Stopplinje (1020)</EnumItem>
                  <EnumItem>Sykkelkryssing (1026.1)</EnumItem>
                  <EnumItem>Sykkelkryssing inntil gangfelt (1026.2)</EnumItem>
                  <EnumItem>Sykkelsymbol (1039)</EnumItem>
                  <EnumItem>Symbol for forflytningshemmede (1042)</EnumItem>
                  <EnumItem>Tekst, buss (1050.2)</EnumItem>
                  <EnumItem>Tekst, sambruksfelt (1052)</EnumItem>
                  <EnumItem>Tekst, skole (1050.5)</EnumItem>
                  <EnumItem>Tekst, stopp (1050.1)</EnumItem>
                  <EnumItem>Tekst, taxi (1050.3)</EnumItem>
                  <EnumItem>Tekst, vegvisning (1050.4)</EnumItem>
                  <EnumItem>Vikelinje (1022)</EnumItem>
                  <EnumItem>Vikesymbol (1036)</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>dimensjonerendeForholdForSymbolhøyder</Name>
         <Definition>Angir hva som ligger til grunn for størrelse for symbolene.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_DimensjonerendeForholdForSymbolhøyder">
                  <EnumItem>Fartsgrense over 60</EnumItem>
                  <EnumItem>Fartsgrense tom. 60</EnumItem>
                  <EnumItem>Sykkelveg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedtaksnummer</Name>
         <Definition>Angir vedtaksnummer.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>nedfresing</Name>
         <Definition>Angir hvorvidt det er frest ned en plan forsenkning i asfalten for å legge vegmerkingen i. Hensikten med dette er bl.a. at vegmerkingen blir mindre utsatt for brøyteskader.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_Nedfresing">
                  <EnumItem>Ja</EnumItem>
                  <EnumItem>Nei</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>bredde</Name>
         <Definition>Angir bredde av vegobjektet. Bredde måles på tvers av symbol/tekst sin lengderetning. Tverrgående oppmerking som f.eks. "gangfelt har lengderetning på tvers av vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>lengde</Name>
         <Definition>Angir lengde av vegobjektet, måles i oppmerkingens lengderetning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>areal</Name>
         <Definition>Angir arealet av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tekst_Vegvisning</Name>
         <Definition>Angir vegnummer eller stedsnavn som er tekster i vegbanen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallViketrekanter</Name>
         <Definition>Angir hvor mange viketrekanter det er.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallRomlelinjer</Name>
         <Definition>Angir hvor mange linjer som inngår i romlefelt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallGangfeltstaver</Name>
         <Definition>Angir hvor mange staver (striper) det er i et gangfelt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>antallMarkeringsfirkanter</Name>
         <Definition>Angir hvor mange markeringsfirkanter det er i tilknytning til tverrgående oppmerking. Aktuelt for sykkelkryssing (1026) og fartshump (1027).</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>løpemeterSkravur</Name>
         <Definition>Angir total lengde av striper i skravur.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>breddeLinjeISperreområdet</Name>
         <Definition>Bredde på enkeltlinjene i sperreområdet. Denne egenskapstypen er bare aktuell for vegoppmerking av type 1014.2 - skrålinjer/vinkler/fyll.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_BreddeLinjeISperreområdet">
                  <EnumItem>0,25</EnumItem>
                  <EnumItem>0,50</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>farge</Name>
         <Definition>Angir hovedfarge til vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_Farge">
                  <EnumItem>Gul</EnumItem>
                  <EnumItem>Hvit</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>tilleggsinformasjon</Name>
         <Definition>Supplerende informasjon om vegobjektet som ikke framkommer direkte av andre egenskapstyper.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektreferanse</Name>
         <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for lettere å kunne skille nye data fra eksisterende data i NVDB.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>prosjektInternObjekt_ID</Name>
         <Definition>Objektmerking. Unik innenfor tilhørende vegprosjekt.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>eier</Name>
         <Definition>Angir hvem som er eier av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_Eier">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Stat, Nye Veier</EnumItem>
                  <EnumItem>Stat, Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vedlikeholdsansvarlig</Name>
         <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegoppmerking, tverrgående_Vedlikeholdsansvarlig">
                  <EnumItem>Fylkeskommune</EnumItem>
                  <EnumItem>Kommune</EnumItem>
                  <EnumItem>Nye Veier</EnumItem>
                  <EnumItem>OPS</EnumItem>
                  <EnumItem>Privat</EnumItem>
                  <EnumItem>Statens vegvesen</EnumItem>
                  <EnumItem>Uavklart</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
