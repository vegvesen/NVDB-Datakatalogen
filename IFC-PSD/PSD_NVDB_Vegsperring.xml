<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Vegsperring</Name>
  <Definition>Angir at veg er fysisk sperret.</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>type</Name>
      <Definition>Angir hvilken type sperring det er tale om.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegsperring_Type">
                <EnumItem>Låst bom</EnumItem>
                <EnumItem>Heve-/senkebom</EnumItem>
                <EnumItem>Svingbom</EnumItem>
                <EnumItem>Rørgelender</EnumItem>
                <EnumItem>Betongblokk</EnumItem>
                <EnumItem>Steinblokk</EnumItem>
                <EnumItem>Stolpe/pullert/kjegle</EnumItem>
                <EnumItem>Bussluse</EnumItem>
                <EnumItem>Annen type vegbom/sperring</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>funksjon</Name>
      <Definition>Angir hvilken funksjon sperringen har.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegsperring_Funksjon">
                <EnumItem>Betalingssperre</EnumItem>
                <EnumItem>Bussluse</EnumItem>
                <EnumItem>Fjernstyrt sperring</EnumItem>
                <EnumItem>Låst sperring</EnumItem>
                <EnumItem>Trafikkavviser</EnumItem>
                <EnumItem>Ulåst Sperring</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>gjeldertidsrom</Name>
      <Definition>Angir tidsrommet (hhmm-hhmm) eller (mnd-mnd) vegsperringen gjelder.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>merknad</Name>
      <Definition>Opplysning om spesielle forhold knyttet til fysisk sperring.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Vegsperring_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
