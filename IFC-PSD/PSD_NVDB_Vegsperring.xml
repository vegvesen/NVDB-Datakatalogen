         <Definition>Angir hvilken type sperring det er tale om.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegsperring_Type">
                  <EnumItem>Annen type vegbom/sperring</EnumItem>
                  <EnumItem>Betongblokk</EnumItem>
                  <EnumItem>Bussluse</EnumItem>
                  <EnumItem>Heve-/senkebom</EnumItem>
                  <EnumItem>Låst bom</EnumItem>
                  <EnumItem>Rørgelender</EnumItem>
                  <EnumItem>Steinblokk</EnumItem>
                  <EnumItem>Stolpe/pullert/kjegle</EnumItem>
                  <EnumItem>Svingbom</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
