<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_VEGSTENGNING</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>hovedårsakTilStengning</Name>
         <Definition>Gir informasjon om hva som er hovedårsak til stengning av vegen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_HovedårsakTilStengning">
                  <EnumItem>Annen årsak</EnumItem>
                  <EnumItem>Brann</EnumItem>
                  <EnumItem>Drivsnø/redusert sikt</EnumItem>
                  <EnumItem>Flom</EnumItem>
                  <EnumItem>Glatt veg</EnumItem>
                  <EnumItem>Skred</EnumItem>
                  <EnumItem>Skredfare</EnumItem>
                  <EnumItem>Teknisk feil</EnumItem>
                  <EnumItem>Trafikkhendelse</EnumItem>
                  <EnumItem>Vegarbeid</EnumItem>
                  <EnumItem>Vind</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>skredtype</Name>
         <Definition>Angir hvilken type skred det er fare for. Bare aktuell hvis hovedårsak til vegstengning er "Skred" eller "Skredfare".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_Skredtype">
                  <EnumItem>Flomskred</EnumItem>
                  <EnumItem>Is</EnumItem>
                  <EnumItem>Is/stein</EnumItem>
                  <EnumItem>Jord/løsmasse</EnumItem>
                  <EnumItem>Snø</EnumItem>
                  <EnumItem>Stein</EnumItem>
                  <EnumItem>Sørpeskred</EnumItem>
                  <EnumItem>Utglidning av veg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengtRetning</Name>
         <Definition>Angir hvilken trafikkretning vegen er stengt for. Det er særlig for veger med midtdeler det er aktuelt å stenge for en trafikkretning. Envegskjørte veger angis som "helt stengt".</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_StengtRetning">
                  <EnumItem>Helt stengt</EnumItem>
                  <EnumItem>Med metreringsretning</EnumItem>
                  <EnumItem>Mot metreringsretning</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stedsangivelse</Name>
         <Definition>Stedsnavn hvor forekomst er lokalisert. Ev. plassering i forhold til stedsnavn.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcText"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>værforholdPåVegen</Name>
         <Definition>Angir hvilke værforhold det var på vegen i forkant av da vegstengning.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_VærforholdPåVegen">
                  <EnumItem>Ingen nedbør</EnumItem>
                  <EnumItem>Regn</EnumItem>
                  <EnumItem>Regn, lite</EnumItem>
                  <EnumItem>Regn, middels</EnumItem>
                  <EnumItem>Regn, mye</EnumItem>
                  <EnumItem>Snø</EnumItem>
                  <EnumItem>Snø, lite</EnumItem>
                  <EnumItem>Snø, middels</EnumItem>
                  <EnumItem>Snø, mye</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vindretning</Name>
         <Definition>Angir hvilken vindretning det var på vegen i forkant av vegstengningen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_Vindretning">
                  <EnumItem>N</EnumItem>
                  <EnumItem>NNV</EnumItem>
                  <EnumItem>NNØ</EnumItem>
                  <EnumItem>NV</EnumItem>
                  <EnumItem>NØ</EnumItem>
                  <EnumItem>S</EnumItem>
                  <EnumItem>SSV</EnumItem>
                  <EnumItem>SSØ</EnumItem>
                  <EnumItem>SV</EnumItem>
                  <EnumItem>SØ</EnumItem>
                  <EnumItem>V</EnumItem>
                  <EnumItem>Vindstille</EnumItem>
                  <EnumItem>VNV</EnumItem>
                  <EnumItem>VSV</EnumItem>
                  <EnumItem>Ø</EnumItem>
                  <EnumItem>ØNØ</EnumItem>
                  <EnumItem>ØSØ</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>temperaturPåVeg</Name>
         <Definition>Angir temperatur på vegen i forkant av vegstengning.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcReal"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengning</Name>
         <Definition>Gir informasjon om stengning av vegen.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegstengning_Stengning">
                  <EnumItem>Kolonnekjøring/styrt avvikling</EnumItem>
                  <EnumItem>Nattestengt</EnumItem>
                  <EnumItem>Stengning av del av vegbane</EnumItem>
                  <EnumItem>Stengt for all trafikk</EnumItem>
                  <EnumItem>Stengt for gående/syklende</EnumItem>
                  <EnumItem>Stengt for personbiler</EnumItem>
                  <EnumItem>Stengt for tungtrafikk</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengtFraDato</Name>
         <Definition>Angir dato for stengning av vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengtFraKlokkeslett</Name>
         <Definition>Angir klokkeslett for stengning av vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengtTilDato</Name>
         <Definition>Angir dato for åpning av vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcDateTime"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>stengtTilKlokkeslett</Name>
         <Definition>Angir klokkeslett for åpning av vegen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
