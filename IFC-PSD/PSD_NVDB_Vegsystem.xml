<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd">
   <IfcVersion/>
   <Name>Pset_NVDB_VEGSYSTEM</Name>
   <Applicability/>
   <ApplicableClasses/>
   <ApplicableTypeValue/>
   <PropertyDefs>
      <PropertyDef>
         <Name>vegkategori</Name>
         <Definition>Kategorisering som angir på hvilket nivå vegmyndigheten for strekningen ligger.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegsystem_Vegkategori">
                  <EnumItem>Europaveg</EnumItem>
                  <EnumItem>Fylkesveg</EnumItem>
                  <EnumItem>Kommunal veg</EnumItem>
                  <EnumItem>Privat veg</EnumItem>
                  <EnumItem>Riksveg</EnumItem>
                  <EnumItem>Skogsveg</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vegfase</Name>
         <Definition>Angir vegens fase i livet.</Definition>
         <PropertyType>
            <TypePropertyEnumeratedValue>
               <EnumList name="PEnum_Vegsystem_Vegfase">
                  <EnumItem>Eksisterende</EnumItem>
                  <EnumItem>Fiktiv</EnumItem>
                  <EnumItem>Planlagt</EnumItem>
                  <EnumItem>Under bygging</EnumItem>
               </EnumList>
            </TypePropertyEnumeratedValue>
         </PropertyType>
      </PropertyDef>
      <PropertyDef>
         <Name>vegnummer</Name>
         <Definition>Angir hvilke deler av vegnettet som rutemessig hører sammen.</Definition>
         <PropertyType>
            <TypePropertySingleValue>
               <DataType type="IfcInteger"/>
            </TypePropertySingleValue>
         </PropertyType>
      </PropertyDef>
   </PropertyDefs>
</PropertySetDef>
