<?xml version="1.0" encoding="UTF-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="http://standards.buildingsmart.org/IFC/RELEASE/IFC4/FINAL/PSD/PSD_IFC4.xsd"> 
  <IfcVersion/>
  <Name>Pset_NVDB_Viltfare</Name>
  <Definition>Strekninger som er skiltet med viltfare</Definition>
  <Applicability/>
  <ApplicableClasses/>
  <ApplicableTypeValue/>
  <PropertyDefs>
    <PropertyDef>
      <Name>art</Name>
      <Definition>Angir hvilken dyreart som er antatt mest forekommende på strekningen(vises på evt skilting).</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Viltfare_Art">
                <EnumItem>Elg</EnumItem>
                <EnumItem>Rein</EnumItem>
                <EnumItem>Hjort</EnumItem>
                <EnumItem>Rådyr</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>beskrivelse</Name>
      <Definition>Angir øvrig intformasjon om strekning. Kan f.eks være sesongvariasjon, utdyping av hvike arter som er problem, omfang av vilt osv.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>lengde</Name>
      <Definition>Viltfarestrekningens lengde.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcReal"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>prosjektreferanse</Name>
      <Definition>Referanse til prosjekt. Det benyttes samme prosjektreferanse som på tilhørende Veganlegg (VT30). Benyttes for å lettere kunne skille nye data fra eksisterende data i NVDB.</Definition>
      <PropertyType>
         <TypePropertySingleValue>
             <DataType type="IfcText"/>
         </TypePropertySingleValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>eier</Name>
      <Definition>Angir hvem som er eier av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Viltfare_Eier">
                <EnumItem>Stat, Statens vegvesen</EnumItem>
                <EnumItem>Stat, Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
    <PropertyDef>
      <Name>vedlikeholdsansvarlig</Name>
      <Definition>Angir hvem som er ansvarlig for vedlikehold av vegobjektet.</Definition>
      <PropertyType>
         <TypePropertyEnumeratedValue>
             <EnumList name="PEnum_Viltfare_Vedlikeholdsansvarlig">
                <EnumItem>Statens vegvesen</EnumItem>
                <EnumItem>Nye Veier</EnumItem>
                <EnumItem>Fylkeskommune</EnumItem>
                <EnumItem>OPS</EnumItem>
                <EnumItem>Kommune</EnumItem>
                <EnumItem>Privat</EnumItem>
                <EnumItem>Uavklart</EnumItem>
             </EnumList>
         </TypePropertyEnumeratedValue>
      </PropertyType>
    </PropertyDef>
  </PropertyDefs>
</PropertySetDef>
