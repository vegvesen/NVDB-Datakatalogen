###  http://rdf.sosi.no/sosi-owl#SOSI_Rot
:SOSI_Rot rdf:type owl:Class ;
          rdfs:label "Rotobjekt for SOSI"@no ;
          skos:definition "Rotobjekt for SOSI"@no .
		  
		  
###  http://rdf.sosi.no/sosi-owl#Objekttype
:Objekttype rdf:type owl:Class ;
               rdfs:subClassOf gfm:AnyFeature ,
                               :SOSI_Rot ,
                               gsp:Feature ;
               skos:definition "Rotklasse for alle objekttyper fra SOSI"@no .


###  http://rdf.sosi.no/sosi-owl#Kodeliste
:Kodeliste rdf:type owl:Class ;
           rdfs:subClassOf :SOSI_Rot ;
           owl:disjointWith :Objekttype ;
           skos:definition "Rotklasse for alle kodelister fra SOSI"@no .


###  http://rdf.sosi.no/sosi-owl#Datatype
:Datatype rdf:type owl:Class ;
           rdfs:subClassOf :SOSI_Rot ;
           owl:disjointWith :Objekttype ;
           skos:definition "Rotklasse for alle datatyper fra SOSI"@no .
		   

